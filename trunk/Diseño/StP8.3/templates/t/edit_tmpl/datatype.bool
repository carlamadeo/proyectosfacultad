Datatype                    <DataTypeID>
    is datatypeclass        boolean;
    has value false         <false_value>;
    has value true          <true_value>;
    has value unknown       <unknown_value>;
    has description         <description_string>;
    has format              <data_format>;
    has units               <data_units>;
    has valid subdomain     <sdom_label>;
    has invalid subdomain   <sdom_label>;
.
