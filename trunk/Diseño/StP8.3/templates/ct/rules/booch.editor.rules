Editor class
{
    { PrintName "StP/Booch Class Editor" }
    { IconFile "boochclass" }
    { RulesFile "rules/class.rules" }
    { CreationCommand "stp_startup -C 'gde -p ${projdir} -s ${system} -ed class'" }
    { QrlFile "rules/qrl/class.qrl" }
    { FileType Diagram }
    { EditorFile
	{ AppType BoochClassDiagram }
	{ PrintName "Booch Class Diagram" }
	{ Suffix ".class" }
	{ FileStore "class_files" }
	{ IsRepSource True }
    }
}


