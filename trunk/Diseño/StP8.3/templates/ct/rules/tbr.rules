// ROW TYPE DEFINITIONS
// these are used to define the operations for each section of the table
RowType 301 Node { 
	{ Semantics
	    { NodeMapping 
		{ MappingName Id }
	        { Id { LabelSelector Label }  
		     { Cell { Hsect HSECT } { Vsect VSECT } { Row ROW } { Col IDCOL } } }

// these determiners are only used when invoking or responding to global update
		{ Name { LabelSelector Label }  
		       { Cell { Hsect HSECT } { Vsect VSECT } { Row ROW } { Col NODE__NAME } } }
		{ Type { LabelSelector Label }  
		       { Cell { Hsect HSECT } { Vsect VSECT } { Row ROW } { Col NODE__TYPE } } }
		{ Signature  { LabelSelector Label }  
                             { Required False }
                    	     { Cell { Hsect HSECT } { Vsect VSECT } { Row ROW } { Col NODE__SIG } } }
		{ Scope  { MappingName Id } 
		         { Precondition
                 	     { LabelFunc TbrObjectIdIsNonZero }
  			     { Cell { Hsect HSECT } { Vsect VSECT } { Row ROW } { Col NODE__SCOPE_NODE_ID } } }
		         { Cell { Hsect HSECT } { Vsect VSECT } { Row ROW } { Col NODE__SCOPE_NODE_ID } } } } }
	{ Methods
		{ EditAnnotation
			{ Name EditAnnotation }
			{ Label "Edit Node Annotation..." }
			{ MappingName Id } 
			{ OS "stpem -first -p ${projdir} -s ${system} -ed oae -C 'AnnotEdit ${annotated.id}' &" } }
		{ Navigation 
			{ Name Navigation }
			{ Label "Node's Reference" }
			{ MappingName Id }
			{ Query "node_ref[node_id=${node.id} && viewpoints]" } 
			{ Query "node_ref[node_id=${node.id}]" } 
//
// this is the default behavior when nothing is specified -- this Method attribute references
// the name of an application-type Method defined in the app.types file
//			{ Method "Navigate" } 
//
		}
		{ Browse
			{ Name node_refs }
			{ Label "Node's node_refs" }
			{ MappingName Id }
			{ Eval "hyperbrowse(\"node\", \"node_ref[node_id=${node.id}]\", \"NODE__ID\");" } }
		{ Browse
			{ Name annot_file }
			{ Label "Node's annot_file" }
			{ MappingName Id }
			{ Eval "hyperbrowse(\"node\", \"file[annot_node[id=${node.id}]]\", \"NODE__ID\");" } }
		{ Browse
			{ Name notes }
			{ Label "Node's notes" }
			{ MappingName Id }
			{ Eval "hyperbrowse(\"node\", \"note[obj_id=${node.id}]\", \"NODE__ID\");" } }
		{ Browse
			{ Name items }
			{ Label "Node's items" }
			{ MappingName Id }
			{ Eval "hyperbrowse(\"node\", \"item[obj_id=${node.id}]\", \"NODE__ID\");" } }
