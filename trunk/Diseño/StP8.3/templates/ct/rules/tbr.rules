		{ Name Cut }
		{ Label "Cu&t" }
		{ Builtin "TableElementCut" }
	}
  	{ MenuEntry
		{ Name Copy }
		{ Label "&Copy" }
		{ Builtin "TableElementCopy" }
	}
  	{ MenuEntry
		{ Name Paste}
		{ Label "&Paste" }
		{ Builtin "TableElementPaste" }
	}
  	{ MenuEntry
		{ Name Delete }
		{ Label "&Delete" }
		{ Builtin "TableElementDelete" }
	}
	{ Spacer
		{ Name TbrSpacer1 }
	}
  	{ MenuEntry 
		{ Name ReduceToSelectedRows }
		{ Label "Reduce To Selected Rows" }
		{ Builtin "EditorQrlEval reduce_browser();" }
		{ ActiveFunc idehook_table_method_active_func_by_type }
