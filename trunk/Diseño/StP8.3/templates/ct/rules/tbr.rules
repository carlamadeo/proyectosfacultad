// TABLE TYPE DEFINITION
// this is the main table definition for the browser, defining its layout and actions.
// you cannot change anything inline here without needing to change the password,
// however you can make many changes in the modifiable table definition
// located at the bottom of this file.
TableType 1 PDMTypesTable
{ 
        { Browser True }

//
//	{ ReadOnly True }  	although this is commented out, it's not to imply that making changes to
//				the browser implies updates to the repository -- the browser is still 
//				a read-only viewer of the repository, but i'll allow editing of the
//				table because it's necessary to edit the view of the repository.

	// this is not modifiable, but it really should be!
	{ Label "PDM Types Table" }

	// modifiable borders
	{ EmptyBorder 	{ Visible False } 				}
	{ RowBorder 	{ Visible True } 	{ Pattern Dotted } 	}
	{ ColBorder 	{ Visible False } 	{ Pattern Dotted } 	}
	{ HsectBorder 	{ Double False } 	{ Thickness Thin } 	} 
	{ TopBorder 	{ Thickness Medium } 				}
	{ BottomBorder 	{ Thickness Medium } 				}
	{ LeftBorder 	{ Thickness Medium } 				}
	{ RightBorder 	{ Thickness Medium } 				}

	{ Initialize
		// modifiable symbols
		{ Unsecured 
			// default widths and heights that work well with a 10-12 point font --
			// if you want a larger font, you'll have to adjust these
			{ ROWHEIGHT	 18 } 
			{ COLWIDTH	 70 }
			{ HEADERHEIGHT	 23 }
			{ HEADERWIDTH	 80 } 
			{ NAMEWIDTH	110 } 
		}
		// non-modifiable symbols
		{ Secured
			// indices for the sections of the table -- headers (singular) and objects (plural).
			// the names of these cannot be changed because they're (unfortunately) encoded into the
			// qrl browse() function for now, 
			// however inclusion of these sections and their order can be changed.
			// (unfortunately) if you change the order, 
			// you must also change the lexical order in the table definition.
			// i believe there are no other assumptions, e.g.,
			// that header and body sections must be paired.
			{ NODE		  1 } { NODES		 2 }
			{ LINK		  3 } { LINKS		 4 }
			{ CNTX		  5 } { CNTXS		 6 }
			{ NODE_REF	  7 } { NODE_REFS	 8 }
			{ LINK_REF	  9 } { LINK_REFS	10 }
			{ CNTX_REF	 11 } { CNTX_REFS	12 }
			{ ITEM		 13 } { ITEMS		14 }
			{ NOTE		 15 } { NOTES		16 }
			{ FILE		 17 } { FILES		18 }
			{ FILE_LOCK	 19 } { FILE_LOCKS	20 }
			{ FILE_HIST	 21 } { FILE_HISTS	22 }
			{ VIEWPOINT	 23 } { VIEWPOINTS	24 }

			// symbolic column indices used in semantics, methods, and/or sorting.
			// these can be changed if you move them to the unsecured section,
			// but you must also change the order in the table definition
			{ IDCOL		  1 }
			{ TYPECOL	  2 }
			{ NAMECOL	  3 }
			{ VALUECOL	  3 }
			{ USERCOL	  3 }
			{ SIGCOL	  4 }
			{ TIMECOL	  4 }
			{ HOSTCOL	  5 } 

			// indices for the column layout.
			// the names of these cannot be changed because they're (unfortunately) encoded into the
			// qrl browse() function for now and also in the tbr.qrl file, 
			// however inclusion of these sections and their order can be changed.
			// (unfortunately) if you change the order, 
			// you must also change the lexical order in the table definition.
			{ NODE__ID			1 }
			{ NODE__TYPE			2 }
			{ NODE__NAME			3 }
			{ NODE__SIG			4 }
			{ NODE__SCOPE_NODE_ID		5 }
			{ NODE__ANNOT_FILE_ID		6 }
			{ NODE__GUID			7 } // ECR 5673

			{ LINK__ID			1 }
			{ LINK__TYPE			2 }
			{ LINK__NAME			3 }
			{ LINK__SIG			4 }
			{ LINK__SCOPE_NODE_ID		5 }
			{ LINK__ANNOT_FILE_ID		6 }
			{ LINK__GUID			7 } // ECR 5673
			{ LINK__FROM_NODE_ID		8 }
			{ LINK__TO_NODE_ID		9 }

			{ CNTX__ID			1 }
			{ CNTX__TYPE			2 }
			{ CNTX__NAME			3 }
			{ CNTX__SIG			4 }
			{ CNTX__SCOPE_NODE_ID		5 }
			{ CNTX__ANNOT_FILE_ID		6 }
			{ CNTX__GUID			7 } // ECR 5673
			{ CNTX__LINK_ID			8 }

			{ NODE_REF__ID			1 }
			{ NODE_REF__NODE_ID		2 }
			{ NODE_REF__FILE_ID		3 }
			{ NODE_REF__SCOPE_NODE_REF_ID	4 }
			{ NODE_REF__X_COORD		6 }
			{ NODE_REF__Y_COORD		7 }
			{ NODE_REF__APPID		8 }

			{ LINK_REF__ID			1 }
			{ LINK_REF__LINK_ID		2 }
			{ LINK_REF__FILE_ID		3 }
			{ LINK_REF__FROM_NODE_REF_ID	4 }
			{ LINK_REF__TO_NODE_REF_ID	5 }
			{ LINK_REF__X_COORD		6 }
			{ LINK_REF__Y_COORD		7 }
			{ LINK_REF__APPID		8 }

