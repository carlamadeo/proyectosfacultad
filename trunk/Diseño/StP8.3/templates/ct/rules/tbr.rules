	//    	{ Tag "EditAnnotation" } }
	//{ Spacer
	//	{ Name TbrSpacer3 }
	//}
  	{ SubMenu
		{ Name NavigationSubmenu }
		{ Label "&GoTo" }
	}
  	{ MenuEntry
		{ Name ScaleToLabel }
		{ Label "Adjust Scale to Fit Label" }
		{ Builtin "TableElementScale" }
	}
  	{ MenuEntry
		{ Name DefaultScale }
		{ Label "Default Scale" }
		{ Builtin "TableElementUnscale" }
	}
}

