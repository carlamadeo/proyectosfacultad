Editor ome
{
    { PrintName "StP/OMT Object Model Editor" }
    { IconFile "ome" }
    { RulesFile "rules/ome.rules" }
    { CreationCommand "stp_startup -C 'gde -p ${projdir} -s ${system} -ed ome'" }
    { QrlFile "rules/qrl/ome.qrl" }
    { FileType Diagram }
    { EditorFile
	{ AppType OMTObjectDiagram }
	{ PrintName "OMT Object Diagram" }
	{ Suffix ".ome" }
	{ FileStore "ome_files" }
	{ IsRepSource True }
    }
}

