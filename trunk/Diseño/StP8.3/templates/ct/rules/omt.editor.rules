Editor otbr
{
  { PrintName  "StP/OMT Class Browser (Tabular Format)" }
  { IconFile "otbr" }
  { RulesFile "rules/otbr.rules" }
  { QrlFile "rules/qrl/otbr.qrl" }
  { CreationCommand "stp_startup -C 'gte -p ${projdir} -s ${system} -ed otbr'" }
  { FileType Table }
  { EditorFile
      { AppType OMTTabularBrowser }
      { PrintName "OMT Class Browser Table" }
      { Suffix ".otbr" }
      { FileStore "otbr_files" }
    }
}

