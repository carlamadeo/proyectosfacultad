DiagramPrintSetting editor {
    { ParseCondition "us_papersizes();" }
    { Version                           7.0 }
    { OutputTarget                      PlatformPrinter }
    { OutputFileName                    "" }  // allow to go to stdout
    { OutputPrinterName                 "" }
    { PageHeight                        792.000 }
    { PageWidth                         612.000 }
    { PageLeftMargin                    36.000 }
    { PageRightMargin                   36.000 }
    { PageTopMargin                     36.000 }
    { PageBottomMargin                  36.000 }
    { PageOrientation                   Automatic }
    { CaptionShow                       True }
    { CaptionText                       "${file} on ${date}" }
    { CaptionVerticalPlacement          Bottom }
    { CaptionAlignment                  Right }
    { DiagramCaptionOrientation         AsDiagram }
    { DiagramScaleMethod                Fit } // ECR 6611
    { DiagramScale                      1.000 }
    { DiagramPageRange                  All }
    { DiagramHorizontalAlignment        Center }
    { DiagramVerticalAlignment          Center }
    { DiagramNumberThePages             False }
    { DiagramShowTheCutLines            False }
    { DiagramInternalMargin             18.000 }
    { DiagramFontScaleTuneFactor        0.930 }
    { DiagramLabelFont
	// ECR4932
	// the print outlook must be the same as the one in the
	// screen, you know, the WYSIWYG problem. Was Helvetica
	{ DiagramFontFamily             "Helvetica" }
	{ DiagramFontWeight             "Normal" }
	{ DiagramFontAngle              "Normal" }
	{ DiagramFontSize               12.0 }
    }
    { DiagramDisplayMarkFont
	// ECR4932
	// the print outlook must be the same as the one in the
	// screen, you know, the WYSIWYG problem. Was Helvetica
	{ DiagramFontFamily             "Helvetica" }
	{ DiagramFontWeight             "Normal" }
	{ DiagramFontAngle              "Normal" }
	{ DiagramFontSize               12.0 }
    }
    { DiagramCaptionFont
	// ECR4932
	// the print outlook must be the same as the one in the
	// screen, you know, the WYSIWYG problem. Was Helvetica
	{ DiagramFontFamily             "Helvetica" }
	{ DiagramFontWeight             "Normal" }
	{ DiagramFontAngle              "Normal" }
	{ DiagramFontSize               12.0 }
    }
}

