Editor ate
{
    { PrintName "Attribute Table Editor" }
    { IconFile "ate" }
    { RulesFile "rules/ate.rules" }
    { QrlFile "rules/qrl/ate.qrl" }
    { CreationCommand "stp_startup -C 'gte -p ${projdir} -s ${system} -ed ate'" }
    { FileType Table }
    { EditorFile
	{ AppType AttributeTable }
	{ PrintName "Attribute Table" }
	{ Suffix ".ate" }
	{ FileStore "ate_files" }
	{ IsRepSource True }
    }
}

