Note 800100 // DB2Column
{
    { AppType DB2Column }
    { HelpText "The DB2 Table Column note lets you add items for declaring an DB2 table column. This information is used in SQL generation." }
    { PrintName "DB2 Table Column" }
    { Bounds { Min 0 } { Max 1 } }
    { Item
	{ AppType ObjectComment }
	{ HelpText "The Table Column Comment item captures a short description of the table column for the DB2 Catalog." }
	{ PrintName "Table Column Comment" }
	{ Bounds { Min 0 } { Max 1} }
    }
}


Note 80014 // OracleColumn
{
    { AppType OracleColumn }
    { HelpText "The Oracle Table Column note lets you add items for declaring an Oracle table column. This information is used in SQL generation." }
    { PrintName "Oracle Table Column" }
    { Bounds { Min 0 } { Max 1 } }
    { Item
	{ AppType IsClusterColumn }
	{ HelpText "The Cluster Column? item indicates this Inherited Key is in the table cluster." }
	{ PrintName "Cluster Column?" }
	{ Bounds { Min 0 } { Max 1 } }
	{ InitValue "True" }
    }
    { Item
	{ AppType ObjectComment }
	{ HelpText "The Table Column Comment item captures a short description of the table column for the Oracle Data Dictionary." }
	{ PrintName "Table Column Comment" }
	{ Bounds { Min 0 } { Max 1} }
    }
}


