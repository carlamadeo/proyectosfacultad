// 2167A notes

Note 802021
{
    { AppType LocalDatabase }
    { Bounds { Min 0 } }
    { PrintName "Database" }
    { Item
        { AppType NoteKey }
        { Bounds { Min 1 } { Max 1 } }
        { PrintName "Key" }
        { IsKey True }
        { IsVisible False }
        { InitValue "${unique_key}" }
    }
    { Item
        { AppType ERDiagram }
        { Bounds { Min 1 } { Max 1 } }
        { PrintName "E-R Diagram" }
    }
    { Item
        { AppType AccessMethod }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Access Method" }
    }
}


