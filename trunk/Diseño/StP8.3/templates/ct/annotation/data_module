Include "annotation/se_generic_object"

Note 802038
{
    { AppType GlobalDefinition }
    { PrintName "Global Definition" }
    { HelpText "The Global Definition note lets you add items which are used for C Code Generation." } 
    { Bounds { Min 1 } { Max 1 } }
    { Item
        { AppType DataType }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Type" }
	{ InitValue "int" }
        { HelpText "Use the Data Type note to specify the data type of this object." }
    }
    { Item
        { AppType ArraySize }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Array Size" }
        { HelpText "The size of the array, specified as [size] or [size1][size2], etc." }
    }
    { Item
        { AppType StorageClass }
        { PrintName "Storage Class" }
        { HelpText "The Storage Class item lets you specify the storage class for Global Data. Possible values are | static | const | static const." }
        { Bounds { Min 0 } { Max 1 } }
    }
}


Note 802040
{
    { AppType GlobalComment }
    { PrintName "Global Comment" }
    { HelpText "The Global Comment note is used to store comments that describe the global variable.  These comments are typically the kind that appear directly at the definition of the global variable in the code." }
    { Bounds { Min 0 } { Max 1 } }
}


