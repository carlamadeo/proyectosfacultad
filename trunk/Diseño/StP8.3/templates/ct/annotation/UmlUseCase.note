Note	801952
{
  { AppType UmlUseCaseDefinition }
  { HelpText "The Use Case Definition note lets you add items that capture modeling information about the Use Case element" }
  //{ HelpText "The Use Case Definition note lets you add use case description." }
  { PrintName "Use Case Definition" }
  { Bounds { Min 1 } { Max 1 } }
  { Item
      { AppType UmlUseCasePrecondition }
      { HelpText "The Precondition on a Use Case lets you define the conditions which have to be satisfied before invoking this Use Case." }
      //{ HelpText "The Precondition item lets you assign a name to a precondition." }
      { PrintName "Use Case Precondition" }
      { Bounds { Min 1 } { Max 1 } }
      { InitValue "" }
      //{ IsVisible False }
      //{ IsKey True }
  }
  { Item
      { AppType UmlUseCasePostcondition }
      { HelpText "The Postcondition on a Use Case lets you define the conditions which have to be satisfied after invoking this Use Case." }
      //{ HelpText "The Postcondition item lets you assign a name to a postcondition." }
      { PrintName "Use Case Postcondition" }
      { Bounds { Min 1 } { Max 1 } }
      { InitValue "" }
      //{ IsVisible False }
      //{ IsKey True }
  }
  { Item
      { AppType UmlUseCaseException }
