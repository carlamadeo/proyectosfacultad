      //{ HelpText "The Exception item lets you assign a name to an exception." }
      { PrintName "Use Case Exception" }
      { Bounds { Min 1 } { Max 1 } }
      { InitValue "" }
      //{ IsVisible False }
      //{ IsKey True }
  }
