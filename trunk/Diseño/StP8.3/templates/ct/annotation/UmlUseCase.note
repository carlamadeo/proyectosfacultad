  { Item
    { AppType UmlUseCaseExtensionPoint }
    { HelpText "An extension point is a reference to one location within a use case at which action sequences from other use cases may be inserted. Each extension point has a unique name within a use case, and a description of the location within the behavior of the use case." }
    { PrintName "Use Case Extension Point" }
    { Bounds { Min 0 } }
    { InitValue "" }
  }    
