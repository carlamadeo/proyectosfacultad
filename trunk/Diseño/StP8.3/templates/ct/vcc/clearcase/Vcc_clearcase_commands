% sccsid[] = %W% %Y% %D%; Copyright (C) IDE 1984-1994

% These must be set to the users VOB and VIEW
%vcc_vobtag=\vobname
%vcc_viewtag=viewname

vcc_create_command=\templates\ct\vcc\clearcase\vcc_clearcase_create.tcl -v ${ti.vcc_vobtag} -w ${ti.vcc_viewtag} -f ${ti.vcc_file} -t compressed_file -c "${ti.vcc_comment}" ${ti.vcc_flags}
vcc_create_flags=

vcc_get_command=\templates\ct\vcc\clearcase\vcc_clearcase_checkout.tcl -v ${ti.vcc_vobtag} -w ${ti.vcc_viewtag} -f ${ti.vcc_file} -s unreserved ${ti.vcc_flags}
vcc_get_flags=r1

vcc_edit_command=\templates\ct\vcc\clearcase\vcc_clearcase_checkout.tcl -v ${ti.vcc_vobtag} -w ${ti.vcc_viewtag} -f ${ti.vcc_file} -s reserved ${ti.vcc_flags} 
vcc_edit_flags=r1

vcc_unedit_command=\templates\ct\vcc\clearcase\vcc_clearcase_uncheckout.tcl -v ${ti.vcc_vobtag} -w ${ti.vcc_viewtag} -f ${ti.vcc_file} ${ti.vcc_flags}
vcc_unedit_flags=

vcc_new_v_command=\templates\ct\vcc\clearcase\vcc_clearcase_checkin.tcl -v ${ti.vcc_vobtag} -w ${ti.vcc_viewtag} -c "${ti.vcc_comment}" -f ${ti.vcc_file}  ${ti.vcc_flags} 
vcc_new_v_flags=

vcc_hist_command=\templates\ct\vcc\clearcase\vcc_clearcase_history.tcl -v ${ti.vcc_vobtag} -w ${ti.vcc_viewtag} -f ${ti.vcc_file} ${ti.vcc_flags}
vcc_hist_flags=

vcc_list_command=\templates\ct\vcc\clearcase\vcc_clearcase_ls.tcl -v ${ti.vcc_vobtag} -w ${ti.vcc_viewtag} ${ti.vcc_flags}
vcc_list_flags=

vcc_delete_command=\templates\ct\vcc\clearcase\vcc_clearcase_rmelem.tcl -v ${ti.vcc_vobtag} -w ${ti.vcc_viewtag} -c "${ti.vcc_comment}" -force -f ${ti.vcc_file} ${ti.vcc_flags}
vcc_delete_flags=

% ECR 6807
vcc_rename_command=\templates\ct\vcc\clearcase\vcc_clearcase_rename.tcl
vcc_rename_flags=
