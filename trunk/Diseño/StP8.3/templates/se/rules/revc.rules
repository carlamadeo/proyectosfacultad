Global 1 REDiagramGen
{
    { ClassCommand
        { Name "REDiagramGen" }
        { Label "Generate New Model..." }
	{ ReapplyFilterAfterExec All }
        { ShowPropsOnExec True }
        { Type Nowin }
        { Command "gense -forward_messages ${msgd_handle} -p ${projdir} -s ${system} -Desktop ${prop.diagrams} ${prop.Backup} ${prop.RepBuildType}" }
	{ Properties
	    { Choice
			{ Name "diagrams" }
			{ Label "Generate:" }
			{ Type CheckBox }
			{ Orientation Vertical }
			{ ChoiceItem
				{ Name "diagram_structure_charts" }
				{ Label "Structure Charts" }
				{ OnValue " -gensce" }
				{ OffValue "" }
				{ Init On }
			}
			{ ChoiceItem
				{ Name "diagram_data_structures" }
				{ Label "Data Structures" }
				{ OnValue " -gendse" }
				{ OffValue "" }
				{ Init On }
			}
			{ ChoiceItem
				{ Name "diagram_flow_charts" }
				{ Label "Flow Charts" }
				{ OnValue " -genfce" }
				{ OffValue "" }
				{ Init Off }
			}
	    }
	    { Button
			{ Name "ShowDiagramOpts" }
			{ Label "Options..." }
			{ Event
				{ Type Select }
				{ Callback "DiagramOptionsShow_cb" }
			}
	    }
	    { Choice
			{ Name "Backup" }
			{ Type CheckBox }
			{ ChoiceItem
    			{ Label "Backup Existing Diagrams" }
				{ Init On }
				{ OnValue "" }
				{ OffValue "-nobackup" }
			}
	    }
	    { Choice
			{ Name "RepBuildType" }
			{ Label "Build:" }
					{ Type AbbrMenu }
			{ ChoiceItem
				{ Label "Standard " }
				{ Init On }
				{ OnValue "" }
				{ OffValue "" }
			}
			{ ChoiceItem
				{ Label "Privileged (for DB Owners)" }
				{ Init Off }
				{ OnValue "-fast" }
				{ OffValue "" }
			}
	    }
        { Group
            { Name "top_line" }
            { Type Horizontal }
			// ECR 5850 start
			{ ChildSizePolicy ResizeNone }
			// ECR 5850 end
            { Members ( diagrams Backup ) }
        }
	    { Group
			{ Name "diagrams_group" }
			{ Label "Diagrams" }
			{ Type Vertical }
			// ECR 5850 start
			{ ChildSizePolicy ResizeNone }
			// ECR 5850 end
			{ HorizontalAlign AlignRight }
			{ Members ( top_line ShowDiagramOpts ) }
	    }            
	    { Group
			{ Name "repository_group" }
			{ Label "Repository" }
			// ECR 5850 start
			{ ChildSizePolicy ResizeNone }
			// ECR 5850 end
			{ Type Vertical }
			{ Members ( RepBuildType ) }
	    }
        { TopGroup
            { Align Left }
			// ECR 5850 start
			{ ChildSizePolicy ResizeNone }
			// ECR 5850 end
            { Members ( diagrams_group repository_group ) }
        }
	    { Qrl { QrlFile "qrl/properties/revc_cb.inc" } }
	    { Button
			{ Name "OK" }
			{ Event 
				{ Type Select }
				{ Callback "GenDiag_cb" }
			}
	    }
	}
    }
}
	



Global 1 RESynchronizer
{
    { ClassCommand
        { Name "RESynchronizer" }
        { Label "Synchronize Model..." }
        { ShowPropsOnExec True }
//fix for spr 2552 - Change below attribute to default value, none 
//	{ ReapplyFilterAfterExec All }
        { Type Nowin }
        { Command "gense -forward_messages ${msgd_handle} -p ${projdir} -s ${system} -Desktop ${prop.Operation}" }
	//{ Type NoWin }
	//{ Command "" }

	{ Properties
	    { Label "Synchronization" }
	    { Qrl { QrlFile "qrl/properties/revc_cb.inc" } }

	    { Choice
		{ Name "Operation" }
		//{ Label "Operation:" }
		{ Type AbbrMenu }
		{ ChoiceItem
		    { Label "Load Existing Differences" }
		    { OnValue "-syncexisting" }
		    { OffValue "" }
		}
		{ ChoiceItem
		    { Label "Find Differences" }
		    { OffValue "" }
		    { OnValue "-syncnew" }
		    { Init On }
		}
	    }

	    { Button
		{ Name "ShowDiagramOpts" }
		{ Label "Change Diagram Generation Options..." }
		{ Event
		    { Type Select }
		    { Callback "DiagramOptionsShow_cb" }
		}
	    }

	    { Choice
		{ Name "PartialSync" }
		{ Label "Synchronize Partial Model:" }
		{ Type CheckBox }
		{ ChoiceItem
		    { Label "" }
		    { Init Off }
		    { Activation
			{ Target "ShowFocusFiles" }
			{ On Activate }
		    }
		    { Activation
			{ Target "ShowFocusFiles" }
			{ Off InActivate }
		    }
		}
	    }

	    { Button
		{ Name "ShowFocusFiles" }
		{ Label "Choose Files for Partial Model..." }
		{ Event
		    { Type Select }
                    { Callback "FocusPopUp_cb" }
		}
	    }

	    { Button
		{ Name "OK" }
		{ Label "OK" }
		{ Event 
		    { Type Select }
		    { Callback "SyncSetOpts_cb" }
		}
	    }

	    { Button
		{ Name "Apply" }
		{ Label "Apply" }
		{ Event 
		    { Type Select }
		    { Callback "SyncSetOpts_cb" }
		}
	    }

	    { Button
		{ Name "Reset" }
		{ Label "Reset" }
		{ Event 
		    { Type Select }
		    { Callback "SE_generic_reset_cb" }
		}
	    }

	    { Group
		{ Name "SyncFirstLine" }
		{ Type Horizontal }
		{ Align Top }
		{ Members ( Operation ShowDiagramOpts ) }
	    }

	    { Group
		{ Name "SyncSecondLine" }
		{ Type Horizontal }
		{ Align Top }
		{ Members ( PartialSync ShowFocusFiles ) }
	    }

	    { Group
		{ Name "AllSyncOpts" }
		{ Type Vertical }
		{ Align Center }
		{ Members ( SyncFirstLine SyncSecondLine ) }
	    }
	}
    }
}
	


Global 1 REReportGen
{
    { ClassCommand
        { Name "REReportGen" }
        { Label "Generate C Code Metrics Reports..." }
        { ShowPropsOnExec True }
        { Type Nowin }
	{ Command "re_report -forward_messages ${msgd_handle} -p ${projdir} -s ${system} ${prop.ReportOperation} ${prop.TopFunctions} ${prop.ComplexFunctions} ${prop.ComplexSelect} ${prop.IncludeHier} ${prop.Uncommented} ${prop.FunctionDefs} ${prop.ProtoSelect} ${prop.FunctionLocalization} ${prop.CharactersPerLine} ${prop.FieldSpacing} '-lpr${prop.PrintCommand}'" }

	{ Properties
	    { Label "Metrics Reports" }
	    { Qrl { QrlFile "qrl/properties/revc_cb.inc" } }

	    { Choice
		{ Name "TopFunctions" }
		{ Type CheckBox }
		{ Label "" }
                { ChoiceItem
		    { Label "Top Function Report" }
                    { Init On }
                    { OnValue "-topfun" }
                    { OffValue "" }
		}
	    }

	    { Choice
		{ Name "ComplexFunctions" }
		{ Type CheckBox }
		{ Label "" }
                { ChoiceItem
		    { Label "Complex Function Report" }
                    { Init On }
                    { OnValue "-complex" }
                    { OffValue "" }
		    { Activation
			{ Target "ComplexSelect" }
			{ On Activate }
		    }
		    { Activation
			{ Target "ComplexSelect" }
			{ Off InActivate }
		    }
		}
	    }

	    { Choice
		{ Name "FunctionDefs" }
		{ Label "" }
		{ Type CheckBox }
                { ChoiceItem
                    { Label "Function Definition Report" }
                    { Init Off }
                    { OnValue "-fundef" }
                    { OffValue "" }
		    { Activation
			{ Target "ProtoSelect" }
			{ On Activate }
		    }
		    { Activation
			{ Target "ProtoSelect" }
			{ Off InActivate }
		    }
		}
	    }


	    { Choice
		{ Name "IncludeHier" }
		{ Label "" }
		{ Type CheckBox }
                { ChoiceItem
                    { Label "Include Hierarchy Report" }
                    { Init Off }
                    { OnValue "-include" }
                    { OffValue "" }
		}
	    }

	    { Choice
		{ Name "Uncommented" }
		{ Type CheckBox }
		{ Label "" }
                { ChoiceItem
		    { Label "Uncommented Objects Report" }
                    { Init Off }
                    { OnValue "-uncommented" }
                    { OffValue "" }
		}
	    }

	    { Choice
		{ Name "FunctionLocalization" }
		{ Label "" }
		{ Type CheckBox }
                { ChoiceItem
                    { Label "Function Localization Report" }
                    { Init Off }
                    { OnValue "-funscope" }
                    { OffValue "" }
		}
	    }

	    { Group
		{ Name "Reports" }
		{ Type Vertical }
		{ Align Labels }
		{ Members ( TopFunctions ComplexFunctions IncludeHier FunctionDefs FunctionLocalization Uncommented ) }
	    }

	    { Choice
		{ Name "ReportOperation" }
		{ Label "Operation:" }
		{ Type Exclusive }
                { ChoiceItem
                    { Label "Generate        " }
                    { Init On }
                    { OnValue "-generate" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "View        " }
                    { Init Off }
                    { OnValue "-view" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "Print        " }
                    { Init Off }
                    { OnValue "-print" }
                    { OffValue "" }
                    { Activation
                        { Target "PrintCommand" }
                        { On Activate }
                    }
                    { Activation
                        { Target "PrintCommand" }
                        { Off InActivate }
                    }

                }
	    }

	    { Text
		{ Name "PrintCommand" }
		{ Label "Print Command:" }
		{ MinLength 1 }
		{ DispLength 20 }
		//{ MaxLength  }
		// { InitVal "mp -lo @ | lpr" }
		//{ InitVal "lpr @" }
                // default command and printer is selected 
                // if not specify a valid command with "@" for file substitution
		{ InitVal " " }
	    }


	    { Choice
		{ Name "CharactersPerLine" }
		{ Label "Characters Per Line:" }
		{ Type AbbrMenu }
                { ChoiceItem
                    { Label "65" }
                    { Init Off }
                    { OnValue "-w 65" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "80" }
                    { Init Off }
                    { OnValue "-w 80" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "100" }
                    { Init Off }
                    { OnValue "-w 100" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "120" }
                    { Init Off }
                    { OnValue "-w 120" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "As Is " }
                    { Init On }
                    { OnValue "-w 1000" }
                    { OffValue "" }
                }
	    }

	    { Choice
		{ Name "FieldSpacing" }
		{ Label "Field Delimiter:" }
		{ Type AbbrMenu }
                { ChoiceItem
                    { Label "1 Space" }
                    { Init Off }
                    { OnValue "-t 0" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "4 Spaces" }
                    { Init Off }
                    { OnValue "-t 4" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "8 Spaces" }
                    { Init On }
                    { OnValue "-t 8" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "Tab" }
                    { Init Off }
                    { OnValue "-t 0" }
                    { OffValue "" }
                }
	    }

	    { Text
		{ Name "ComplexSelect" }
		{ Label "Minimum Complexity:" }
		{ MinLength 1 }
		{ DispLength 3 }
		{ MaxLength 10 }
		{ InitVal "10" }
	    }

	    { Choice
		{ Name "ProtoSelect" }
		{ Label "Function Definitions:" }
		{ Type AbbrMenu }
                { ChoiceItem
                    { Label "As ANSI Prototypes" }
                    { Init On }
                    { OnValue "-ansi" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "As K+R Prototypes" }
                    { Init Off }
                    { OnValue "-kr" }
                    { OffValue "" }
                }
                { ChoiceItem
                    { Label "As a Sorted List" }
                    { Init Off }
                    { OnValue "" }
                    { OffValue "" }
                }
	    }

	    { Group
		{ Name "ReportButtons" }
		{ Type Vertical }
		{ Align Left }
		{ VerticalSpacing 30 }
		{ Members ( CharactersPerLine FieldSpacing PrintCommand ComplexSelect ProtoSelect ) }
	    }

	    { Group
		{ Name "ReportBody" }
		{ Type Horizontal }
		{ Align Top }
		{ HorizontalSpacing 80 }
		{ Members ( Reports ReportButtons ) }
	    }

	    { Group
		{ Name "WholeReport" }
		{ Type Vertical }
		{ Align Center }
		{ Members ( ReportOperation ReportBody ) }
	    }
	}
    }
}



