		    { Activation
			{ Target "ExactMatch" }
			{ On InActivate }
		    }
		}
		{ ChoiceItem
		    { Label "Constant:" }
                    { OnValue "-T constant" }
                    { OffValue "" }
		    { Activation
			{ Target "ExactMatch" }
			{ On InActivate }
		    }
		}
		{ ChoiceItem
		    { Label "Typedef:" }
                    { OnValue "-T typedef" }
                    { OffValue "" }
		    { Activation
			{ Target "ExactMatch" }
			{ On InActivate }
		    }
		}
		{ ChoiceItem
		    { Label "Literal Value:" }
                    { OnValue "-T literal" }
                    { OffValue "" }
		    { Activation
			{ Target "ExactMatch" }
			{ On Activate }
		    }
		    { Activation
			{ Target "Definitions" }
			{ On InActivate }
		    }
		    { Activation
			{ Target "Definitions" }
			{ Off Activate }
		    }
		    { Activation
			{ Target "ReadAccess" }
			{ On InActivate }
		    }
		    { Activation
			{ Target "ReadAccess" }
			{ Off Activate }
		    }
		    { Activation
			{ Target "WriteAccess" }
			{ On InActivate }
		    }
		    { Activation
			{ Target "WriteAccess" }
			{ Off Activate }
		    }
		}
		{ ChoiceItem
		    { Label "Data Structure:" }
                    { OnValue "-T datas" }
                    { OffValue "" }
		    { Activation
			{ Target "ExactMatch" }
			{ On InActivate }
		    }
		}
		{ ChoiceItem
