Global 1 DfeCheckDecomp
{
    { Properties
	{ Choice
	    { Name "CheckDecomp" }
	    { Label "" }
	    { Type CheckBox }
	    { ChoiceItem
		{ Label "OffPage connections balance with parent Process" }
		{ OnValue "-x decomp '1'" }
		{ OffValue "-x decomp '0'" }
		{ Init On }
	    }
	}
    }
}

