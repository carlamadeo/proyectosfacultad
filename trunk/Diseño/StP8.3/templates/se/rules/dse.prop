            { Event
                { Type Select }
                { Callback "Dse2167ADialog_Apply" }
            }
        }
        { Button
            { Name "Apply" }
            { Event
                { Type Select }
                { Callback "Dse2167ADialog_Apply" }
            }
        }
        { Button
            { Name "Reset" }
            //{ Label "Reset From Selected Object" }
            { Label "Reset" }
            { Event
                { Type Select }
                { Callback "Dse2167ADialog_Reset" }
            }
        }
        { Button
            { Name "Cancel" }
            { Label "Cancel" }
            { Event
                { Type Select }
                { Callback "Dse2167ADialog_Cancel" }
            }
        }
        { Button
            { Name "Help" }
            { Label "Help" }
        }
        { Footer
            { Name "Status" }
            { LeftFooter "" }
            { Active True }
        }
    }
}

PropertySheet DSESemanticChecks
{   
    { Name "DSESemanticChecks" }
    { Label "Check Semantics Selectively"}
    { ResetAfterExec False }
    { UseProperty DseCheckOne }
    { UseProperty DseCheckTwo }
    { UseProperty DseVerbose }
    { Properties
        { Qrl { QrlFile "qrl/properties/dse_props.inc" } }
        { Button
            { Name "OK" }
            { Event
                { Type Select }
                { Callback "OkDseCheckSemantics" }
            }
        }
        { Button
            { Name "Apply" }
            { Event
                { Type Select }
                { Callback "ApplyDseCheckSemantics" }
            }
        }
	{ Group
	    { Name "Top" }
	    { Type Vertical }
	    { Align Labels }
	    { Members ( check_one check_two verbose ) }
	}
	{ Group
	    { Name "All" }
	    { Type Vertical }
	    { Members (Top Buttons)}
	    { VerticalAlign Center}
	}
    }
}


Include "se/rules/dse_check.rules"
