        }
        { Text
            { Name "Units" }
            { Label "Units: " }
            { MinLength 0 }
            { DispLength 40 }
            { MaxLength 512 }
        }
        { Text
            { Name "Scaling" }
            { Label "Scaling: " }
            { MinLength 0 }
            { DispLength 40 }
            { MaxLength 512 }
        }
        { Text
            { Name "Precision" }
            { Label "Precision: " }
            { MinLength 0 }
            { DispLength 40 }
            { MaxLength 512 }
        }
        { Text
            { Name "Accuracy" }
            { Label "Accuracy: " }
            { MinLength 0 }
            { DispLength 40 }
            { MaxLength 512 }
        }
        { Text
            { Name "Conventions" }
            { Label "Conventions: " }
            { MinLength 0 }
            { DispLength 40 }
