NodeSymbol External
{
    { Shape 
        { Predefined Rectangle } 
        { ToolbarImage "demarco_external" }
    }
}

NodeSymbol Process
{
    { Shape 
        { Predefined Ellipse } 
        { ToolbarImage "demarco_process" }
    }
