Global 1 ReportIncludeSceDiagrams
{
    { Properties
        { Choice
            { Name "include_sce_diagrams" }
            { Label "Include SCE Diagrams" }
            { Type CheckBox }
            { ChoiceItem
                { Name "include_sce_diagrams" }
                { Label "" }
                { OnValue "-x include_sce_diagrams 'True'" }
                { OffValue "-x include_sce_diagrams 'False'" }
                { Init On }
            }
        }
    }
}


