
Global 1 ReportTargetOutputFormat
{
    { Properties
        { Choice
            { Name "output_target" }
            { Label "Use Target Format:" }
            { Type Exclusive }
            
            { ChoiceItem
                { Label "RTF (Microsoft Word)" }
                { Value "rtf" }
                { Activation
                    { Target "output_include_diagrams" }
                    { On Activate }
                }
            }
			{ ChoiceItem
                { Label "HTML" }
                { Value "html" }
                { Activation
                    { Target "output_include_diagrams" }
                    { On Activate }
                }
            }
			{ ChoiceItem
                { Label "FrameMaker" }
                { Value "mif" }
                { Activation
                    { Target "output_include_diagrams" }
                    { On Activate }
                }
            }    
 //           { ChoiceItem
 //               { Label "Interleaf" }
 //               { Value "leaf" }
 //               { Activation
 //                   { Target "output_include_diagrams" }
 //                   { On Activate }
 //               }
 //           }        
            { ChoiceItem
                { Label "ASCII" }
                { Value "ascii" }
                { Activation
                    { Target "output_include_diagrams" }
                    { On InActivate }
                }
	    }
	}
    }
}


Global 1 ReportSymbols
{
    { Properties
        { Choice
            { Name "output_symbols" }
            { Label "Use Symbol Set:" }
            { Type Exclusive }
            { ChoiceItem
                { Label "DeMarco/Yourdon" }
                { Value "-x output_symbols dydfe" }
            }    
            { ChoiceItem
                { Label "Gane/Sarson" }
                { Value "-x output_symbols gsdfe" }
            }    
	}
    }
}




Global 1 ReportDefaultOutputDirAndFileObjectsProperty
{
    { Properties
        { Text
            { Name "output_dir" }
            { Label "Output File Directory:" }
            { Type Text }
            { MinLength 0 }
            { MaxLength 300 }
            { DispLength 40 }
            { InitVal "${projdir}${system}/qrl_files" }
        }
        // ECR 3651a begin
        { Button
            { Name "browse_for_out_dir" }
            { Label "Choose" }
            { Event
                { Type Select }
                { Callback "select_output_dir_callback" }
            }
        }
        { Group
            { Name "output_file_group" }
            { Label "" }
            { Type Horizontal }
            //ECR 5833 begin - commented out next lines
            //{ Resizable ResizeAny }
            //{ ChildSizePolicy ResizeWidth }
            //{ LayoutPolicy ResizeAny }
            { Align Labels }
            //ECR 5833 end
            { Members ( output_dir browse_for_out_dir ) }
        }
        // ECR 3651a end
        { Text
            { Name "output_file_design" }
            { Label "Output File Name:" }
            { Type Text }
            { MinLength 0 }
            { MaxLength 100 }
            { DispLength 20 }
            { InitVal "DesignReport" }
        }
        { Group
            { Name "main_output_file_group" }       //ECR 5833
            { Type Vertical }
            { Align Labels }
            // ECR 3651a begin
            // { Members ( output_dir output_file_design ) }
            { Members ( output_file_group output_file_design ) }
            // ECR 3651a end
    	}
    }
}

