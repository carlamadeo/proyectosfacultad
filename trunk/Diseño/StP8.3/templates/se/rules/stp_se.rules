Global 1 CheckSemanticsWholeModelProperty
{
    { Properties
        { Name "CheckSemanticsWholeModelProperty" }
        { Qrl { QrlFile "se/qrl/check/semantic_check_whole_model.qrl" } }
        { Footer
            { Name "Footer1" }
            { LeftFooter "" }
        }
        { Text
            { Name "out_dir" }
            { Label "Output File:" }
            { Type Text }
            { MinLength 0 }
            { DispLength 52 }
            { InActive True }
            { Resizable ResizeNone }
        }
        // ECR 3651a begin
        { Button
            { Name "browse_for_out_file" }
            { Label "Choose" }
            { InActive True }
            { Event
                { Type Select }
                { Callback "select_out_file_callback" }
            }
        }
        { Group
            { Name "out_file_group" }
            { Label "" }
            { Type Horizontal }
            { Resizable ResizeAny }
            { ChildSizePolicy ResizeWidth }
            { LayoutPolicy ResizeAny }
// ECR 6616 begin
            { Align Labels }
            //{ Align Bottom }
// ECR 6616 end
            { Members ( out_dir browse_for_out_file ) }
        }
        // ECR 3651a end
        { Choice
            { Name "out_form" }
            { Label "Output Format:" }
            { Type AbbrMenu }
            { Event
               { Type Select }
               { Callback "select_format_check_sem_cb" }
            }
            { ChoiceItem
                { Name "FM" }
                { Label "FrameMaker                                                      " }
            }
            { ChoiceItem
                { Name "RTF" }
                { Label "RTF (Microsoft Word)                                            " }
            }
            { ChoiceItem
                { Name "HTML" }
                { Label "HTML File                                                       " }
            }
            { ChoiceItem
                { Name "ML" }
                { Label "Message Log                                                     " }
                { Init On }
            }
        } 
// ECR 7410 begin
        { Choice
            { ParseCondition "is_windows_platform();"}
            { Name "view_after_run" }
            { Label "" }
            { Type CheckBox }
            { Active False }
            { ChoiceItem
                { Label "View After Run" }
                { OffValue "" }
                { OnValue " -viewafter" }
                { Init On }
            }
        }
// ECR 7410 end
        { Group
           { Name "TheGroup" }
           { Type Vertical }
           { HorizontalAlign AlignLabels }
           //ECR 5468 begin
           //{ Members ( out_form out_dir ) }
           { Members ( out_form view_after_run ) }
           //ECR 5468 end
        } 
    }
}

