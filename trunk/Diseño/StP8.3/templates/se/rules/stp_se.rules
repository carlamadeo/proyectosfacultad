        { Name "GenerateAnalysisReviewReportClass" }
        { Label "Generate Analysis Review Report..." }
        { Command "" }
        { UseProperty ReportTargetOutputFormat }
        { UseProperty ReportSymbols }
        { UseProperty ReportDefaultOutputDirAndFileObjectsPropertyAnalysis }
        { UseProperty ReportIncludeCspecTables }
//        { UseProperty ReportIncludeDseDiagrams }
        { UseProperty ReportIncludeAnnotations }
        { UseProperty ReportIncludeAllFileHistory }
        { UseProperty ReportVerbose }
        { Properties
            { Group
                { Name "Top" }
                { Type Vertical }
                { Align Labels }
                //ECR 5832 begin
                { Resizable ResizeNone }
                { ChildSizePolicy ResizeNone }
                { Members ( output_target output_symbols output_file_group_analysis include_cspec_tables include_annotations include_all_file_history verbose ) }
                //ECR 5832 end
            }
            { Group
                { Name "All" }
                { Type Vertical }
                { Members ( Top Buttons) }
                { VerticalAlign Center }
            }
        }
    }    
}

