Global 1 OutputDirProperty
{
    { Properties
	    { Text
	        { Name "output_dir" }
	        { Label "Output File Directory:" }
	        { Type Text }
    	    { MinLength 1 }
    	    { MaxLength 300 }
    	    { DispLength 40 }
	        { InitVal "${projdir}${system}/${filestore}" }
	    }
        // ECR 3651a begin
        { Button
            { Name "browse_for_out_dir" }
            { Label "Choose" }
            { Event
                { Type Select }
                { Callback "select_output_dir_callback" }
            }
        }
        { Group
            { Name "output_file_group" }
            { Label "" }
            { Type Horizontal }
            { Resizable ResizeAny }
            { ChildSizePolicy ResizeWidth }
            { LayoutPolicy ResizeAny }
            { Align Bottom }
            { Members ( output_dir browse_for_out_dir ) }
        }
        // ECR 3651a end
    }
}

