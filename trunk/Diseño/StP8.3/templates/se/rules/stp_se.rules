Global 1 GenerateDesignReviewReportClass
{
    { ClassCommand
        { Name "GenerateDesignReviewReportClass" }
        { Label "Generate Design Review Report..." }
        { Command "" }
        { UseProperty ReportTargetOutputFormat }
        { UseProperty ReportDefaultOutputDirAndFileObjectsProperty }
        { UseProperty ReportIncludeSceDiagrams }
        { UseProperty ReportIncludeDseDiagrams }
        { UseProperty ReportIncludeAnnotations }
//        { UseProperty ReportIncludeSourceListings }
        { UseProperty ReportIncludeAllFileHistory }
        { UseProperty ReportVerbose }
        { Properties
            { Group
                { Name "Top" }
                { Type Vertical }
                { Align Labels }
                //ECR 5833 begin
                { Resizable ResizeNone }
                { ChildSizePolicy ResizeNone }
                { Members ( output_target main_output_file_group include_sce_diagrams include_dse_diagrams include_annotations include_all_file_history verbose ) }
                //ECR 5833 end
            }
            { Group
                { Name "All" }
