// gsdfe.rules: the gane-sarson dfe editor

Include "se/rules/dfe.rules"

NodeSymbol External
{
    // ECR 4958 begin
    { AutoScaleLarger True }
    // ECR 4958 end
    { Shape
        { Predefined ShadowBox }
        { ToolbarImage "gane_external" }
    }
    { DisplayMark
        { Name DuplicateExternal }
        { CondFunc "GSCondFunc(${node.id});" }
	//{ Condition "node[id == ${node.id} && count[node_refs] > 1];" }
	{ BitmapMark0 "dupext64" }
	{ BitmapMark1 "dupext48" }
	{ BitmapMark2 "dupext32" }
        { Anchor LowerRight }
        { HAdjust Right }
        { VAdjust Bottom }
        { XPixelOffset 38 }
        { YPixelOffset 18 }
	{ Help "Shows up if External has multiple references" }
    }
}

NodeSymbol Process
{
    // ECR 4958 begin
    { AutoScaleLarger True }
    // ECR 4958 end
    { Shape 
        { Predefined GanesProcess } 
        { ToolbarImage "gane_process" }
    }
    //{ DefaultSize 1.0 1.0 }
    { DisplayMark
        { Name Pindex }
        { YPixelOffset -2 }
    }
}

NodeSymbol Store
{
    // ECR 4958 begin
    { AutoScaleLarger True }
    // ECR 4958 end
    { Shape 
        { Predefined GanesStore }
        { ToolbarImage "gane_store" } 
    }
    { DisplayMark
        { Name DuplicateStore }
	{ CondFunc "GSCondFunc(${node.id});" }
	//{ Condition "node[id == ${node.id} && node_refs];" }
	//{ Condition "node[id == ${node.id} && count[node_refs] > 1];" }
	//{ Condition "node[name = 'sdfsdf' && count[node_refs] > 1];" }
	{ BitmapMark0 "dupgstore64" }
	{ BitmapMark1 "dupgstore48" }
	{ BitmapMark2 "dupgstore32" }
        { Anchor  CenterLeft }
        { HAdjust Left }
        { VAdjust Center }
        { XPixelOffset 5 }
        { YPixelOffset 0 }
	{ Help "Shows up if Store has multiple references" }
    }
}

DisplayMarkGroups
{
    { ContinuousDisplayMarks ( Pindex FlowType DuplicateStore DuplicateExternal ) }
}

