
Class 0 SysRequirementsAnalysisJointReview
{
    { ParseCondition "(toolinfo_variable(\"SysRqtsModeler\") == \"True\");" }
    { Label "Joint Reviews (Use Cases)" }
    { Image "use_cases" }
    { ShowOnDesktop True }

 { ObjectCommand
        { Name "GenerateReviewReport" }
	{ Label "Generate Use Case Review Report ..." }
	{ ShowPropsOnExec True }
        { SingleObject True } 
	{ Type Nowin }
	{ Command "qrp -forward_message ${msgd_handle} -p ${projdir} -s ${system}  -x use_case_name '${cmd.objects}' -x DocumentFormat '${prop.DocumentFormat}'  -x RequirementsTools '${prop.RequirementsTools}'  -t '${prop.DocumentFormat}' -o ${projdir}${system}/qrl_files/Review_${cmd.objects}.${prop.DocumentFormat} support/qrl/GenerateOneReviewDocument.qrl"}
        { UseProperty ValRequirementsTools}
        { UseProperty ValDocumentFormat }
  }
}



