// StP/Validator


Category CatModel
{ 
    { ShowOnDesktop True }
    { ParseCondition "is_validator_available(\"integrated\");" }
    { Group
         (
             TestTypes
          )
    }
}
 

Category CatValidator
{
    { ShowOnDesktop True }
    { ParseCondition "is_validator_available(\"integrated\") && toolinfo_variable(\"SRI\") != \"True\";" }
    { Label "Validation" }
    { Group
         (
             Validator
             ValidatorCustom
          )
    }
}

Category CatValidator
{
    { ShowOnDesktop True }
    { ParseCondition "is_validator_available(\"integrated\") && toolinfo_variable(\"SRI\") == \"True\";" }
    { Label "Validation" }
    { Group
         (
             Validator
             STW
             ValidatorCustom
          )
    }    
}



