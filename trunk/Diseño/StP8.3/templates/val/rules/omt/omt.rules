LinkSymbol EventTrace
{
  { DisplayMark
      { Name HasParameters }
      { MappingName EventInstanceMapping }
      { ItemCond (TDataItemName) }
      { StringMark "D" }
      { Anchor FromEnd }
      { VAdjust Top }
      { HAdjust Center }
      { XPixelOffset 10 }
      { YPixelOffset 0 }
  }
  { DisplayMark
      { Name HasConditions }
      { MappingName EventInstanceMapping }
      { ItemCond (TActsOnlyIf) }
      { StringMark "C" }
      { Anchor FromEnd }
      { VAdjust Top }
      { HAdjust Center }
      { XPixelOffset 20 }
      { YPixelOffset 0 }
  }
  { DisplayMark
      { Name HasExpectedOutput }
      { MappingName EventInstanceMapping }
      { ItemCond (HasExpectedOutput) }
      { StringMark "O" }
      { Anchor ToEnd }
      { VAdjust Top }
      { HAdjust Center }
      { XPixelOffset 20 }
      { YPixelOffset 0 }
  }
}

