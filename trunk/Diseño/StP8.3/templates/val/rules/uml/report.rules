
Global TTargetFormat
{
    { Properties
        { Choice
            { Name "targetformat" }
            { Label "Report format:" }
            { Type AbbrMenu }
            { ChoiceItem
                { Label "HTML" }
                { Value "html" }
                { Init On }
            }
            { ChoiceItem
                { Label "Adobe Frame Maker(MIF)" }
                { Value "mif" }
            }
            { ChoiceItem
                { Label "MS Word(RTF)" }
                { Value "rtf" }
            }
        }
    }
}


Global 7 TTFormatFile
{
    { Properties
        { Text
            { Name "tformatfile" }
            { Label "Format File:" }
            //{ Init "stpreport" }
            { Type Text }
            { MinLength 1 }
            { MaxLength 80 }
            { DispLength 20 }
        }
    }
}

