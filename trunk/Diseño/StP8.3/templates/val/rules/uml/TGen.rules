Global 5 TTVerbose
{
    { Properties
        { Choice
            { Name "verbose" }
            { Label "Verbose:" }
            { Type CheckBox }
            { ChoiceItem
                { Label "" }
                { OnValue "-x TVerboseFlag On" }
                { OffValue "" }
                { Init On }
            }
        }
    }
}
 



 
Global 5 TTHier
{
    { Properties
        { Choice
            { Name "thier" }
            { Label "Descend Hierarchy:" }
            { Type CheckBox }
            { ChoiceItem
                { Label "" }
                { OnValue "-x THierarchy Yes" }
                { OffValue "-x THierarchy No" }
                { Init On }
            }
        }
    }
}
 


 
Global 5 TTPreProc
{
    { Properties
        { Choice
            { Name "tpreproc" }
            { Label "Preprocessed:" }
            { Type CheckBox }
            { ChoiceItem
                { Label "" }
                { OnValue "-f" }
                { OffValue "" }
                { Init Off }
            }
        }
    }
}
 



Global 5 TTNameOrPnum
{
    { Properties
        { Choice
            { Name "tnameorpnum" }
            { Label "Use Process:" }
            { Type AbbrMenu }
            { ChoiceItem
                { Label "Name" }
                { Value "-x TNameOrPnum Name" }
		{ Init On }
            }
            { ChoiceItem
                { Label "Process Number" }
                { Value "-x TNameOrPnum Pnum" }
            }
        }
    }
}



 



Global 5 TTMsgLevel
{
    { Properties
        { Choice
            { Name "tmsglevel" }
            { Label "Message Level:" }
            { Type AbbrMenu }
            { ChoiceItem
                { Label "0" }
                { Value "0" }
            }
            { ChoiceItem
                { Label "1" }
                { Value "1" }
            }
            { ChoiceItem
                { Label "2" }
                { Value "2" }
            }
            { ChoiceItem
                { Label "3" }
                { Value "3" }
		{ Init On }
            }
        }
    }
}




Global 5 TTSummaryLineSpacing
{
    { Properties
        { Choice
            { Name "tsummarylinespacing" }
            { Label "Summary Line Spacing:" }
            { Type AbbrMenu }
            { ChoiceItem
                { Label "Single" }
                { Value "1" }
		{ Init On }
            }
            { ChoiceItem
                { Label "Double" }
                { Value "2" }
            }
        }
    }
}


// Select this option if you wish to list the extra sdf information.

Global 5 TTListExtras
{
    { Properties
        { Choice
            { Name "tlistextras" }
            { Label "List Extras:" }
            { Type CheckBox }
            { ChoiceItem
                { Label "" }
                { OffValue "FALSE" }
                { OnValue "TRUE" }
                { Init On }
            }
        }
    }
}


// Select this option if you wish to require preconditions are TRUE.

Global 5 TTSatisfyPreconditions
{
    { Properties
        { Choice
            { Name "tsatisfypreconditions" }
            { Label "Satisfy Preconditions:" }
            { Type CheckBox }
            { ChoiceItem
                { Label "" }
                { OffValue "FALSE" }
                { OnValue "TRUE" }
                { Init Off }
            }
        }
    }
}




