
// Annotations added for StP/Validator


//
// 	Note 804003 below is a redefinition of "annoation/t_dataitem.note"
//

Note 804003
{
    { AppType TestDataItem }
    { HelpText "The Test Parameter note makes parameters to operations in OME and FME testable. There can be zero or more of these notes." }
    { Bounds { Min 0 } }
    { PrintName "Data Definition" }
    { Item
	{ AppType TDataItemName }
	{ HelpText "The name of this Parameter. Change the default name to the name of the parameter being defined." }
	{ Bounds { Min 1 } { Max 1 } }
	{ PrintName "Parameter Name" }
	{ IsKey True }
	{ InitValue  "" }
    }
    { Item
	{ AppType TDataTypeName }
	{ HelpText "The type of this parameter.  Can be either be an StP/T primitive type (integer, string, boolean, character, real, unspecified) or a user defined type, which must be defined in the Test Type Editor. This field is required for the parameter to participate in testing." }
	{ Bounds { Min 0 } { Max 1 } }
	{ PrintName "User Defined Data Type" }
    }
    { Item
        { AppType TDataType }
        { HelpText "Predefined data types as defined in tsdl.std." }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Predefined T Data Type" }
    }
    { Item
        { AppType TFixedValue }
        { HelpText "Data item with a fixed value." }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Fixed Value" }
    }
    { Item
        { AppType TExhaustiveTest }
        { HelpText "Test Exhaustively On." }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Test Exhaustively On" }
	{ InitValue  "True" }
        { IsReadOnly True }
    }
}


