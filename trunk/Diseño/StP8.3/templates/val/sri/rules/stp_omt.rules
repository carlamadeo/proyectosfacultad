

Class 0 STW
{
  { Label "STW Integration" }
  { Image "icon/oousecase_32" }
  { ShowOnDesktop True }
  { OMSObjects
      { Match "node[OMTUseCase && node_refs]" }
  } 
}


Global 5 STWSource
{
    { Properties
	{ Text
	    { Name "Source" }
	    { Label "Source Command:" }
	    { Init "Play a keysave file that saves 1 ascii file." }
    	    { Type Text }
    	    { MinLength 1 }
	    { MaxLength 80 }
	    { DispLength 80 }
	}
    }
}



 
Global 5 STWActivation
{
    { Properties
	{ Text
	    { Name "Activation" }
	    { Label "Activation Command:" }
	    { Init "Xplabak5 -k TESTCASE.ksv" }
    	    { Type Text }
    	    { MinLength 1 }
	    { MaxLength 80 }
	    { DispLength 80 }
	}
    }
}



 
Global 5 STWEvaluationCommand
{
    { Properties
        { Choice
            { Name "EvaluationCommand" }
	    { Label "Evaluation Command:" }
            { Type AbbrMenu }
            { ChoiceItem
                { Label "No Evaluation" }
                { OnValue "True" }
                { OffValue "False" }
		{ Init On }
		{ Activation
		    { Target "Evaluation" }
                    { On InActivate }
                }
            }
            { ChoiceItem
                { Label "Evaluation With User" }
                { OnValue "True" }
                { OffValue "False" }
		{ Activation
		    { Target "Evaluation" }
                    { On InActivate }
                }
            }
            { ChoiceItem
                { Label "Evaluation With Baseline" }
                { OnValue "True" }
                { OffValue "False" }
		{ Activation
		    { Target "Evaluation" }
                    { On Activate }
                }
            }
            { ChoiceItem
                { Label "Evaluation With Function" }
                { OnValue "True" }
                { OffValue "False" }
		{ Activation
		    { Target "Evaluation" }
                    { On Activate }
                }
            }
        }
    }
}



 
Global 5 STWEvaluation
{
    { Properties
	{ Text
	    { Name "Evaluation" }
	    { Label "Evaluation Text:" }
	    { Init "\"TESTCASE.rsl\" vs. \"TESTCASE.bsl\"" }
    	    { Type Text }
    	    { MinLength 1 }
	    { MaxLength 80 }
	    { DispLength 80 }
	}
    }
}


 
Global 5 STWEnvironment
{
    { Properties
	{ Text
	    { Name "Environment" }
	    { Label "Environment Command:" }
	    { Init "NONE" }
    	    { Type Text }
    	    { MinLength 1 }
	    { MaxLength 80 }
	    { DispLength 80 }
	}
    }
}


 
Global 5 STWTermination
{
    { Properties
	{ Text
	    { Name "Termination" }
	    { Label "Termination Command:" }
	    { Init "NONE" }
    	    { Type Text }
    	    { MinLength 1 }
	    { MaxLength 80 }
	    { DispLength 80 }
	}
    }
}



Class STW
{
    { ObjectCommand
        { Name "GenerateATS" }
	{ Label "Generate ATS Template ..." }
	{ ShowPropsOnExec True }
	{ Type Normal }
	{ Command "qrp -p ${projdir} -s ${system}  -x UseCases '${cmd.objects}' -x STWSource '${prop.Source}' -x STWActivation '${prop.Activation}' -x STWEvaluation '${prop.Evaluation}'  -x STWEvaluationCommand '${prop.EvaluationCommand}'  -x STWEnvironment '${prop.Environment}' -x STWTermination '${prop.Termination}' val/sri/qrl/GenerateATS.qrl" }
        { UseProperty STWSource }
        { UseProperty STWActivation }
        { UseProperty STWEvaluation }
        { UseProperty STWEvaluationCommand }
        { UseProperty STWEnvironment }
        { UseProperty STWTermination }
        { Properties
	    { Activation
	        { Target "Source" }
                { On Activate }
            }
	    { Activation
	        { Target "Activation" }
                { On Activate }
            }
	    { Activation
	        { Target "EvaluationCommand" }
                { On Activate }
            }
	    { Activation
	        { Target "Evaluation" }
                { On Activate }
            }
	    { Activation
	        { Target "Environment" }
                { On Activate }
            }
	    { Activation
	        { Target "Termination" }
                { On Activate }
            }
	    { Footer
		{ Name "Footer1" }
		{ LeftFooter "Choose Operation and Properties" }
	    }
    	    { Activation 
		{ Target "Footer1" }
		{ OnInit Active }
	    }
    	    { Footer
    	    	{ Name "Footer2" }
    	    	{ RightFooter "Use Case Test Script Generator" }
    	    }
    	    { Activation 
		{ Target "Footer2" }
		{ OnInit Active }
	    }
		
	    { Group
		{ Name "options2" }
		{ Type Vertical }
                { Align Labels }
    	    	{ Align Bottom }
		{ Members ( Source Activation EvaluationCommand Evaluation Environment Termination) }
	    }
	    { Group
	  	{ Name "midcolumn" }
	  	{ Type Vertical }
	  	{ Members ( options2 ) }
	    }
    	    { Group
    	    	{ Name "All" }
    	    	{ Type Horizontal }
		{ Members (  midcolumn) }
    	    }
    	}
    }


    { ObjectCommand
        { Name "GenerateRC" }
	{ Label "Generate Resource Configuration File" }
	{ Command "qrp -p ${projdir} -s ${system}  -x UseCases '${cmd.objects}' val/sri/qrl/GenerateRC.qrl" }
    }



    { ObjectCommand
        { Name "GenerateCDF" }
	{ Label "Generate CDF Template" }
	{ Command "qrp -p ${projdir} -s ${system}  -x UseCases '${cmd.objects}' val/sri/qrl/GenerateCDF.qrl" }
    }



    { ObjectCommand
        { Name "GenerateKSV" }
	{ Label "Generate Key Save Files" }
	{ Command "qrp -p ${projdir} -s ${system}  -x UseCases '${cmd.objects}'   val/sri/qrl/GenerateKSV.qrl" }
    }


    { ObjectCommand
        { Name "ExecuteSMARTS" }
	{ Label "Execute SMARTS" }
	{ Command "qrp -p ${projdir} -s ${system}  -x UseCases '${cmd.objects}' val/sri/qrl/ExecuteSMARTS.qrl" }
    }


} 
