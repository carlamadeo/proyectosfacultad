Global 1 UmlNameProp
{
    { Properties
        { Text
            { Name "ObjectName" }
            { Label "Name:" }
            { MinLength 1 }
            { DispLength 25 }
            { Resizable ResizeNone }
            { Event
                { Type Text }
                { Callback "ps_ValueChanged_cb" }
            }
        }
    }
}

