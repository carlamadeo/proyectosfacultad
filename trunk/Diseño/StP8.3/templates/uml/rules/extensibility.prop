// This is the new button that will be introduced into
// the property sheets of all objects, so that the description
// can be modified
//
Global 1 UmlObjectDescriptionButton
{
	
	{ Properties
        
        { Button
            { Name "ObjectDescription" }
            { Label "Description..." }
            { Event
                { Type Select }
                { Callback "ps_NoteDescription_cb" }
            }
            { Resizable ResizeNone }
        }
	}
}

// This is the property sheet that allows text entry. It is
// basically only a large text entry field. Note that the
// use of this PS can be overriden by setting the toolinfo
// NoteDescEditor for using an external editor
//
PropertySheet UmlObjectDescriptionSheet
{
	{ Name "UmlObjectDescriptionSheet" }
    { Label "Object Description" }
    { OwnerActionCallback ObjectDescriptionPS_deactivate } // ECR 6880
    { Properties
    { Qrl { QrlFile "rules/qrl/objectdescription.ps.qrl" } }
            { Button 
			{Name "OK" } // Name must be "OK", otherwise the sheet
						 // will not be closed
			{ Label "OK" }
			{ Event 
				{ Type Select }
				{ Callback "ps_ObjectDescription_OK_cb" }
			}
			}


        	{ Text
            { Name "ObjectDescription" }
            { Label "Description" }
            { LabelAbove True }
            { DispRows 15 }
            { DispLength 80 }
            // 000224-02 AG:  Set MaxLength
            { MaxLength 8192 }
            { Resizable ResizeNone }
        	}

    
            
            { Group
            	{ Name ObjectDescriptionMain }
            	{ Type Horizontal }
				{ ChildSizePolicy ResizeAny }
                
                { Resizable ResizeAny }
            	{ Members ( ObjectDescription ) }
         	}
        
    }
}

// ECR 3789 end
