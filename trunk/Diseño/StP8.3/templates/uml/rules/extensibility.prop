Global 1 UmlExtensibilityProps
{
    { Properties
        { Text
            // Added ParseCondition for ECR3162
            { ParseCondition "current_editor_apptype() != \"UmlStereotypeDiagram\";" }
            { Name "Stereotype" }
            { Label "Stereotype:" }
            // Changed to 15 for ECR3162
            { DispLength 15 }
            { Resizable ResizeWidth }
            { Event
                { Type Text }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        // Begin Add ECR3162
        { Button
            { ParseCondition "current_editor_apptype() != \"UmlStereotypeDiagram\";" }
        	{ Name "ChooseStereotype" }
        	{ Label "Choose..." }
        	{ Event
        		{ Type Select }
        		{ Callback "ps_ChooseStereotype_cb" }
        	}
        }
        // End Add ECR3162
        { Text
            { Name "Constraints" }
            { Label "Constraints (one per line):" }
            { DispRows 3 }
            { DispLength 20 }
            { Resizable ResizeAny }
            { Event
                { Type Text }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        { Text
            { Name "TaggedValues" }
            { Label "Tagged Values (one per line):" }
            { DispRows 3 }
            { DispLength 20 }
            { Resizable ResizeAny }
            { Event
                { Type Text }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        // Begin Add ECR3162
        { Group
            { ParseCondition "current_editor_apptype() != \"UmlStereotypeDiagram\";" }
        	{ Name StereotypeGroup }
        	{ Type Horizontal }
          { ResizePolicy ResizeAny } //ECR 4501
            { Align Labels }        //ECR 6073
        	{ Members (Stereotype ChooseStereotype ) }
        }
        // End Add ECR3162
        { Group
            { Name Extensibility }
            { Label "Extensibility" }
            { Type Vertical }
            { VerticalSpacing 5 }
            { HorizontalAlign AlignLabels }
            //{ ChildSizePolicy ResizeAny } //ECR 4501
            { VariableRow "1 2" }
            //{ Resizable ResizeAny } //ECR 4501
            //{ LayoutPolicy ResizeAny } //ECR 4501
            { ResizablePolicy ResizeAny } //ECR 4501
            // Changed to StereotypeGroup for ECR3162
            { Members ( StereotypeGroup
                        Constraints
                        TaggedValues ) }
        }
    }
}


