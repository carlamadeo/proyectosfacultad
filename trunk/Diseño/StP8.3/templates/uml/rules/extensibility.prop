// Begin Add ECR3162
PropertySheet StereotypeChooser
{
    { Name "StereotypeChooser" }
    { Label "Choose Stereotype"} 
    { OwnerActionCallback StereotypeChooser_deactivate } // ECR 6880
    { Properties
        { Qrl { QrlFile "rules/qrl/stereotype.ps.qrl" } }
        { Button 
            {Name "OK" } // Name must be "OK", otherwise the sheet
                         // will not be closed
            { Label "OK" }
            { Event 
                { Type Select }
                { Callback "ps_StereoTypeChooser_OK_cb" }
            }
        }
        { List
            { Name StereotypeList }
            { Label "Stereotypes" }
            { Rows 17 }
            { Width 30 }
            { Exclusive True }
            { LabelAbove True }
            { Event
                { Type Select }
                { Callback "ps_StereotypeList_cb" }
            }
            // ECR 7170 begin
            { Event
                { Type DoubleClick }
                { Callback "ps_StereotypeList_DoubleClick_cb" }
            }
            // ECR 7170 end
            { Resizable ResizeNone }

        }
        { Text
            { Name "StereotypeUMLRefPage" }
            { Label "Page in UML Reference:" }
            { LabelAbove True }
            { DispRows 1 }
            { DispLength 20 }
            { Resizable ResizeAny }
        }
        { Text
            { Name "StereotypeDescription" }
            { Label "Description" }
            { LabelAbove True }
            { DispRows 15 }
            { DispLength 30 }
            { Resizable ResizeNone }
        }
        { Choice
            { Name StereoTypeGroupChoice }
            { Label "Class:" }
            { LabelAbove True }
            { Type AbbrMenu }
            // Added "    " for ECR4270
            { ChoiceItem { Label "Predefined    " } }
            { ChoiceItem { Label "User defined" } }
            { Event
                { Type Select }
                { Callback "ps_ChangeStereotypeGroup_cb" }
            }
        }
        { Group
            {Name StereoChooserVG1 }
            { Type Vertical }
            { Label "Selection" }
            { ChildSizePolicy ResizeAny }
            { Resizable ResizeAny }
            { Members ( StereotypeList StereoTypeGroupChoice) }
        }
        { Group
            { Name StereoChooserVG2 }
            { Type Vertical }
            { Label "Documentation" }
            { ChildSizePolicy ResizeAny }
            { Resizable ResizeAny }
            { Members ( StereotypeDescription StereotypeUMLRefPage ) }
        }
        { Group
            { Name StereoChooserMain }
            { Type Horizontal }
            { ChildSizePolicy ResizeAny }
            { Resizable ResizeAny }
            { Members ( StereoChooserVG1 StereoChooserVG2 ) }
        }
    }
}
// End Add ECR3162

// ECR 3789 begin
// 000224-01 AG: Setting MaxLength Value for Text entry. Up to now
//				 only 80 characters could be entered.

Global 1 UmlExtensibilityPropsXXX
{
    { Properties
        
        { Button
            { Name "ObjectDescriptionXXX" }
            { Label "Description..." }
            { Event
                { Type Select }
                { Callback "ps_NoteDescription_cb" }
            }
        }

 	    { Group
