Global 1 UmlMultiplicityProp
{
    { Properties
        // Begin Change ECR 4654
        { Choice
            { Name "MultiplicityChoice" }
            { Label "Multiplicity:" }
            { Type AbbrMenu }
            { ChoiceItem { Label "    " } }
            { ChoiceItem { Label "0..1" } }
            { ChoiceItem { Label "0..*" } }
            { ChoiceItem { Label "1..*" } }
            { ChoiceItem { Label "1   " } }
            { ChoiceItem { Label "*   " } }
            { Event
                { Type Select }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        { Text
            { Name Multiplicity }
            { Label "" }
            { MinLength 0 }
            { DispLength 25 }
            { Resizable ResizeWidth }
            { InitVal "Many" }
            { Event
                { Type Text }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        { Group
            { Name "MultiplicityGroup" }
            { Type Horizontal }
            { HorizontalAlign AlignLabels }
            { Members ( MultiplicityChoice Multiplicity ) } 
        }
        // End Change ECR 4654
    }
}

