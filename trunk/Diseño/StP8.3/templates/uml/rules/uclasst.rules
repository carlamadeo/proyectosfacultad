// JED

// The signals map to nodes of type UmlSignal and links from the class

// to the node. The UmlSignal nodes are not scoped to the class like

// operations are, because they can be shared by multiple objects and

// editors. There are 2 link types, corresponding to the two table

// sections: UmlSignalSends and UmlSignalReceives



CellType 200 SignalReceivedName

{

	{ Semantics

		{ NodeMapping

			{ MappingName SignalReceivedName }

			{ InverseMapFunc uml_signal_inverse_map_func }	

			{ Precondition { LabelSelector Label } }

			{ Name { LabelSelector Label } }

			{ Type UmlSignal }

			{ Signature 

				{ Required False }

				{ LabelFunc uclasst_canonical_args }

				{ Cell

					{ Hsect HSECT }

					{ Vsect VSECT }

					{ Row ROW }

					{ Col SignalSignatureCol }

				}

			}

		}

		{ LinkMapping 

			{ MappingName SignalReceivedLink }

			{ Type UmlSignalReceives }

			// from the class....

			{ FromNode 

				{ MappingName Class }

				{ Cell

					{ Hsect ClassHsect } 

					{ Vsect MainDefinitionVsect }

					{ Row 1 }

					{ Col 1 }

				}

			}

			// to the signal

			{ ToNode 

				{ MappingName SignalReceivedName }

				{ Cell 

					{ Hsect HSECT }

					{ Vsect VSECT }

					{ Row ROW }

					{ Col COL }

				}

			}

		}

	}

