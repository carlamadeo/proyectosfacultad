                { Name "package" }
                { Label "Packages" }
                { OnValue " || (UmlPackageIsImported & value='True') " }
                { OffValue "" }
                { Init On }
            }
            { ChoiceItem
                { Name "class" }
                { Label "Classes" }
                { OnValue " || (UmlClassIsImported & value='True') " }
                { OffValue "" }
                { Init On }
            }
        }
    }
}

// C++ property sheets

Global 1 CxxDefaultOutputDirAndFileObjectsProperty
{
    { Properties
        { Text
            { Name "output_dir" }
            { Label "Directory:" }
            { Resizable ResizeAny}
            { Type Text }
            { MinLength 0 }
            { MaxLength 300 }
            { DispLength 60 }
            { InitVal "${projdir}${system}/src_files" }
        }
        // ECR 3651
        { Button
            { Name "browse_for_output_dir" }
            { Label "Choose" }
            { Event
                { Type Select }
                { Callback "select_output_dir_callback" }
            }
        }
        { Group
            { Name "folder_group" }
            { Label "" }
            { Type Horizontal }
            //ECR 6007 begin
            //{ Resizable ResizeAny }
            //{ ChildSizePolicy ResizeWidth }
            //{ LayoutPolicy ResizeAny }
            { HorizontalAlign AlignLabels }
            //ECR 6007 end
            { Members ( output_dir browse_for_output_dir ) }
        }
        // ECR 3651
        { Choice
            { Name "file_per_class" }
            { Type CheckBox }
            { ChoiceItem
                { Name "cc" }
                { Label "One File Name Per Class" }
                { OnValue "-x file_per_class 'True'" }
                { OffValue "-x file_per_class 'False'" }
                { Init On }
                { Activation
                    { Target "use_packages_in_filenames" }
                    { On Activate }
                }
                { Activation
                    { Target "use_packages_in_filenames" }
                    { Off InActivate }
                }
                { Activation
                    { Target "output_file" }
                    { On InActivate }
                }
                { Activation
                    { Target "output_file" }
                    { Off Activate }
                }
                { Activation
                    { Target "keep_old_classes" }
                    { On InActivate }
                }
                { Activation
                    { Target "keep_old_classes" }
                    { Off Activate }
                }
            }
        }
        { Choice
            { Name "use_packages_in_filenames" }
            { Type CheckBox }
            { ChoiceItem
                { Name "use_packages_in_filenames" }
                { Label "Use Package Names in File Names" }
                { OnValue "-x use_packages_in_filenames 'True'" }
                { OffValue "-x use_packages_in_filenames 'False'" }
                { Init Off }
            }
        }
        { Text
            { Name "output_file" }
            { Label "File Name:" }
            { Resizable ResizeWidth }
            { Type Text }
            { MinLength 1 }
            { MaxLength 100 }
            { DispLength 15 }
            { InitVal "" }
        }
        { Choice
            { Name "keep_old_classes" }
            { Type CheckBox }
            { ChoiceItem
                { Name "keep_old_classes" }
                { Label "Preserve Code From Other Classes" }
                { OnValue "-x keep_old_classes 'True'" }
                { OffValue "-x keep_old_classes 'False'" }
                { Init Off }
            }
        }
        { Group
            { Name "output_file_subgroup1" }
            { Type Vertical }
            { VerticalSpacing 1 }
            { HorizontalAlign AlignLabels }
            { Resizable ResizeWidth }
            { ChildSizePolicy ResizeWidth }
            { Members ( file_per_class output_file ) }
        }
        { Group
            { Name "output_file_subgroup2" }
            { Type Vertical }
            { VerticalSpacing 1 }
            { HorizontalAlign AlignLabels }
            { Resizable ResizeWidth }
            { ChildSizePolicy ResizeWidth }
            { Members (  use_packages_in_filenames keep_old_classes ) }
        }
        { Group
            { Name "output_file_group" }
            { Type Horizontal }
            { HorizontalSpacing 20 }
            { VariableColumn "0" }
            { Resizable ResizeAny }
            { ChildSizePolicy ResizeAny }
            { HorizontalAlign AlignLabels }
            { Members ( output_file_subgroup1 output_file_subgroup2 ) }
        }
    }
}

Global 1 CXXHeaderImplementationFileExtension
{
    { Properties
        { Text
            { Name "cxxheader" }
            { Label "Interface (Header) File Extension:" }
            { Type Text }
            { MinLength 1 }
            { MaxLength 10 }
            { DispLength 10 }
            { InitVal ".h" }
            { Resizable ResizeNone }
        }
        { Text
            { ParseCondition "!is_windows_platform();" }
            { Name "cxximplementation" }
            { Label "Implementation (Body) File Extension:" }
            { Type Text }
            { MinLength 1 }
            { MaxLength 10 }
            { DispLength 10 }
            { InitVal ".C" }
            { Resizable ResizeNone }
        }
        { Text
            { ParseCondition "is_windows_platform();" }
            { Name "cxximplementation" }
            { Label "Implementation (Body) File Extension:" }
            { Type Text }
            { MinLength 1 }
            { MaxLength 10 }
            { DispLength 10 }
            { InitVal ".cpp" }
            { Resizable ResizeNone }
        }
        { Choice
            { Name "cxxinlinechoice" }
            { Type CheckBox }
            { ChoiceItem
                { Name "cxxinlineinclass" }
                { Label "Place Inline Functions Within Class Declaration" }
                { OnValue "-x inlines_in_class 'True'" }
                { OffValue "-x inlines_in_class 'False'" }
                { Init Off }
                { Activation
                    { Target "cxxinline" }
                    { On InActivate }
                }
                { Activation
                    { Target "cxxinline" }
                    { Off Activate }
                }
            }
        }
        { Text
            { Name "cxxinline" }
            { Label "Inline Functions File Extension:" }
            { Type Text }
            { MinLength 1 }
            { MaxLength 10 }
            { DispLength 10 }
            { InitVal ".h" }
            { Resizable ResizeNone }
        }
        { Group
            { Name "inline_group" }
            { Type Vertical }
            { VerticalSpacing 1 }
            { Resizable ResizeNone }
            { HorizontalAlign AlignLabels }
            { Members ( cxxinlinechoice cxxinline ) }
        }
    }
}

