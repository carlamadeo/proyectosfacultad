// common property sheets

Global 1 ImplementTypesAndInterfaces
{
    { Properties
        { Choice
            { Name "types_and_interfaces_choice" }
            { Type CheckBox }
            { ChoiceItem
                { Name "types_and_interfaces_choice_item" }
                { Label "Generate Code for Types/Interfaces" }
                { OnValue "-x generate_types_and_interfaces 'True'" }
                { OffValue "-x generate_types_and_interfaces 'False'" }
                { Init On }
                { Activation
                    { Target "type_attributes_choice" }
                    { On Activate }
                }
                { Activation
                    { Target "type_attributes_choice" }
                    { Off InActivate }
                }
            }
        }
        { Choice
            { Name "type_attributes_choice" }
            { Type CheckBox }
            { ChoiceItem
                { Name "type_attributes_choice_item" }
                { Label "Generate Type Attributes" }
                { OnValue "-x generate_type_attributes 'True'" }
                { OffValue "-x generate_type_attributes 'False'" }
                { Init On }
            }
        }
        { Group
            { Name "types_and_interfaces_group" }
            { Type Vertical }
            { VerticalSpacing 1 }
            { HorizontalAlign AlignLabels }
            { Members ( types_and_interfaces_choice type_attributes_choice) }
        }
    }
}

Global 1 Comments
{
    { Properties
        { Choice
            { Name "comments" }
            { Label "Generate Comments for:" }
            { Type CheckBox }
            { Orientation Vertical }
            // ECR 5846 { Resizable ResizeAny }
            { ChoiceItem
                { Name "class_comment" }
                { Label "Classes" }
                { OnValue " -x comment_classes 'True' " }
                { OffValue " -x comment_classes 'False' " }
                { Init Off }
            }
            { ChoiceItem
                { Name "attr_comment" }
                { Label "Attributes" }
                { OnValue " -x comment_attributes 'True' " }
                { OffValue " -x comment_attributes 'False' " }
                { Init Off }
            }
            { ChoiceItem
                { Name "op_comment" }
                { Label "Operations" }
                { OnValue " -x comment_operations 'True' " }
                { OffValue " -x comment_operations 'False' " }
                { Init Off }
            }
        }
    }
}

Global 1 FormatComments
{
    { Properties
        { Choice
            { Name "format_comments" }
            { Type CheckBox }
            { ChoiceItem
                { Name "format_comments" }
                { Label "Format to:" }
                { OnValue "-x format_comments 'True'" }
                { OffValue "-x format_comments 'False'" }
                { Init Off }
                { Activation
                    { Target "format_comments_line_length" }
                    { On Activate }
                }
                { Activation
                    { Target "format_comments_line_length" }
                    { Off InActivate }
                }
            }
        }
        { Text
            { Name "format_comments_line_length" }
            { Type Text }
            { MinLength 1 }
            { MaxLength 4 }
            { DispLength 4 }
            { InitVal "80" }
            { Resizable ResizeNone }
        }
        { Message
            { Name "format_comments_characters" }
            { Label " characters per line" }
        }
        { Group
            { Name "format_comments_group" }
            { VerticalAlign AlignCenter }
            { Type Horizontal }
            { HorizontalSpacing 0 }
            { Members ( format_comments format_comments_line_length format_comments_characters ) }
        }
    }
}

Global 1 ExcludeExternal
{
    { Properties
        { Choice
            { Name "exclusions" }
            { Type CheckBox }
            { Orientation Vertical }
            { Resizable ResizeAny }
            { ChoiceItem
                { Name "external" }
                { Label "Ignore Classes Annotated As External" }
                { OnValue " || (UmlClassIsImported & value='True') " }
                { OffValue "" }
                { Init On }
            }
        }
    }
}

Global 1 ExcludeExternalPackageAndClass
{
    { Properties
        { Choice
            { Name "exclusions" }
            { Label "Ignore External:" }
            { Type CheckBox }
            { Orientation Vertical }
            { Resizable ResizeAny }
