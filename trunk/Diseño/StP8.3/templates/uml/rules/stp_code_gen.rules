                { ChildSizePolicy ResizeWidth }
                { LayoutPolicy ResizeAny }
                { HorizontalAlign AlignLabels }
                { Members ( ada_root_class_type_group ada_subclass_type_privacy ada_implement_associations GenAccessOperationExternal ) }
            }
            { Group
                { Name "Options4" }
                { Label "Comments:" }
                { Type Horizontal }
                { VariableColumn "0" }
