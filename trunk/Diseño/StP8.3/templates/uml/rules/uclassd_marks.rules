LinkSymbol Dependency
{
    { DisplayMark
		{ Name DependencyStereotype }
		{ ItemCond (UmlStereotype) }
		{ StringMark "${annot.UmlExtensibilityDefinition.UmlStereotype}" }
		{ Anchor LabelCenterRight }
		{ HAdjust Center }
		// JED - HAdjust Right is too far. use pixels
		{ XPixelOffset 45 }
		{ VAdjust Center }
		{ Help "Dependency Stereotype" }
    }
}


LinkSymbol AssociationLinks
{
	{ DisplayMark
		{ Name AssocToMultiplicity }
		{ Anchor ToEnd }
		{ ItemCond (UmlMultiplicity) }
		{ MappingName ToMapping }
		{ StringMark "${annot.UmlRoleDefinition.UmlMultiplicity}" }
		{ Help "Show multiplicity indicators on associations" }
		{ HAdjust Center }
		{ VAdjust Center }
		{ XPixelOffset 20 }
		{ YPixelOffset 20 }
	}
    // ECR 75
	{ DisplayMark
		{ Name AssocToIsOrdered }
		{ Anchor ToEnd }
		{ CondFunc "role_is_ordered(\"${annot.UmlRoleDefinition.UmlRoleIsOrdered}\");" }
		{ MappingName ToMapping }
		{ StringMark "{ordered}" }
		{ Help "Show {ordered} indicators on associations" }
		{ HAdjust Center }
		{ VAdjust Center }
		{ XPixelOffset 35 }
		{ YPixelOffset -35 }
	}
	// End ECR 75
// ECR 5861 begin
//
//	{ DisplayMark
//		{ Name AssocToConstraint }
//		{ Anchor ToEnd }
//		{ ItemCond (UmlConstraintItem) }
//		{ MappingName ToMapping }
//		{ StringMarkFunc "uml_dpymark_list_formatter(gde_dmark_matched_items,\"{\",\"}\",\",\");" }
//		{ Help "Show constraints on associations" }
//		// { HAdjust Left }
//		// { VAdjust Top }
//		{ XPixelOffset -2 }
//		// { YPixelOffset -2 }
//	}
//
// ECR 5861 end

    // ECR 4396 moved Displaymark AssocToHasQualifier and AssocToQualifierValues to LinkSymbol Association
	{ DisplayMark
		{ Name AssocToNavigability }
		// { ItemCond (UmlRoleNavigability) }
		{ MappingName ToMapping }
		{ Anchor ToEnd }
		// ECR 157
		{ EndMarkFunc "uml_navigability_mark(\"${annot.UmlRoleDefinition.UmlRoleNavigability}\",${link.id});" }
	}
}

LinkSymbol Association
{
	{ DisplayMark
		{ Name AssocFromMultiplicity }
		{ Anchor FromEnd }
		{ ItemCond (UmlMultiplicity) }
		{ MappingName FromMapping }
		{ StringMark "${annot.UmlRoleDefinition.UmlMultiplicity}" }
		{ Help "Show multiplicity indicators on associations" }
		{ HAdjust Center }
		{ VAdjust Center }
		{ XPixelOffset 20 }
		{ YPixelOffset 20 }
	}
    // ECR 75
	{ DisplayMark
		{ Name AssocFromIsOrdered }
		{ Anchor FromEnd }
		{ CondFunc "role_is_ordered(\"${annot.UmlRoleDefinition.UmlRoleIsOrdered}\");" }
		{ MappingName FromMapping }
		{ StringMark "{ordered}" }
		{ Help "Show {ordered} indicators on associations" }
		{ HAdjust Center }
		{ VAdjust Center }
		{ XPixelOffset 35 }
		{ YPixelOffset -35 }
	}
	// End ECR 75
// ECR 5861 begin
//
//	{ DisplayMark
//		{ Name AssocFromConstraint }
//		{ Anchor FromEnd }
//		{ ItemCond (UmlConstraintItem) }
//		{ MappingName FromMapping }
//		{ StringMarkFunc "uml_dpymark_list_formatter(gde_dmark_matched_items,\"{\",\"}\",\",\");" }
//		{ Help "Show constraints on associations" }
//		{ XPixelOffset -2 }
//	}
//
// ECR 5861 end
	{ DisplayMark
		{ Name AssocFromHasQualifier }
		{ ItemCond (UmlRoleQualifier) }
		{ MappingName FromMapping }
		{ EndMarkFunc "uml_has_qualifier(\"${annot.UmlRoleDefinition.UmlRoleQualifier}\");" }
		{ Anchor FromEnd }
		{ Help "Show qualifiers on association" }
    }
    { DisplayMark
		{ Name AssocFromQualifierValues }
		{ ItemCond (UmlRoleQualifier) }
		{ MappingName FromMapping }
		{ StringMarkFunc "uml_dpymark_list_formatter(gde_dmark_matched_items,\"\",\"\",\",\n\");" }
		{ Anchor FromEnd }
		{ HAdjust AtEnd } 
		{ VAdjust AtEnd }
		{ XPixelOffset 0 }
		{ YPixelOffset 0 }
		{ Help "Show qualifiers on association" }
    }
    // ECR 4396 moved Displaymark from LinkSymbol AssociationLinks
    { DisplayMark
		{ Name AssocToHasQualifier }
		{ ItemCond (UmlRoleQualifier) }
		{ MappingName ToMapping }
		{ EndMarkFunc "uml_has_qualifier(\"${annot.UmlRoleDefinition.UmlRoleQualifier}\");" }
		{ Anchor ToEnd }
		{ Help "Show qualifiers on association" }
    }
    { DisplayMark
		{ Name AssocToQualifierValues }
		{ ItemCond (UmlRoleQualifier) }
		{ MappingName ToMapping }
		{ StringMarkFunc "uml_dpymark_list_formatter(gde_dmark_matched_items,\"\",\"\",\",\n\");" }
		{ Anchor ToEnd }
		{ HAdjust AtEnd } 
		{ VAdjust AtEnd }
		{ XPixelOffset 0 }
		{ YPixelOffset 0 }
		{ Help "Show qualifiers on association" }
    }
    // ECR 4396 end
	{ DisplayMark
		{ Name AssocFromNavigability }
		// { ItemCond (UmlRoleNavigability) }
		{ MappingName FromMapping }
		{ Anchor FromEnd }
		// ECR 157
		{ EndMarkFunc "uml_navigability_mark(\"${annot.UmlRoleDefinition.UmlRoleNavigability}\",${link.id});" }
	}
	// from agg indicator
	{ DisplayMark
		{ Name FromAggregationIndicator }
		{ Anchor FromEnd }
		{ ItemCond (UmlAggregationType) }
		{ MappingName FromMapping }
		// There can be 2 items of this type: one for the FromEnd and
		// one for the ToEnd. So we must pass the right one.
		{ EndMarkFunc "uml_agg_endmark(\"${annot.UmlRoleDefinition.UmlAggregationType}\");" }
	}
	// to agg indicator
	{ DisplayMark
		{ Name ToAggregationIndicator }
		{ Anchor ToEnd }
		{ ItemCond (UmlAggregationType) }
		{ MappingName ToMapping }
		// There can be 2 items of this type: one for the FromEnd and
		// one for the ToEnd. So we must pass the right one
		{ EndMarkFunc "uml_agg_endmark(\"${annot.UmlRoleDefinition.UmlAggregationType}\");" }
	}

	// ECR 7178 - begin
    { DisplayMark
		{ Name AssocStereotype }
		{ MappingName AssociationNodeMapping }
		{ ItemCond (UmlStereotype) }
		{ StringMark "${annot.UmlExtensibilityDefinition.UmlStereotype}" }
		{ Anchor LabelCenterRight }
		{ HAdjust Center }
		// JED - HAdjust Right is too far. use pixels
		{ XPixelOffset 0 }
		{ YPixelOffset -15 }
		{ VAdjust Center }
		{ Help "Association Stereotype" }
    }
	// ECR 7178 - end
}

