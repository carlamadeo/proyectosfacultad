NodeSymbol Operation
{
    { DisplayMark
		{ Name OperationVisibility }
		{ ItemCond (UmlMemberVisibility) }
		{ StringMarkFunc "uml_member_display_mark_str(gde_dmark_matched_items);" }
		{ Anchor LabelCenterLeft }
		{ HAdjust Right }
		{ VAdjust Center }
		{ Help "Operation Visibility" }
    }
    { DisplayMark
		{ Name OpIsClass }
		{ ItemCond (UmlMemberIsClass) }
		{ LabelMarkFunc "uml_member_is_class(gde_dmark_matched_items);" }
		{ Help "Operation Is Class-scoped" }
    }
    { DisplayMark
		{ Name OperationReturnType }
		{ ItemCond (UmlOperationReturnType) }
		{ StringMarkFunc "uml_operation_return_type(gde_dmark_matched_items);" }
		// JED - is not vertically aligned with label
		// I could use LabelLowerRight, but that screws up printing.
		// decision: live with it being a little off
		{ Anchor LabelCenterRight }
		{ HAdjust Left }
		{ VAdjust Center }
		{ Help "Operation Return Type" }
    }
    // --- START PATCH ECR 6726 ---
    { DisplayMark
   		{ Name OperationStereotype }
    	{ ItemCond (UmlStereotype) }
    	{ Anchor CenterLeft } 
		{ HAdjust Right }
		{ XPixelOffset -15 }
		{ YPixelOffset 13 }
    	{ StringMark "${annot.UmlExtensibilityDefinition.UmlStereotype}" }
    	{ Help "Stereotype for Operation" }
    }
    // --- END PATCH ECR 6726 ---
}

