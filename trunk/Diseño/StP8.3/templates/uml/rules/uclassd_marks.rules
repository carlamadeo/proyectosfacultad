NodeSymbol AllClassSymbolsThatCanBeInAPackage
{
    { DisplayMark
		{ Name ClassQualifiedName }
		{ MappingName ClassMapping }
		{ Condition "node[id=${node.id} && in_links[UmlContains && link_refs]];" }
		{ StringMarkFunc "\"( from \" + uml_class_id_scope_name(${node.id}) + \")\";" }
		{ Anchor LabelLowerCenter }
		{ HAdjust Center }
		{ VAdjust Top }
		{ YPixelOffset 10 }   // ECR 4224
		{ Help "Package scope qualified name" }
    }
}

