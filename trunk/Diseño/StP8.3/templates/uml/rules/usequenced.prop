Include "rules/extensibility.prop"
Include "rules/seq_collab_messages.prop"

PropertySheet UmlSequence_ps
{
    { Name "UmlSequence_ps" }
    {Label "Properties"} {UseProperty UmlObjectDescriptionButton}
    { UseProperty UmlNameProp }
    { UseProperty UmlExtensibilityProps }
    { UseProperty UmlBottomButtons }
    { OwnerActionCallback UmlSequence_ps_ActionDone } // ECR 6880
    { Properties
        { Qrl { QrlFile "rules/qrl/usequenced.ps.qrl" } }
// ECR 6926 begin
//        { Space
//            { Name Sp1 }
//        }
//        { Space
//            { Name Sp2 }
//        }
//        { Group
//            { Name RightGroup }
//            { Type Vertical }
//            { ResizePolicy ResizeNone }
//            { HorizontalAlign AlignLabels }
//            { Members ( Sp1
//                        ObjectName ObjectDescription
//                        Sp2
//                        Extensibility ) }
//        }
        { Group
            { Name NameGroup }
            { Label "Object" }
            { Type Vertical }
            { VerticalSpacing 5 }
            { Resizable ResizeWidth }
            { HorizontalAlign AlignLabels }
            { Members ( ObjectName 
                        ObjectDescription ) }
        }
        { TopGroup
            { Align Left }
            { Members ( NameGroup
                        Extensibility ) }
        }
// ECR 6926 end
    }
}

PropertySheet UmlSequenceLink_ps
{
    { Name "UmlSequenceLink_ps" }
    {Label "Properties"} {UseProperty UmlObjectDescriptionButton}
    { UseProperty UmlMessageNameProp }
    { UseProperty UmlSeqCollabProp }
    { UseProperty UmlExtensibilityProps }
    { UseProperty UmlBottomButtons }
    { OwnerActionCallback UmlSequenceLink_ps_ActionDone } // ECR 6880
    { Properties
        { Qrl { QrlFile "rules/qrl/usequenced.ps.qrl" } }
// ECR 6926 begin
//        { Space
//            { Name Sp1 }
//        }
//        { Space
//            { Name Sp2 }
//        }
//        { Group
//            { Name NameFromToGroup }
//            { Type Vertical }
//            { HorizontalAlign AlignLabels }
//            { ResizePolicy ResizeNone }
//            { Members ( Sp1 MessageObjectName Sp2 SeqCollabStuff ) }
//        }
//        { TopGroup
//            { Align Left }
//            { Members ( NameFromToGroup Extensibility ) }
//        }
        { Group
            { Name NameGroup }
            { Label "Message" } // ECR 7425 - changed from 'Link' to 'Message'
            { Type Vertical }
            { VerticalSpacing 5 }
            { Resizable ResizeWidth }
            { HorizontalAlign AlignLabels }
            { Members ( MessageObjectName 
                        ObjectDescription ) }
        }
        { TopGroup
            { Align Left }
            { Members ( NameGroup 
                        SeqCollabStuff 
                        Extensibility ) }
        }
// ECR 6926 end
    }
}

