PropertySheet ClassRename
{	
  { Name ClassRename }
  { Label "Rename Class"}
  { OwnerActionCallback uml_class_rename_action_done }
  { Properties
      { Button
	  { Name "OK" }
	  { Label "Rename" }
	  { Event
	      { Type Select }
	      { Callback "gte_global_object_rename_doit_callback" }
	    }
	}
      { Button
	  { Name "Apply" }
	  { Label "Check Impact" }
	  { Event
	      { Type Select }
	      { Callback "gte_global_object_rename_check_impact_callback" }
	    }
	}
      { Button
	  { Name "Cancel" }
	  { Label "Cancel" }
	  { Event
	      { Type Select }
	      { Callback "gte_global_object_rename_cancel_callback" }
	    }
	}
      { Choice
	  { Name "dep_UmlClass_UmlOperation_OpConstructor" }
	  { Type PropCheckBox }
	  { Label "" }
	  { ChoiceItem
	      { Label "Rename Constructors For Class" }
	      { InitVal 0 }
	    }
	  { Event 
	      { Type Select}
	      { Callback uml_rename_opt_dep_cb }
	    }
	}
      { Message
	  { Name "NewLabelLabel" }
	  { Label "New Label:"}
      	}
      { Text
	  { Name "NewLabel"}
	  { MinLength	1}
	  { DispLength	80}
	  { MaxLength	5000}
	  { DispRows      5}
	  { MaxRows       5000}
	  { VertScrollbar 1}
	}
      { Footer
	  { Name "Status" }
	  { LeftFooter "" }
	  { Active True }
	}
      { Group
	  { Name "Top" }
	  { Type Vertical }
	  { Members (NewLabelLabel NewLabel dep_UmlClass_UmlOperation_OpConstructor)}
	  { VerticalAlign Left}
	}
      { Group
	  { Name "All" }
	  { Type Vertical }
	  { Members (Top Buttons)}
	  { VerticalAlign Center}
	}
    }
}

PropertySheet OperationRename
{	
    { Name OperationRename }
    { Label "Rename Operation or Signal"}
    { OwnerActionCallback uml_operation_rename_action_done }
    { Properties
	{ Button
	    { Name "OK" }
	    { Label "Rename" }
	    { Event
		{ Type Select }
		{ Callback  "gte_global_object_rename_doit_callback" }
	    }
	}
	{ Button
	    { Name "Apply" }
	    { Label "Check Impact" }
	    { Event
		{ Type Select }
		{ Callback "gte_global_object_rename_check_impact_callback" }
	    }
	}
	{ Button
	    { Name "Cancel" }
	    { Label "Cancel" }
	    { Event
		{ Type Select }
		{ Callback "gte_global_object_rename_cancel_callback" }
	    }
	}
	{ Choice
	    { Name "dep_UmlOperation_UmlOperation_OpOverloaded" }
	    { Type PropCheckBox }
	    { Label "" }
	    { ChoiceItem
		{ Label "Rename Overloaded Operations or Signals in Class" }
		{ InitVal 0 }
	    }
	    { Event 
		{ Type Select}
		{ Callback uml_rename_opt_dep_cb }
	    }
	}
	{ Message
	    { Name "NewLabelLabel" }
	    { Label "New Label:"}
      	}
	{ Text
	    { Name "NewLabel"}
	    { MinLength	1}
	    { DispLength	80}
	    { MaxLength  5000}
	    { DispRows      5}
	    { MaxRows       5000}
	    { VertScrollbar 1}
	}
	{ Footer
	    { Name "Status" }
	    { LeftFooter "" }
	    { Active True }
	}
	{ Group
	    { Name "Top" }
	    { Type Vertical }
	    { Members
		(
		    NewLabelLabel
		    NewLabel 
		    dep_UmlOperation_UmlOperation_OpOverloaded
		)
	    }
	    { VerticalAlign Left}
	}
	{ Group
	    { Name "All" }
	    { Type Vertical }
	    { Members (Top Buttons)}
	    { VerticalAlign Center}
	}
    }
}
