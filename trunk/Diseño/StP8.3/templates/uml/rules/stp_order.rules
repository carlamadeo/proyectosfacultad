// Changed for ECR 4731

Category CatModelDiagrams
{
    { Label "Diagrams" }
    { Image "folder" }
    { ShowOnDesktop True }
    { Group
         (
	     UmlUseCaseDiagramFiles
	     UmlClassDiagramFiles	
	     UmlSequenceDiagramFiles
	     UmlCollaborationDiagramFiles
	     UmlStateDiagramFiles	
	     UmlActivityDiagramFiles
	     UmlComponentDiagramFiles
	     UmlDeploymentDiagramFiles
	     UmlStereotypeDiagramFiles
          )
    }
}

Category CatModelTables
{
    { ShowOnDesktop True }
    { Label "Tables" }
    { Image "folder" }
    { Expand False }
    { Group
         (
	     UmlClassTableFiles	
	     UmlStateTableFiles	
	     RequirementTableFiles
          )
    }
}
 
Category CatModelElements
{
    { ShowOnDesktop True }
    { Label "Repository View" }
    { Image "folder" }
    { Expand False }
    { Group
         (
	     UmlUseCaseObjects
	     UmlClassObjects	
	     UmlPackageObjects
	     UmlStereotypeObjects
         )
    }
}

Category CatReports
{
    { ShowOnDesktop True }
    { Label "Document View" }
    { Image "folder" }
    { Expand False }
    { Group
         (
         MicrosoftWord  // SPR 2467 - Microsoft Word CatModelElements
         Rme
         )
    }
}