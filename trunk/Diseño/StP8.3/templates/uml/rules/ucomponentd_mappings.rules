NodeSymbol sourceComponent
{
    { NodeMapping
        { MappingName SourceComponent }
        { AppType UmlComponentSource }
        { NameIsRequired True }
    }
}

Include "rules/componentDeployment_mappings.rules.common"

// ECR 5184 start
LinkSymbol  ClassResidence
{
	{ LinkMapping
		{ MappingName ClassResidenceSC }
		{ AppType UmlContains }
		{ NameIsRequired False }
		{ FromNode { MappingName SourceComponent } }
		{ ToNode { MappingName ClassMapping } }
	}
}  
// ECR 5184 end


LinkSymbol Dependency
{
    { LinkMapping
	{ MappingName DependencyMappingSI }
	{ AppType UmlComponentDependency }
	{ NameIsRequired False }
	{ FromNode { MappingName SourceComponent } }
	{ ToNode { MappingName InterfaceComponent } }
    }
    { LinkMapping
	{ MappingName DependencyMappingBS  }
	{ AppType UmlComponentDependency }
	{ NameIsRequired False }
	{ FromNode { MappingName BinaryComponent } }
	{ ToNode { MappingName SourceComponent } }
    }
    { LinkMapping
	{ MappingName DependencyMappingES }
	{ AppType UmlComponentDependency }
	{ NameIsRequired False }
	{ FromNode { MappingName ExecutableComponent } }
	{ ToNode { MappingName SourceComponent } }
    }
    { LinkMapping
	{ MappingName DependencyMappingSE }
	{ AppType UmlComponentDependency }
	{ NameIsRequired False }
	{ FromNode { MappingName SourceComponent } }
	{ ToNode { MappingName ExecutableComponent } }
    }
    { LinkMapping
	{ MappingName DependencyMappingSB }
	{ AppType UmlComponentDependency }
	{ NameIsRequired False }
	{ FromNode { MappingName SourceComponent } }
	{ ToNode { MappingName BinaryComponent } }
    }
    { LinkMapping
	{ MappingName DependencyMappingSS }
	{ AppType UmlComponentDependency }
	{ NameIsRequired False }
	{ FromNode { MappingName SourceComponent } }
	{ ToNode { MappingName SourceComponent } }
    }
}

LinkSymbol Interface
{
    { LinkMapping
	{ MappingName InterfaceMappingIS }
	{ AppType UmlComponentInterface }
	{ NameIsRequired False }
	{ FromNode { MappingName InterfaceComponent } }
	{ ToNode { MappingName SourceComponent } }
    }
}

LinkSymbol isComponentOf
{
    { LinkMapping
	{ MappingName IsComponentOfSO }
	{ AppType UmlComponentComposit }
	{ NameIsRequired False }
	{ FromNode { MappingName SourceComponent } }
	{ ToNode { MappingName ObjectComponent } }
    }
}

