Global 1 UmlTransitionObjectName
{
    { Properties
        { Text
            { Name "ObjectName" }
            { Label "Label:" }
            { MinLength 1 }
            { DispLength 61 }
            { Resizable ResizeNone }
        }
    }
}

Global 1 UmlActivityTransitionObjectName
{
    { Properties
        { Text
            { Name "TransitionGuard" }
            { Label "Guard:" }
            { MinLength 0 }
            { DispLength 30 }
            { Resizable ResizeNone }
            { Event
                { Type Text }
                { Callback "ps_TransitionValueChanged_cb" }
            }
        }
        { Button
        	{ Name "ChooseGuard" }
        	{ Label "Choose..." }
        	{ Event
        		{ Type Select }
        		{ Callback "ps_ChooseGuard_cb" }
        	}
        }
        { Text
            { Name "TransitionAction" }
            { Label "Action List (one per line):" }
            { MinLength 0 }
            { DispRows 3 }
            { DispLength 21 }
            { Resizable ResizeNone }
            { Event
                { Type Text }
                { Callback "ps_TransitionValueChanged_cb" }
            }
        }
        { Button
        	{ Name "ChooseAction" }
        	{ Label "Choose..." }
        	{ Event
        		{ Type Select }
        		{ Callback "ps_ChooseAction_cb" }
        	}
        }
        { Group
            { Name TransitionGuardGroup }
            { HorizontalSpacing 5 }
            { Type Horizontal }
            { Members ( TransitionGuard ChooseGuard ) }
        }
        { Group
            { Name TransitionActionGroup }
            { Type Horizontal }
            { HorizontalSpacing 5 }
            { VerticalAlign Center }
            { Members ( TransitionAction ChooseAction ) }
        }
        { Group
            { Name ActivityTransitionAttributes }
            { Label "Transition Attributes" }
            { Type Vertical }
            { VerticalSpacing 5 }
            { HorizontalAlign AlignLabels }
            { Members ( TransitionGuardGroup 
                        TransitionActionGroup ) }
        }
    }
}

