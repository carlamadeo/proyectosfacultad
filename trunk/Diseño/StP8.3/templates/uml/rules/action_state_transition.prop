PropertySheet GuardChooser
{
	{ Name "GuardChooser" }
    { Label "Choose Guard" }
    { OwnerActionCallback GuardChooser_deactivate } // ECR 6880
    { Properties
        { Qrl { QrlFile "rules/qrl/uml_choosers.ps.qrl" } }
        { Button 
    		{Name "Reset" }
    		{ Label "Refresh" }
    		{ Event 
    			{ Type Select }
    			{ Callback "ps_ChangeGuardGroup_cb" }
    		}
    	}
        { Button 
    		{Name "OK" } // Name must be "OK", otherwise the sheet
    					 // will not be closed
    		{ Label "OK" }
    		{ Event 
    			{ Type Select }
    			{ Callback "ps_TransitionGuardChooser_OK_cb" }
    		}
    	}
    	{ Text
            { Name "ParentClass" }
            { Label "Parent-Class:" }
            { LabelAbove True }
            { DispRows 1 }
            { DispLength 30 }
            { Resizable ResizeNone }
    	}
        { Button
        	{ Name "OpenClass" }
        	{ Label "GoTo Class" }
        	{ Event
        		{ Type Select }
        		{ Callback "ps_OpenClass_cb" }
        	}
        }
        { Group
            { Name ClassGroup }
            { HorizontalSpacing 5 }
            { Type Horizontal }
            { VerticalAlign Bottom }
            { Members ( ParentClass OpenClass ) }
        }
        { Choice
            { Name GuardTypeGroupChoice }
            { Label "Type:" }
            { LabelAbove True }
            { Type AbbrMenu }
            { ChoiceItem { Label "Attributes    " } }
            { ChoiceItem { Label "Operations    " } }
            { ChoiceItem { Label "States        " } }
            { Event
                { Type Select }
                { Callback "ps_ChangeGuardGroup_cb" }
            }
        }
    	{ List
            { Name GuardList }
            { Label "Available Guards:" }
            { Rows 10 }
            { Width 60 }
            { Exclusive True }
            { LabelAbove True }
        	{ Resizable ResizeNone }
     	}
        { Group
        	{ Name GuardChooserMain }
        	{ Type Vertical }
        	{ Label "Selection" }
    		{ ChildSizePolicy ResizeAny }
            { Resizable ResizeAny }
            { VerticalSpacing 5 }
           	{ Members ( ClassGroup GuardTypeGroupChoice GuardList ) }
        }
        
    }
}

