Include "rules/componentDeployment_links.rules.common"

LinkSymbol deploys
{
    { PrintName "Deploys" }
    { ToArrow False}
    { Uncentered True }
    { CanBeInclude True }
    { Connects 
 	{ From deploymentComponent }
	{ To binaryComponent }
    }
    { Connects 
	{ From deploymentComponent }
	{ To executableComponent }
    }
    { Connects 
	{ From deploymentComponent }
	{ To objectComponent }
    }
    { Connects 
	{ From deploymentComponent }
	{ To interfaceComponent }
    }
    { ReplaceSet ( none ) }
	// ECR 7287 - begin
    { DisplayMark
		{ Name DeploysStereotype }
		{ ItemCond (UmlStereotype) }
		{ StringMark "«${annot.UmlExtensibilityDefinition.UmlStereotype}»" }
    	{ Anchor LabelCenterRight }
    	{ HAdjust Center }
    	{ XPixelOffset 45 }
    	{ YPixelOffset -10 }
    	{ VAdjust Center }
		{ Help "Deployment Stereotype" }
    }
	// ECR 7287 - end
}

// Begin Add ECR 4515
LinkSymbol Communicates
{
    { PrintName "Communicates" }
    { Connects 
    	{ From deploymentComponent }
    	{ To deploymentComponent }
    	{ IsDefault True }
    }
    { IsDirectional False }
    { CanHaveLabel True }
    { Uncentered True }
    { ReplaceSet ( none ) }
	// ECR 7287 - begin
    { DisplayMark
		{ Name CommunicatesStereotype }
		{ ItemCond (UmlStereotype) }
		{ StringMark "«${annot.UmlExtensibilityDefinition.UmlStereotype}»" }
    	{ Anchor LabelCenterRight }
    	{ HAdjust Center }
    	{ XPixelOffset 45 }
    	{ YPixelOffset -10 }
    	{ VAdjust Center }
		{ Help "Communicates Stereotype" }
    }
	// ECR 7287 - end
}
// End Add ECR 4515
