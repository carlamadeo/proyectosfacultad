Note 800014 	// OracleColumn
{
    { Item
	{ AppType DefaultValue }
	{ HelpText "The Default Value item specifies the value to be used for a column when none is provided." }
	{ PrintName "Default Value" }
	{ Bounds { Min 0 } { Max 1 } }
    }
    { Item
	{ AppType IsClusterColumn }
	{ HelpText "The Cluster Column? item indicates that a column is a cluster column." }
	{ PrintName "Cluster Column?" }
	{ Bounds { Min 0 } { Max 1 } }
	{ InitValue "True" }
    }
    { Item
	{ AppType ConstraintName }
	{ HelpText "The Check Name item names a column constraint based on the valid values of a column." }
	{ PrintName "Check Name" }
	{ Bounds { Min 0 } { Max 1 } }
	{ InitValue "${sym_label} check" }
    }
    { Item
	{ AppType ObjectComment }
	{ HelpText "The Table Column Comment item captures a short description of the table column for the Oracle Data Dictionary." }
	{ PrintName "Table Column Comment" }
	{ Bounds { Min 0 } { Max 1} }
    }
}

Note  800007	// SybaseColumn
{
    { Item
	{ AppType DefaultValue }
	{ HelpText "The Default Value item specifies the value to be used for a column when none is provided.  Valid values are:  a constant expression, with quotation marks around it if it is a string; user, for the username of the person entering the row; or null." }
	{ PrintName "Default Value" }
	{ Bounds { Min 0 } { Max 1 } }
    }
    { Item
	{ AppType DefaultName }
	{ HelpText "The Default Name item provides a name to be used for a default value associated with a column." }
	{ PrintName "Default Name" }
	{ Bounds { Min 0 } { Max 1 } }
    }
}

Note 800008	// InformixColumn
{
    { Item
	{ AppType BlobSpace }
	{ HelpText "The Blob (Binary Large Object) Space item is used to store large text and binary objects on a file system or device separate from the object's table. Blob Space specifies the location at which you want to store the object." }
	{ PrintName "Blob Space" }
	{ Bounds { Min 0 } { Max 1 } }
    }
    { Item
	{ AppType DefaultValue }
	{ HelpText "The Default Value item specifies the value to be used for a column when none is provided." }
	{ PrintName "Default Value" }
	{ Bounds { Min 0 } { Max 1 } }
    }
    { Item
	{ AppType ConstraintName }
	{ HelpText "The Check Name item names a column constraint based on the valid values of a column." }
	{ PrintName "Check Name" }
	{ Bounds { Min 0 } { Max 1 } }
	{ InitValue "${sym_label} check" }
    }
}

Note 800100 // DB2Column
{
    { Item
	{ AppType WithDefault }
	{ HelpText "The With Default? item indicates the column should be initialized with a system default value if no value is provided when the table row is inserted or updated." }
	{ PrintName "With Default?" }
	{ Bounds { Min 0 } { Max 1} }
	{ InitValue "True" }
    }
    { Item
	{ AppType ObjectComment }
	{ HelpText "The Table Column Comment item captures a short description of the table column for the DB2 Catalog." }
	{ PrintName "Table Column Comment" }
	{ Bounds { Min 0 } { Max 1} }
    }
}

Note 800104 // IngresColumn
{
    { Item
	{ AppType WithDefault }
	{ HelpText "The With Default? item indicates the column should be initialized with a system default value if no value is provided when the table row is inserted or updated." }
	{ PrintName "With Default?" }
	{ Bounds { Min 0 } { Max 1} }
	{ InitValue "True" }
    }
    { Item
	{ AppType SystemMaintained }
	{ HelpText "The System Maintained? item specifies that the server sets and prevents modification of the column value." }
