Include "annotation/generic_object"
Note 899009
{
    { AppType ReportFormatFile }
    { PrintName "Report Format File" }
    { HelpText "The Report Format File annotation allows you to specify a format file that will override the 'rme_report' format file that is used as default." }
    { Bounds { Min 0 } { Max 1 } }
    { Item
        { AppType FormatFileName }
        { PrintName "Format File Name" }
	{ InitValue "rme_report" }
        { Bounds { Min 1 } { Max 1 } }
        { HelpText "This item is required and contains the name of the format file to be used." }
    }
}
