}

Note 802005
{
    { AppType ModuleDefinition }
    { PrintName "Module Definition" }
    { HelpText "The Module Definition note lets you add items which are used for C code generation." }
    { Bounds { Min 1 } { Max 1 } }
    { Item
        { AppType ModuleReturnType }
        { PrintName "Module Return Type" }
        { HelpText "The Module Return Type item lets you specify the return type for a module." }
        { Bounds { Min 0 } { Max 1 } }
        { InitValue "int" }
    }
    { Item
        { AppType StorageClass }  
        { PrintName "Storage Class" } 
        { HelpText "The Storage Class item lets you specify the storage class for Modules.  Possible values are | static | const | static const. " } 
        { Bounds { Min 0 } { Max 1 } } 
    } 
    { Item
        { AppType VariableArguments }
        { PrintName "Variable Arguments" }
        { HelpText "The Variable Arguments item lets you specify whether or not a module accepts a variable arugment list." }
        { Bounds { Min 0 } { Max 1 } }
        { InitValue "False" }
    }
    { Item
        { AppType CodeGenId }
        { PrintName "Code Generation ID" }
        { HelpText "Contains the unique ID for code generated object."}
        { Bounds { Min 0 } { Max 1 } }
        { IsVisible False }
    }
}

 
Note 802036
{
    { AppType LexicalModule }
    { PrintName "Lexical Module" }
    { HelpText "The Lexical Module note is used to specify if a module is a lexical module or not." }
    { Bounds { Min 0 } { Max 1 } }
    { Item
        { AppType IsLexicalModule }
        { PrintName "Is Lexical Module" }
        { HelpText "The Is Lexical Module is True if the module is a lexical inclusion module, and False otherwise." }
        { Bounds { Min 1 } { Max 1 } }
        { InitValue "True" }
    }
}


