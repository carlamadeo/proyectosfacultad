// SCE ModulePDL note
//

Note 802004
{
    { AppType ModulePDL } 
    { HelpText "Use the description of this note to enter the PDL information." }
    { Bounds { Min 0 } { Max 1 } } 
    { PrintName "Module PDL" }
    { Item
	{ AppType PDL }
	{ Bounds { Min 1 } { Max 1 } }
	{ PrintName "Has PDL?" }
	{ IsReadOnly True }
	{ InitValue "True" }
    }
}



