// DSE sequence, selection and iteration. 
// 12/14/95  These three notes are all put together so annotation cloning/copying works amongst them,
// which removes several migration problems.

Include "annotation/data_definition.note"
Include "annotation/structure_component.note"
Include "annotation/se_generic_object"

// Add these selection items
 
Note 802006
{
    // the first two are really only intended for selections
    { Item
        { AppType Selector }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Selector" }
        { HelpText "Use the Selector item to enter the tag value for this selection.  The tag value determines which data substructure is used in a particular case." }
    }
    { Item
        { AppType SelectorType }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Selector Type" }
        { HelpText "Use the Selector Type item to enter the data type of the tag value." }
    }

    // the next two are for migration purposes.
    { Item
        { AppType LowerIteration }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Lower Bound" }
        { IsReadOnly True }
        { HelpText "Lower Bound is for migration purposes only." }
    }
    { Item
        { AppType UpperIteration }
        { Bounds { Min 0 } { Max 1 } }
        { PrintName "Upper Bound" }
        { IsReadOnly True }
        { HelpText "Upper Bound is for migration purposes only." }
    }
}

Include "annotation/t_description.note"


