FileObject	401600	BoochClassDiagram
{
    { Method 
	{ Name	"Navigate" }
	{ Cmd 	"stpem -ed class -first -p ${projdir} -s ${system} -C 'EditorQrlEval NavLoadAndFind(\"${file.name}\",\"${symbol_ref.appid}\");' &" } }
    { Method 
	{ Name	"SymbolNavigate" }
	{ Cmd 	"stpem -ed class -first -p ${projdir} -s ${system} -C 'EditorQrlEval NavLoadAndFind(\"${file.name}\",\"${symbol_ref.appid}\");' &" } }
    { Method 
	{ Name	"FileNavigate" }
	{ Cmd 	"stpem -ed class -first -p ${projdir} -s ${system} -C 'EditorQrlEval NavLoadAndFind(\"${file.name}\",NULL);' &" } }
    { Method 
	{ Name	"Print" }
	{ Cmd 	"dprint -ed class -p ${projdir} -s ${system} -rules_print ${print_rules_file} -print_setting ${setting_name} ${file} &" } }
}

