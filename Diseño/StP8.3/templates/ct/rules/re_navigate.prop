PropertySheet BrowserCodeBrowseChooser
{
    { Name "BrowserCodeBrowseChooser" }
    { Label "StP: Choose Proper Code Reference" }
    { Properties
	{ Qrl { QrlFile "rules/qrl/re_navigate.inc" } }
        // the chooser
	{ List
	    { Name "Chooser" }
	    { Label "Choices:" }
	    { Rows 6 }
	    { Width 80 }
	    { Exclusive True }
	    { LabelAbove True }
	    { Event
                { Type SelectRow }
                { Callback "REBrowserBrowseCodeSelectRow_cb" }
            }
	    { Event
                { Type DoubleClickRow }
                { Callback "REBrowserBrowseCodeDoIt_cb" }
            }
	}
	{ Button
	    { Name "OK" }
	    { Label "Browse To Code" }
	    { Event
		{ Type Select }
		{ Callback "REBrowserBrowseCodeDoIt_cb" }
	    }
	}
	{ Button
	    { Name "Apply" }
	    { Label "Browse To Design" }
	    { Event
		{ Type Select }
		{ Callback "REBrowserBrowseDesignDoIt_cb" }
	    }
	}
    }
}


PropertySheet SimpleTextViewer
{
    { Name "SimpleTextViewer" }
    { Label "StP: View Text" }
    { Properties
	{ Text
	    { Name "TheText" }
	    { Label "" }
	    { LabelAbove True }
	    { DispRows 20 }
	    { DispLength 70 }
	}
        { Text
 	    { Name "TextFile" }
 	    { Label "" }
 	    { LabelAbove True }
 	    { HorizontalScrollBar 1 }
 	    { MinLength 0 }
 	    { DispLength 60 }
 	    { MaxLength 512 }
	}
        { Group
	    { Name Glue }
	    { Type Vertical }
	    { Align Left }
            { Members ( TextFile TheText ) }
        }
    }
}
 
PropertySheet TextViewBuffer
{
    { Name "TextViewBuffer" }
    { Label "StP: View Code" }
    { Properties
        { Qrl { QrlFile "rules/qrl/re_navigate.inc" } }

        { Text
             { Name "TheTextViewBuffer" }
             { Label "" }
             { LabelAbove True }
             { DispRows 24 }
             //{ MinLength 0 }
             { DispLength 94 }
             //{ MaxLength 512 }
// ECR 6840 begin
             { Resizable ResizeAny }
// ECR 6840 end
        }

        { Button
            { Name "CurFile" }
            { Label "File:" }
            { Event
                { Type Select }
                { Callback "TextViewerLoad_cb" }
            }
        }
 
        { Text
 	        { Name "TextFile" }
 	        { Label "" }
 	        { LabelAbove True }
 	        { HorizontalScrollBar 1 }
 	        { MinLength 0 }
 	        { DispLength 60 }
 	        { MaxLength 512 }
            { Event
                 { Type NewLine }
                 { Callback "TextViewerLoad_cb" }
            }
        }

        // ECR 3651a begin
        { Button
            { Name "browse_for_src_file" }
            { Label "Choose" }
            { Event
                { Type Select }
                { Callback "select_src_file_callback" }
            }
        }
        // ECR 3651a end

        { Button
 	    { Name "GoToSCE" }
	    { Label "Design Module" }
            { Event
                 { Type Select }
                 { Callback "TextViewerBrowseModule_cb" }
            }
        }
        
        { Button
	    { Name "GoToDSE" }
	    { Label "Design Type" }
            { Event
                 { Type Select }
                 { Callback "TextViewerBrowseType_cb" }
            }
        }

        { Group
	    { Name TopLine }
	    { Type Horizontal }
	    { Align Left }
            { Members ( CurFile TextFile TextFileSelection browse_for_src_file GoToSCE GoToDSE ) }
        }


        { Group
	    { Name TViewer }
	    { Type Vertical }
// ECR 6840 begin
        { Resizable ResizeAny }
        { ChildSizePolicy ResizeAny }
// ECR 6840 end
	    { Align Left }
            { Members ( TopLine TheTextViewBuffer ) }
        }
    }
}


