PropertySheet SourceCodeDirectoryDialogue
{
    { Name SourceCodeDirectoryDialogue }
    { Label "StP: Set Source Code Directory" }
    { Properties
        {  Qrl { QrlFile "rules/qrl/re_navigate.inc" } }
	{ Text
	    { Name SCDir }
	    { Label "Source Directory:" }
	    { MinLength 0 }
	    { DispLength 60 }
	    { MaxLength	200 }
        }
    // ECR 3651a begin
    { Button
        { Name "browse_for_source_dir" }
        { Label "Choose" }
        { Event
            { Type Select }
            { Callback "select_source_code_dir_callback" }
        }
    }
    { Group
        { Name "folder_group" }
        { Label "" }
        { Type Horizontal }
        { Resizable ResizeAny }
        { ChildSizePolicy ResizeWidth }
        { LayoutPolicy ResizeAny }
        { Align Bottom }
        { Members ( SCDir browse_for_source_dir ) }
    }
    // ECR 3651a end
	{ Button
	    { Name "OK" }
	    { Label "OK" }
	    { Event
		{ Type Select }
		{ Callback "SourceCodeDir_cb" }
	    }
	}
	{ Footer
	    { Name Status }
	    { RightFooter "" }
	    { Active True }
	}
    }

}
