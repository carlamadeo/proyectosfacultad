Editor cse
{
  { PrintName "Control Spec Editor" }
  { IconFile "cse" }
  { RulesFile "rules/cse.rules" }
  { QrlFile "rules/qrl/cse.qrl" }
  { CreationCommand "stp_startup -C 'gte -p ${projdir} -s ${system} -ed cse'" }
  { FileType Table }
  { EditorFileGroup
      { AppType CspecTable }
      { PrintName "Cspec Table" }
      { FileStore "cse_files" } 
      { Suffix ".cspec" }
      { IsRepSource False }
    }
  { EditorFile
      { AppType DecisionTable }
      { PrintName "Decision Table" }
      { FileStore "cse_files" } 
      { Suffix ".det" }
      { IsRepSource True }
    }
  { EditorFile
      { AppType ProcessActivationTable }
      { PrintName "Process Activation Table" }
      { FileStore "cse_files" }
      { Suffix ".pat" }
      { IsRepSource True }
    }
  { EditorFile
      { AppType ProcessActivationMatrix }
      { PrintName "Process Activation Matrix" }
      { FileStore "cse_files" }
      { Suffix ".pam" }
      { IsRepSource True }
    }
  { EditorFile
      { AppType StateTransitionTable }
      { PrintName "State Transition Table" }
      { FileStore "cse_files" }
      { Suffix ".stt" }
      { IsRepSource True }
    }
  { EditorFile
      { AppType StateEventMatrix }
      { PrintName "State Event Matrix" }
      { FileStore "cse_files" }
      { Suffix ".sem" }
      { IsRepSource True }
    }
  { EditorFile
      { AppType EventLogicTable }
      { PrintName "Event Logic Table" }
      { FileStore "cse_files" }
      { Suffix ".elt" }
      { IsRepSource True }
    }
  { EditorFile
      { AppType ActionLogicTable }
      { PrintName "Action Logic Table" }
      { FileStore "cse_files" }
      { Suffix ".alt" }
      { IsRepSource True }
    }
}


