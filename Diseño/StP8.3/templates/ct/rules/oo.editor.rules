Editor cte
{
    { PrintName "StP Class Table Editor" }
    { IconFile "cte" }
    { RulesFile "rules/cte.rules" }
    { QrlFile "rules/qrl/cte.qrl" }
    { CreationCommand "stp_startup -C 'gte -p ${projdir} -s ${system} -ed cte'" }
    { FileType Table }
    { EditorFile
	{ AppType OMTClassTable }
	{ PrintName "Class Table" }
	{ Suffix ".cte" }
	{ FileStore "cte_files" }
	{ IsRepSource True }
    }
}

