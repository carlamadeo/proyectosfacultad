        { Entry
                { Type MenuShortCut }
                { Image "refresh" }
                { MenuEntry "View:UpdateDmarks" }
                { HelpTip "Refresh Display Marks" }
        }

        { Entry
                { Type Separator }
        }

        { Entry
                { Type MenuShortCut }
                { Image "align" }
                { MenuEntry "Tools:Align" }
                { HelpTip "Align" }
        }
        // ECR 149
        { Entry
            { Type Toggle }
            { Name "OrthogonalArcs" }
            // ECR 7464
            // { Builtin "EditorQrlEval gde_toggle_draw_ortho();" }
            { Builtin "EditorQrlEvalNoSave gde_toggle_draw_ortho();" }
            { HelpTip "Toggle Orthogonal Drawing" }
            { Image "ortho_draw" }
