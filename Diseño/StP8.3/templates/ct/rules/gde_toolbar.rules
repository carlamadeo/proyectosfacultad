        // Add ECR 4049
        { Entry
	    { Type Separator }
        }
        // Add ECR 3789
        { Entry
            { ParseCondition "is_product_uml();" }
            { Type MenuShortCut }
            { Image "edit_notedesc" }
            { MenuEntry "MenuBarEdit:EditNoteDescEM" }
        	{ ActiveFunc idehook_one_semantic_sym_selected_item_active_func }
            { HelpTip "Object Description" }
        }
        // End Add ECR 3789
        { Entry
            { ParseCondition "is_product_uml();" }
            { Type MenuShortCut }
            { Image "properties" }
            { MenuEntry "MenuBarEdit:ObjectProperties" }
            { HelpTip "Properties" }
        }
        // Properties in SE are coded individually for each editor 
        { Entry
            { ParseCondition "current_editor_apptype() == \"DseDiagram\";" }
            { ActiveFunc idehook_dse_structure_props_active_func }
            { Type MenuShortCut }
            { Image "properties" }
            { MenuEntry "MenuBarEdit:SetPropSeq" }
            { HelpTip "Properties" }
        }
        { Entry
            { ParseCondition "current_editor_apptype() == \"SceDiagram\";" }
            { ActiveFunc idehook_sce_object_props_active_func }
            { Type MenuShortCut }
            { Image "properties" }
            { MenuEntry "MenuBarEdit:SetSceProps" }
            { HelpTip "Properties" }
        }
        { Entry
            { ParseCondition "is_product_se() && current_editor_apptype() != \"SceDiagram\" && current_editor_apptype() != \"DseDiagram\";" }
            { ActiveFunc never_active }
            { Type Button }
            { Image "properties" }
            { HelpTip "Properties" }
        }
        { Entry
            { Type MenuShortCut }
            { Image "object_annot" }
            { MenuEntry "MenuBarEdit:EditAnnot" }
            { HelpTip "Object Annotation" }
        }
        { Entry
            { Type MenuShortCut }
            { Image "diag_annot" }
            { MenuEntry "MenuBarEdit:EditDiagAnnot" }
            { HelpTip "Diagram Annotation" }
        }

        // ECR6316 
        { Entry
              { Type Separator }
        }

        { Entry
              { Name "CheckSyntax" }
              { Type MenuShortcut }
              { MenuEntry "Tools:CheckSyntax" }
              { Image "check_syntax" }
              { HelpTip "Check Syntax" }
        }

        { Entry
              { Name "CheckSemantics" }
              { Type MenuShortcut }
              { MenuEntry "Tools:CheckSemantics" }
              { Image "check_semantics" }
              { HelpTip "Check Semantics" }

			  // ECR6673
              { ParseCondition "current_editor_apptype() != \"FceDiagram\";" }
        }
        { Entry
              { Type Separator }
        }
