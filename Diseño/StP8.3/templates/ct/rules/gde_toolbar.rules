                { Type MenuShortCut }
                { Image "zoom_to_fit" }
                // depending on whether there is a current selection, it
                // will fire off either the Fit Selection or Fit Diagram
                { MenuEntry "View:ZoomToSelection" }
                { HelpTip "Zoom To Fit" }
        }

