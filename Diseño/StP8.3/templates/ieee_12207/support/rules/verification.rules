

Class 0 SysRequirementsAnalysisVerification
{
    { ParseCondition "(toolinfo_variable(\"SysRqtsModeler\") == \"True\");" }
    { Label "Verification (Model)" }
    { Image "requirements_tables" }
    { ShowOnDesktop True }
}



Class 0 SwRequirementsAnalysisVerification
{
    { ParseCondition "(toolinfo_variable(\"SwRqtsModeler\") == \"True\");" }
    { Label "Verification (Model)" }
    { Image "requirements_tables" }
    { ShowOnDesktop True }

    { UseClassCommand VerifyTestReadiness }
    { UseClassCommand SoftwareReliability }
}



Class 0 SwArchitectureDesignVerification
{
    { ParseCondition "(toolinfo_variable(\"ComponentModeler\") == \"True\");" }
    { Label "Verification (Model)" }
    { Image "component_diagrams" }
    { ShowOnDesktop True }
}




Class 0 SwDetailedDesignVerification
{
    { ParseCondition "(toolinfo_variable(\"ClassModeler\") == \"True\");" }
    { Label "Verification (Model)" }
    { Image "class_diagrams" }
    { ShowOnDesktop True }

}



Global 1 VerifyTestReadiness
{
 { ClassCommand
        { Name "VerifyTestReadiness" }
	{ Label "Generate Test Readiness Report" }
	{ ShowPropsOnExec True }
	{ Type Nowin }
	{ Command "qrp  -forward_message ${msgd_handle} -p ${projdir} -s ${system} -t rtf -o ${projdir}${system}/qrl_files/TestReadinessReport.rtf support/qrl/GenerateTestReadinessReport.qrl"}

  }

}

Global 1 SoftwareReliability
{
 { ClassCommand
        { Name "SoftwareReliability" }
	{ Label "Generate Operational Profiles" }
	{ ShowPropsOnExec True }
	{ Type Nowin }
	{ Command "qrp  -forward_message ${msgd_handle} -p ${projdir} -s ${system} -t rtf -o ${projdir}${system}/qrl_files/OperationalProfile.rtf support/qrl/GenerateOperationalProfile.qrl"}

  }
}

Global 5 DocumentPath
{
    { Properties
	{ Text
	    { Name "OutputPath" }
	    { Label "Output Directory:" }
	    { Init "" }
    	    { Type Text }
    	    { MinLength 1 }
	    { MaxLength 80 }
	    { DispLength 20 }
	}
    }
}


