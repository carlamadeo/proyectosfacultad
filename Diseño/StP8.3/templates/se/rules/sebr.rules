// TABLE ELEMENT TYPE DEFINITIONS
// these are used to define the operations for each section of the table

HsectType 1 SEModel
{
  { RowSorts
      { Sort
	  { Name SortByNameIncreasing }
	  { Label "Diagrams By Name" }
	  { Key 	
	      { Value 
		  { LabelSelector Label }
		  { Cell
		      { Hsect HSECT }
		      { Vsect DESCRIPTION_VSECT }
		      { Row ROW }
		      { Col OBJECT_NAME_COL } } } 
	      { Ordering LexicographicalIncreasing } } } 
      { Sort
	  { Name SortByNameTypeIncreasing }
	  { Label "Diagrams By Type, Name" }
	  { Key 	
	      { Value 
		  { LabelSelector Label }
		  { Cell
		      { Hsect HSECT }
		      { Vsect DESCRIPTION_VSECT }
		      { Row ROW }
		      { Col DIAGRAM_TYPE_COL } } } 
	      { Ordering LexicographicalIncreasing } } 
	  { Key 	
	      { Value 
		  { LabelSelector Label }
		  { Cell
		      { Hsect HSECT }
		      { Vsect DESCRIPTION_VSECT }
		      { Row ROW }
		      { Col OBJECT_NAME_COL } } } 
	      { Ordering LexicographicalIncreasing } } } } }

RowType 10 SEModel
{ 
  { Semantics
      { FileMapping 
	  { MappingName Id }
	  { Id 
	      { LabelSelector Label }  
	      { Cell { Hsect HSECT } { Vsect ID_VSECT } { Row ROW } { Col OID_COL } } } } }
  { Methods
      { Navigation 
	  { Name SEModel }
	  { Label "SE Diagram" }
	  { MappingName Id }
	  { Query "file[id=${file.id}]" } }
      { Browse
	  { Name classes }
	  { Label "SE Diagram's Modules" }
	  { MappingName Id }
	  { Eval "SEHyperbrowse(\"node[Module && node_refs[file_id==${file.id}]]\");" } }
      { Browse
	  { Name allnodes }
	  { Label "SE Diagram's Processes" }
	  { MappingName Id }
	  { Eval "SEHyperbrowse(\"node[Process && node_refs[file_id==${file.id}]]\");" } }
      { Browse
	  { Name everything }
	  { Label "SE Diagram's Entire Contents" }
	  { MappingName Id }
	  { Eval "SEHyperbrowseModelContents();" } }
    }
}

