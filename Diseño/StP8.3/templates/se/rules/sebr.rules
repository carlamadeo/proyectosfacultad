		  { HorzAlignment Left }	
		  { Bold True }	} } } }
  { Hsect 
      { Label "Modules QBE" } 	
      { Shading Unshaded }
      { Frozen True } 
      { Scrollbar False }
      { Vsect 
	  { Row 	
              { Type ModuleQuery }
	      { Cell } } } 
      { Vsect 
	  { Row 	
              { Type ModuleQuery }
	      { Cell 
                  { Type ModulePattern }
		  { Label "*" }
		  { HorzAlignment Left }
		  { Bold True } }
	      { Cell 
                  { RepeatToEnd True }
                  { Type ModuleParentPattern }
		  { Label "*" }
		  { HspanSection True }
		  { HorzAlignment Left }
		  { Bold True } } } } }

  { Hsect 
      { Type Module }
      { Label "Modules Body" } 	
      { Vsect 
	  { Row 	
              { Type Module }
              { RepeatToEnd True }
	      { Cell 
                  { Type ModuleId }
		  { HorzAlignment Right } } } }
      { Vsect 
	  { Row 	
              { Type Module }
              { RepeatToEnd True }
	      { Cell 
                  { Type Module }
		  { HorzAlignment Left } } 
	      { Cell 
                  { Type ModuleParent }
		  { HspanSection True }
		  { HorzAlignment Left } } } } }

  // Parameters

  { Hsect 
      { Label "Parameters Header" } 	
      { ReadOnly True }
      { Shading Shade1 }
      { Frozen True } 
      { Scrollbar False }
      { RowHeight HEADER_HEIGHT }
      { Vsect 
	  { Row 	
	      { Cell 
		  { Label "Id" }
		  { HorzAlignment Right }
		  { Bold True } } } }
      { Vsect 
	  { Row 			
	      { Cell 
		  { Label "Parameter" } 	
		  { HorzAlignment Left }	
		  { Bold True }	}
	      { Cell 
		  { Label "Type" } 	
		  { HorzAlignment Left }	
		  { Bold True }	} 
	      { Cell 
		  { Label "Kind" } 	
		  { HspanSection True }
		  { HorzAlignment Left }	
		  { Bold True }	} } } }
  
  { Hsect 
      { Label "Parameters QBE" } 	
      { Shading Unshaded }
      { Frozen True } 
      { Scrollbar False }
      { Vsect 
	  { Row 	
              { Type ParameterQuery }
	      { Cell } } } 
      { Vsect 
	  { Row 	
              { Type ParameterQuery }
	      { Cell 
                  { Type ParameterPattern }
		  { Label "*" }
		  { HorzAlignment Left }
		  { Bold True } }
	      { Cell 
                  { Type ParameterTypePattern }
		  { Label "*" }
		  { HorzAlignment Left }
		  { Bold True } } 
	      { Cell 
                  { Type ParameterKindPattern }
		  { Label "*" }
		  { HspanSection True }
		  { HorzAlignment Left }
		  { Bold True } } } } }

  { Hsect 
      { Type Parameter }
      { Label "Parameters Body" } 	
      { Vsect 
	  { Row 	
              { Type Parameter }
              { RepeatToEnd True }
	      { Cell 
                  { Type ParameterId }
		  { HorzAlignment Right } } } }
      { Vsect 
	  { Row 	
              { Type Parameter }
              { RepeatToEnd True }
	      { Cell 
                  { Type Parameter }
		  { HorzAlignment Left } }
	      { Cell 
                  { Type ParameterType }
		  { HorzAlignment Left } }
	      { Cell 
                  { Type ParameterKind }
		  { HspanSection True }
	          { HorzAlignment Left } } } } } 

  // Processes

  { Hsect 
      { Label "Processes Header" } 	 
      { ReadOnly True }
      { Shading Shade1 }
      { Frozen True } 
      { Scrollbar False }
      { RowHeight HEADER_HEIGHT }
      { Vsect 
	  { Row 	
	      { Cell 
		  { Label "Id" }
		  { HorzAlignment Right }
		  { Bold True } } } }
      { Vsect 
	  { Row 			
	      { Cell 
		  { Label "Process" } 	
		  { HorzAlignment Left }	
		  { Bold True }	}
	      { Cell 
		  { Label "Index" } 	
		  { HorzAlignment Left }	
		  { Bold True }	}
	      { Cell 
		  { Label "Parent Process" } 	
		  { HorzAlignment Left }	
		  { Bold True }	}
	      { Cell 
		  { Label "Children Processes" } 	
                  { Width WIDE_COL_WIDTH }
		  { HorzAlignment Left }	
		  { HspanSection True }
		  { Bold True }	} } } } 
  { Hsect 
      { Label "Processes QBE" } 	
      { Shading Unshaded }
      { Frozen True } 
      { Scrollbar False }
      { Vsect 
	  { Row 	
              { Type ProcessQuery }
	      { Cell } } } 
      { Vsect 
	  { Row 	
              { Type ProcessQuery }
	      { Cell 
                  { Type ProcessPattern }
		  { Label "*" }
		  { HorzAlignment Left }
		  { Bold True } }
	      { Cell 
