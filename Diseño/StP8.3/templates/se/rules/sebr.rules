TableType 1 SETabularBrowser
{ 
  { Label "SE Tabular Browser" }
  { Browser True }


  // modifiable borders
  { EmptyBorder 	{ Visible False } }
  { RowBorder 		{ Visible True } { Pattern Dotted } }
  { ColBorder 		{ Visible False } { Pattern Dotted } }
  { HsectBorder 	{ Double False }  { Thickness Thin } } 
  { VsectBorder 	{ Double False }  { Thickness Thin } } 
  { LeftBorder 		{ Thickness Medium } }
  { RightBorder 	{ Thickness Medium } }
  
  { Initialize

      // modifiable symbols
      { Unsecured 
	  { ROW_HEIGHT	          18 } 
	  { WIDE_COL_WIDTH	 160 }
	  { NARROW_COL_WIDTH	 100 }
	  { ID_COL_WIDTH	  70 }
	  { HEADER_HEIGHT         23 } }

      // non-modifiable symbols
      { Secured

	  // indices for the horizontal sections, 
	  // which correspond to the major semantic categories in the methodology
          { SE_MODEL_HEADER_HSECT       1 } 
	  { SE_MODEL_QBE_HSECT          2 } 
	  { SE_MODEL_HSECT              3 } 

	  { MODULE_HEADER_HSECT	        4 } 
	  { MODULE_QBE_HSECT            5 }
	  { MODULE_HSECT                6 }

	  { PARAMETER_HEADER_HSECT      7 } 
	  { PARAMETER_QBE_HSECT         8 }
	  { PARAMETER_HSECT	        9 }

	  { PROCESS_HEADER_HSECT        10 } 
	  { PROCESS_QBE_HSECT	        11 }
	  { PROCESS_HSECT	        12 }

	  { FLOW_HEADER_HSECT           13 } 
	  { FLOW_QBE_HSECT              14 }
	  { FLOW_HSECT	                15 }

	  { ADT_HEADER_HSECT            16 } 
	  { ADT_QBE_HSECT               17 }
	  { ADT_HSECT	                18 }

	  // indices for the vertical sections
	  { ID_VSECT                    1 } 
	  { DESCRIPTION_VSECT           2 }

	  // column indices for each horizontal section. 
	  // used in semantics, methods, and/or sorting.

	  { OID_COL		        1 }
	  // first column in second section
          { OBJECT_NAME_COL             1 }
	
	  { DIAGRAM_TYPE_COL            2 }

	  { MODULE_PARENTS_COL          2 }

	  { PARAMETER_TYPE_COL	        2 }
	  { PARAMETER_KIND_COL	        3 }

	  { PROCESS_INDICES_COL	        2 }
	  { PROCESS_PARENT_COL	        3 }
	  { PROCESS_CHILDREN_COL	4 }

	  { FLOW_TYPE_COL	        2 }
	  { FLOW_KIND_COL	        3 }

	  { T_PRIMTYPE_COL	        2 }
	}
    } // end Initialize
  
  { RowHeight ROW_HEIGHT } 
  { ColWidth NARROW_COL_WIDTH }

  // SE Diagrams 

  { Hsect 
      { Label "Diagrams Header" } 	
      { ReadOnly True }
      { Shading Shade1 }
      { Frozen True } 
      { Scrollbar False }
      { RowHeight HEADER_HEIGHT }
      { Vsect 
	  { Label "Object Ids" }		
          { Frozen True } 
          { Hidden True }
	  { Scrollbar False } 
	  { Row 	
	      { Cell 
		  { Label "Id" }
                  { Width ID_COL_WIDTH }
		  { HorzAlignment Right }
		  { Bold True } } } }
      { Vsect 
	  { Label "Object Descriptions" }	 
	  { Frozen True }
	  { ColBorder { Visible False } }
	  { Row 			
	      { Cell 
		  { Label "Diagram" } 
                  { Width WIDE_COL_WIDTH }
		  { HorzAlignment Left }	
		  { Bold True }	}
	      { Cell 
		  { Label "Type" } 
		  { HspanSection True }
		  { HorzAlignment Left }	
		  { Bold True }	} } } }
  { Hsect 
      { Label "Diagrams QBE" } 	
      { Shading Unshaded }
      { Frozen True } 
      { Scrollbar False }
      { Vsect
	  { Row 	
              { Type SEModelQuery }
	      { Cell } } } 
      { Vsect 
	  { Row 	
              { Type SEModelQuery }
	      { Cell 
                  { Type SEModelPattern }
		  { Label "*" }
		  { HorzAlignment Left }
		  { Bold True } }
	      { Cell 
                  { Type SEModelTypePattern }
		  { Label "*" }
		  { HspanSection True }
		  { HorzAlignment Left }
		  { Bold True } } } } }
  { Hsect 
      { Type SEModel }
      { Label "Diagrams Body" } 	
      { Vsect 
	  { Row 	
              { Type SEModel }
              { RepeatToEnd True }
	      { Cell 
                  { Type SEModelId }
		  { HorzAlignment Right } } } }
      { Vsect 
	  { Row 	
              { Type SEModel }
              { RepeatToEnd True }
	      { Cell 
                  { Type SEModel }
		  { HorzAlignment Left } } 
	      { Cell 
                  { Type SEModelType }
		  { HspanSection True } 
		  { HorzAlignment Left } } } } }

  // Modules

  { Hsect 
      { Label "Modules Header" } 	
      { ReadOnly True }
      { Shading Shade1 }
      { Frozen True } 
      { Scrollbar False }
      { RowHeight HEADER_HEIGHT }
      { Vsect 
	  { Row 	
	      { Cell 
		  { Label "Id" }
		  { HorzAlignment Right }
		  { Bold True } } } }
      { Vsect 
	  { Row 			
	      { Cell 
		  { Label "Module" } 	
		  { HorzAlignment Left }	
		  { Bold True }	}
	      { Cell 
		  { Label "Parent Modules" } 
