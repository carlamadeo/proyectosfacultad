		  { Label "*" }
		  { HorzAlignment Left }
		  { Bold True } } 
	      { Cell 
                  { Type FlowKindPattern }
		  { Label "*" }
		  { HspanSection True }
		  { HorzAlignment Left }
		  { Bold True } } } } }

  { Hsect 
      { Type Flow }
      { Label "Flows Body" } 	
      { Vsect 
	  { Row 	
              { Type Flow }
              { RepeatToEnd True }
	      { Cell 
                  { Type FlowId }
		  { HorzAlignment Right } } } }
      { Vsect 
	  { Row 	
              { Type Flow }
              { RepeatToEnd True }
	      { Cell 
                  { Type Flow }
		  { HorzAlignment Left } }
	      { Cell 
                  { Type FlowType }
		  { HorzAlignment Left } }
	      { Cell 
                  { Type FlowKind }
		  { HspanSection True }
	          { HorzAlignment Left } } } } } 

  // Abstract Data Types

  { Hsect 
      { Label "Abstract Data Types Header" } 	
      { ReadOnly True }
      { Shading Shade1 }
      { Frozen True } 
      { Scrollbar False }
      { RowHeight HEADER_HEIGHT }
      { Vsect 
	  { Label "Object Ids" }		
          { Frozen True } 
          { Hidden True }
	  { Scrollbar False } 
	  { Row 	
	      { Cell 
		  { Label "Id" }
                  { Width ID_COL_WIDTH }
		  { HorzAlignment Right }
		  { Bold True } } } }
      { Vsect 
	  { Label "Object Descriptions" }	 
	  { Frozen True }
	  { ColBorder { Visible False } }
	  { Row 			
	      { Cell 
		  { Label "Abstract Data Type" } 
                  { Width WIDE_COL_WIDTH }
		  { HspanSection True }
		  { HorzAlignment Left }	
		  { Bold True }	} } } }
  { Hsect 
      { Label "Abstract Data Types QBE" } 	
      { Shading Unshaded }
      { Frozen True } 
      { Scrollbar False }
      { Vsect
	  { Row 	
              { Type SEADTQuery }
