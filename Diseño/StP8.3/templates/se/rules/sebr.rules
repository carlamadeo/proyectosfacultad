HsectType 2 Module
{
  { RowSorts
      { Sort
	  { Name SortByNameIncreasing }
	  { Label "Modules By Name" }
	  { Key 	
	      { Value 
		  { LabelSelector Label }
		  { Cell
		      { Hsect HSECT }
		      { Vsect DESCRIPTION_VSECT }
		      { Row ROW }
		      { Col OBJECT_NAME_COL } } } 
