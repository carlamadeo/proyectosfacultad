		  { HorzAlignment Left } }
	      { Cell 
                  { RepeatToEnd True }
                  { Type ProcessChildren }
		  { HorzAlignment Left } } } } } 

  // Data and Control Flows

  { Hsect 
      { Label "Flows Header" } 	
      { ReadOnly True }
      { Shading Shade1 }
      { Frozen True } 
      { Scrollbar False }
      { RowHeight HEADER_HEIGHT }
      { Vsect 
	  { Row 	
