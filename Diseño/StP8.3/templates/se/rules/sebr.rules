RowType  20  Module                     
{ 
  { Semantics
      { NodeMapping 
	  { MappingName Id }
	  { Id 
	      { LabelSelector Label }  
	      { Cell { Hsect HSECT } { Vsect ID_VSECT } { Row ROW } { Col OID_COL } } } } }
  { Methods
      { Navigation 
	  { Name DefaultView }
	  { Label "Any View of Module" }
	  { MappingName Id }
          { Query "node_ref[file[SceDiagram] && node_id == ${node.id}]" }         
	  { Query "node_ref[node_id == ${node.id}]"} 
       }
      { Navigation
	  { Name Formal }
	  { MappingName Id }
	  { Label "Formal Signature for Module" }
	  { Query "node_ref[node_id == ${node.id} && file[SceDiagram] && in_link_refs[link[SubprogramCall && from_node[FormalCaller]]]]" }	}
      { Navigation
	  { Name Actual }
	  { MappingName Id }
	  { Label "Actual Occurance of Module" }
	  { Query "node_ref[node_id == ${node.id} && file[SceDiagram] && ! in_link_refs[link[SubprogramCall && from_node[FormalCaller]]]]" }	}
      { Browse
	  { Name module_parent }
	  { Label "Module's Callers" }
	  { MappingName Id }
	  { Eval "SEHyperbrowse(\"node[Module && node_refs && out_links[SubprogramCall && to_node_id == ${node.id}]]\");" } }
      { Browse
	  { Name module_children }
	  { Label "Module's Callees" }
	  { MappingName Id }
	  { Eval "SEHyperbrowse(\"node[Module && node_refs && in_links[SubprogramCall && from_node_id == ${node.id}]]\");" } }
      { Browse
	  { Name module_formal }
	  { Label "Module's Formal Parameters" }
	  { MappingName Id }
	  { Eval "SEHyperbrowse(\"cntx[cntx_refs && link[SubprogramCall && from_node[FormalCaller] && to_node[id = ${node.id}]]]\");" } }
      { Browse
	  { Name module_actual }
	  { Label "Module's Actual Parameters" }
	  { MappingName Id }
	  { Eval "SEHyperbrowse(\"cntx[cntx_refs && link[SubprogramCall && ! from_node[FormalCaller] && to_node[id = ${node.id}]]]\");" } }
    }
}

