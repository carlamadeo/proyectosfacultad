              { RepeatToEnd True }
	      { Cell 
                  { Type SEADTId }
		  { HorzAlignment Right } } } }
      { Vsect 
	  { Row 	
              { Type SEADT }
              { RepeatToEnd True }
	      { Cell 
                  { Type SEADT }
		  { HspanSection True }
		  { HorzAlignment Left } } } } }

