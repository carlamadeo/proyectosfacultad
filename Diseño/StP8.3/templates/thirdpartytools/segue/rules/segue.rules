

Class 0 SegueIntegration
{
  { Label "Segue Tools" }
  { ParseCondition "(toolinfo_variable(\"Segue\") == \"True\");" }
  { Image "use_cases" }
  { ShowOnDesktop True }
  { OMSObjects
      { Match "node[UmlUseCase && node_refs]" }
  } 
  { UseObjectCommand GenerateQAOrganizerIntegration }
//  { UseObjectCommand GenerateSilkTestScripts }

}




Global 1 GenerateQAOrganizerIntegration
{
  { ObjectCommand
        { Name "GenerateQAOrganizerIntegration" }
	{ Label "Generate QAOrganizer Test Plan" }
	{ Type Nowin }
	{ Command "qrp  -forward_message ${msgd_handle} -p ${projdir} -s ${system}  -x UseCases '${cmd.objects}'   thirdpartytools/segue/qrl/GenerateQAOrganizerIntegration.qrl" }
  }
}



Global 1 GenerateSilkTestScripts
{
  { ObjectCommand
        { Name "GenerateSilkTestScripts" }
	{ Label "Generate SilkTest Scripts" }
	{ Type Nowin }
	{ Command "tdrv.exe qrp  -forward_message ${msgd_handle}  -p ${projdir} -s ${system}  -x UseCases '${cmd.objects}'   thirdpartytools/segue/qrl/GenerateSilkTestIntegration.qrl" }
  }



}

