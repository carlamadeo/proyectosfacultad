
 Class 0  SysDOORS
{
  { ParseCondition "(toolinfo_variable(\"SysDOORS\") == \"True\");" }
  { Label "Specify Rqts (DOORS)" }
  { ShowOnDesktop True }
  { Image "requirements_tables" }
  { ClassCommand
        { Name "StartDOORS" }
	{ Label "Start DOORS" }
	{ ShowPropsOnExec True }
	{ Type Nowin }
        { Command "${DOORSHOME}\bin\DOORS.exe -project '${DoorsProjectSystem}' -user '${DoorsUserName}' -password '${DoorsPassword}' &" } 

  }
} 



