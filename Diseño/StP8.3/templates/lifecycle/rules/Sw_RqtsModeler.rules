Class 0 RqtsModeler_Sw_TestTypeTables
{
    { ParseCondition "(toolinfo_variable(\"SwRqtsModeler\") == \"True\");" }
    { Label "Model Rqts (Data Type Tables)" }
    { Image "test_type_tables" }
    { ShowOnDesktop True }
 
    { OMSObjects
 //       { Match "file[TTypeTable && items[ LifeCycleName && value == 'SoftwareRequirements']]" }
        { Match "file[TTypeTable ]" }
	{ Editor "tte" }
    }

    { UseClassCommand StartTableEditor }
    { UseObjectCommand EditTable }
    { UseObjectCommand Copy }
    { ObjectCommand
        { Name "Copy" }
        { Label "Copy Table..." }
    }
    { UseObjectCommand Rename }
    { ObjectCommand
        { Name "Rename" }
        { Label "Rename Table..." }
    }
    { UseObjectCommand Delete }
    { ObjectCommand
        { Name "Delete" }
        { Label "Delete Table..." }
    }
    { UseObjectCommand LockingCommands }
    { UseObjectCommand VccCommands }
}
