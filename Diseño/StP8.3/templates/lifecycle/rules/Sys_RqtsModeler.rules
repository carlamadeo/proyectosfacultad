Class 0 RqtsModeler_Sys_TestTypeTables
{
    { ParseCondition "(toolinfo_variable(\"SysRqtsModeler\") == \"True\");" }
    { Label "Model Rqts (Data Types)" }
    { Image "test_type_tables" }
    { ShowOnDesktop True }
 
    { OMSObjects
//        { Match "file[TTypeTable && items[ LifeCycleName && value == 'SystemRequirements']]" }
        { Match "file[TTypeTable ]" }
	{ Editor "tte" }
    }

    { UseClassCommand StartTableEditor }
    { UseObjectCommand EditTable }
    { UseObjectCommand Copy }
    { ObjectCommand
        { Name "Copy" }
        { Label "Copy Table..." }
    }
    { UseObjectCommand Rename }
    { ObjectCommand
        { Name "Rename" }
        { Label "Rename Table..." }
    }
    { UseObjectCommand Delete }
    { ObjectCommand
        { Name "Delete" }
        { Label "Delete Table..." }
    }
    { UseObjectCommand LockingCommands }
    { UseObjectCommand VccCommands }
}
