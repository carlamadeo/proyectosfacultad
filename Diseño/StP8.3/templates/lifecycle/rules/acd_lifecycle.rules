Class 0 GenAdaCode
{
  { UseObjectCommand GenerateAdaTdlCodeObjects }
}

Global 1 GenerateAdaTdlCodeObjects
{
    { ObjectCommand
        { Name "GenerateAdaTdlCodeObjects" }
        { Label "Generate Ada Code by Template..." }
        { Command "qrp -forward_messages ${msgd_handle} -p ${projdir} -s ${system} -x tdl_template '${prop.tdl_template}' -x tdl_language 'ada' -x file_directory '${prop.output_dir}' ${prop.generate_imf} qrl/code_gen/acd_code_gen.qrl" }
        { Chdir False }
        { SingleObject False }
        { UseProperty AdaDefaultOutputDir }
        { Properties
            { Group
                { Name "Options1" }
                { Label "Options:" }
                { Type Vertical }
                { Resizable ResizeAny }
                { ChildSizePolicy ResizeWidth }
                { LayoutPolicy ResizeAny }
                { HorizontalAlign AlignLabels }
                { Members ( tdl_template output_dir generate_imf generate_ada83 ) }
            }
            { TopGroup
                { Align Left }
                { Members ( Options1 ) }
            }
        }
    }
}

Global 1 AdaDefaultOutputDir
{
    { Properties
        { Text
            { Name "tdl_template" }
            { Label "Template:" }
            { Resizable ResizeAny}
            { Type Text }
            { MinLength 0 }
            { MaxLength 300 }
            { DispLength 40 }
            { InitVal "ada_main.tdl" }
        }
        { Text
            { Name "output_dir" }
            { Label "Output Directory:" }
            { Resizable ResizeAny}
            { Type Text }
            { MinLength 0 }
            { MaxLength 300 }
            { DispLength 60 }
            { InitVal "${projdir}${system}/src_files" }
        }
        { Choice
            { Name "generate_imf" }
            { Type CheckBox }
            { ChoiceItem
                { Name "generate_imf" }
                { Label "Generate IMF File" }
                { OnValue "-x generate_imf 'True'" }
                { OffValue "-x generate_imf 'False'" }
                { Init On }
            }
        }
        { Choice
            { Name "generate_ada83" }
            { Type CheckBox }
            { ChoiceItem
                { Name "generate_ada83" }
                { Label "Generate Ada 83 Code" }
                { OnValue "-x tdl_language_version 'Ada83'" }
                { OffValue "-x tdl_language_version 'Ada95'" }
                { Init Off }
            }
        }
    }
}

