HsectType 10 TTestUnitHsect
{
    { RowSorts
        { Sort
            { Name SortByNameAscending }
            { Label "By Name Ascending" }
            { Key
                { Value
                    { LabelSelector Label }
                    { Cell
                        { Hsect HSECT }
                        { Vsect MainDefinitionVsect }
                        { Row ROW }
                        { Col TTestUnitNameCol } } }
                { Ordering LexicographicalIncreasing }
            }
        }
        { Sort
            { Name SortByNameDecending }
            { Label "By Name Decending" }
            { Key
                { Value
                    { LabelSelector Label }
                    { Cell
                        { Hsect HSECT }
                        { Vsect MainDefinitionVsect }
                        { Row ROW }
                        { Col TTestUnitNameCol } } }
                { Ordering LexicographicalDecreasing }
            }
        }
    }
}


