// TestUnit Table Definition

TableType 1 TTestUnitTable
{ 
  { Label "Test Unit Table" } 
  { Initialize
      { Secured
          // key positions in the table
	  { MainDefinitionVsect 1 }
       	  { SecondaryDefinitionVsect 2 }
       	  { TExtractPropsVsect 3 }
       	  { TClassNamesVsect 4 }
       	  { TStateNamesVsect 5 }
       	  { TProcessNamesVsect 6 }

	  { TTestUnitHsect 3 } 
	  { TTestUnitNameCol 1 } 

	  // positions for design and implementation notes
	  // relative to their respective
	  // hsects and vsects from above

	  { TExtractPropsCol 1 }
	  { TInheritanceCol 2 } 
	  { TAssociationCol 3 } 

	  { TClassNameCol 1 }   // repeats
	  { TStateNameCol 1 }   // repeats
	  { TProcessNameCol 1 } // repeats
      } 
  }

  { ColWidth 110 }
  { RowHeight 40 }

  // and on with the definition of the table

  // The row above the header

  { Hsect
      { Label "Header" }
      { Frozen True }
      { Scrollbar False } 
      { ReadOnly True }
      { Shading Shade2 }
      { Vsect
          { Frozen True }
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "TestUnit" }
                  { HspanSection True }
                  { Bold True } } } }
      { Vsect
          { Frozen True }
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "OMT Extraction Properties" }
                  { HspanSection True }
                  { Bold True } } } }
      { Vsect
          { Frozen True }
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "SE Extraction Properties" }
                  { HspanSection True }
                  { Bold True } } } }
      { Vsect
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "OMT Classes" }
                  { HspanSection True }
                  { Bold True } } } }
      { Vsect
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "OMT States" }
                  { HspanSection True }
                  { Bold True } } } }
      { Vsect
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "Processes" }
                  { HspanSection True }
                  { Bold True } } } }
      { Vsect
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "SE Modules" }
                  { HspanSection True }
                  { Bold True } } } }
      { Vsect
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "SE Diagrams" }
                  { HspanSection True }
                  { Bold True } } } }
      { Vsect
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "Include Files" }
                  { HspanSection True }
                  { Bold True } } } }
      { Vsect
          { Frozen True }
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "S Packet Information" }
                  { HspanSection True }
                  { Bold True } } } } }
 
  // the header  
  { Hsect 
      { Label "TestUnit Header" }
      { Frozen True }
      { Scrollbar False } 
      { ReadOnly True }
      { Shading Shade1 }
      { Vsect 
          { Scrollbar False }
          { Label "TestUnit Definition" }
	  { Row
	      { Cell
		  { Label "Name" }
		  { Bold True } } } }
      { Vsect 
          { Label "OMT Extraction Props" }
	  { Row { Cell { Label "Use Model(s)" } { Bold True } }
	        { Cell { Label "Inheritance" } { Bold True } }
	        { Cell { Label "Associations" } { Bold True } } } }
      { Vsect 
          { Label "SE Extraction Props" }
	  { Row { Cell { Label "Use Model(s)" } { Bold True } }
	        { Cell { Label "Name or Number" } { Bold True } }
	        { Cell { Label "Hierarchy" } { Bold True } } } }
      { Vsect 
          { Label "Classes (OMT Only)" }
	  { Row { Cell { Label "Class" } { RepeatToEnd True } { Bold True } } } }
      { Vsect 
          { Label "States (OMT Only)" }
	  { Row { Cell { Label "State" } { RepeatToEnd True } { Bold True } } } }
      { Vsect 
          { Label "Processes" }
	  { Row { Cell { Label "Process" } { RepeatToEnd True } { Bold True } } } }
      { Vsect 
          { Label "Modules (SE Only)" }
	  { Row { Cell { Label "Module" } { RepeatToEnd True } { Bold True } } } }
      { Vsect 
          { Label "Diagrams (SE Only)" }
	  { Row { Cell { Label "Diagram" } { RepeatToEnd True } { Bold True } } } }
      { Vsect 
          { Label "Include Files" }
	  { Row { Cell { Label "File" } { RepeatToEnd True } { Bold True } } } }
      { Vsect 
          { Label "S_Packet Information" }
	  { Row
	      { Cell
		  { Label "Name" }
		  { Bold True } } 
	      { Cell
		  { Label "Subject" }
		  { Bold True } } 
	      { Cell
		  { Label "Description" }
		  { Bold True } } } } }

  // the body  
  { Hsect 
      { Type TTestUnitHsect }
      { Label "TestUnits" }
      { Vsect 
          { Scrollbar False }
	  { Row
	      { RepeatToEnd True }
	      { Cell
		  { Type TTestUnitName }
		  { HorzAlignment Left } } } }
      // OMT Basic Values
      { Vsect 	
	  { Row
	      { RepeatToEnd True }
	      { Cell
		  { Type TExtractProps }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TInheritance }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TAssociation }
		  { HorzAlignment Left } } } }
      // SE Basic Values
      { Vsect 	
	  { Row
	      { RepeatToEnd True }
	      { Cell
		  { Type TSEExtractProps }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TNameOrPnum }
		  { HorzAlignment Left } }
	      { Cell
		  { Type THierarchy }
		  { HorzAlignment Left } } } }
       // Repeating Classes
       { Vsect 	
	   { Row
	       { RepeatToEnd True }
	       { Cell
		   { Type TClassName }
		   { HorzAlignment Left } 
		   { RepeatToEnd True }
		   { HorzAlignment Left } } } }
        // Repeating States
	{ Vsect 	
	    { Row
		{ RepeatToEnd True }
		{ Cell
		    { Type TStateName }
		    { HorzAlignment Left } 
		    { RepeatToEnd True }
		    { HorzAlignment Left } } } }
	// Repeating Processes
	{ Vsect 	
	    { Row
		{ RepeatToEnd True }
		{ Cell
		    { Type TProcessName }
		    { HorzAlignment Left } 
		    { RepeatToEnd True }
		    { HorzAlignment Left } } } } 
	// Repeating Modules
	{ Vsect 	
	    { Row
		{ RepeatToEnd True }
		{ Cell
		    { Type TModuleName }
		    { HorzAlignment Left } 
		    { RepeatToEnd True }
		    { HorzAlignment Left } } } } 
	// Repeating Diagrams
	{ Vsect 	
	    { Row
		{ RepeatToEnd True }
		{ Cell
		    { Type TDiagramName }
		    { HorzAlignment Left } 
		    { RepeatToEnd True }
		    { HorzAlignment Left } } } } 
	// Include Files
	{ Vsect 	
	    { Row
		{ RepeatToEnd True }
		{ Cell
		    { Type TIncludeFile }
		    { HorzAlignment Left } 
		    { RepeatToEnd True }
		    { HorzAlignment Left } } } } 
        // S_Packets
        { Vsect 	
	    { Row
	        { RepeatToEnd True }
	        { Cell
		    { Type TS_PacketName }
		    { HorzAlignment Left } }
	        { Cell
		    { Type TS_PacketSubject }
		    { HorzAlignment Left } }
	        { Cell
		    { Type TS_PacketDesc }
		    { HorzAlignment Left } } } } } }

// Testunit Node Semantics

CellType 1 TTestUnitName
{ 
  { Semantics
      { NodeMapping
          { MappingName TTestUnit }
	  { Precondition { LabelSelector Label } }
	  { Name
	      { LabelSelector Label }
	  } 
	  { Type TTestUnit } } 
      { NoteMapping  
          { MappingName TUTInfo }
	  { Type TUTInfo } 
	  { Parent
              { MappingName TTestUnit }
              { Cell
                  { Hsect HSECT }
                  { Vsect VSECT }
                  { Row ROW }
                  { Col COL } } } } }
}

