Class Test
{
    { ClassCommand
        { Name "StartTableEditor" }
        { Command "stpem -ed ${editor} -s ${system} -p ${projdir} -C EditorStart -C EditorShowDeiconify -C 'FileLoad TestUnits' -C 'EditorQrlEval T_set_SE_hide_show();'" }
    }

    { ObjectCommand
        { Name "ExtractTestUnit" }
        { Command "qrp -p ${projdir} -s ${system} -x TTestUnit ${cmd.objects} qrl/se_t.qrl" }
    }

    { ObjectCommand
        { Name "TestUnitEditor" }
        { DefaultCommand True }
        { Command "stpem -ed tut -p ${projdir} -s ${system} -C EditorStart -C EditorShowDeiconify -C 'FileLoad TestUnits' -C 'EditorQrlEval T_highlight_testunit(\"${cmd.objects}\");' -C 'EditorQrlEval T_set_SE_hide_show();' " }
    }
 
    { ObjectCommand
        { Name "DeleteTestUnit" }
        { Command "stpem -ed tut -p ${projdir} -s ${system} -C EditorStart -C EditorShowDeiconify -C 'FileLoad TestUnits' -C 'EditorQrlEval T_set_SE_hide_show();' -C 'EditorQrlEval T_delete_testunit_row(\"${cmd.objects}\");'" }
    }
}
