		    { Cell
			{ Hsect HSECT }
			{ Vsect TBASIC_VSECT }
			{ Row ROW }
			{ Col TINVALIDSUBDOMAIN_COL } } }
		{ Ordering LexicographicalIncreasing } 
	    } 
	} 
	{ Sort
	    { Name SortByTypeClassName }
	    { Label "By Type Class, then by Name" }
	    { Key
		{ Value
		    { LabelSelector Label }
		    { Cell
			{ Hsect HSECT }
			{ Vsect TBASIC_VSECT }
			{ Row ROW }
			{ Col TTYPECLASS_COL } } }
		{ Ordering LexicographicalIncreasing } 
