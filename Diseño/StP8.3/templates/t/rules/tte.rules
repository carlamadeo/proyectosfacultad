// Type Table Definition

TableType 1 TTypeTable
{ 
    { Label "Test Type Table" } 
    { Initialize
        { Secured 
	    { TTABLEBODY_HSECT 3 } 

	    { TTYPE_VSECT 1 }
	    { TBASIC_VSECT 2 }

	    { TTYPE_COL 1 }

	    { TTYPECLASS_COL 1 }
	    { TINVALIDSUBDOMAIN_COL 2 }
	    
        }
    }

  { ColWidth 110 }
  { RowHeight 40 }

  // and on with the definition of the table

  // The row above the header
  { Hsect
      { Label "Header" }
      { Frozen True }
      { Scrollbar False } 
      { ReadOnly True }
      { Shading Shade2 }
      // type name section
      { Vsect
          { Frozen True }
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "Type" }
                  { HspanSection True }
                  { Bold True } } } }
      // basic attributes section
      { Vsect
          { Frozen True }
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "Basic Type Attributes" }
                  { HspanSection True }
                  { Bold True } } } }
      // int and real section
      { Vsect
          { Frozen True }
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "Integer and Real Type Attributes" }
                  { HspanSection True }
                  { Bold True } } } }
      // boolean section
      { Vsect
          { Frozen True }
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "Boolean Type Attributes" }
                  { HspanSection True }
                  { Bold True } } } }
      // test values
      { Vsect
          { Row
              { RepeatToEnd True }
              { Cell
                  { Label "Test Values" }
                  { HspanSection True }
                  { Bold True } } } } }

  // the header  
  { Hsect 
      { Label "Test Type Header" }
      { Frozen True }
      { Scrollbar False } 
      { ReadOnly True }
      { Shading Shade1 }
      // type name section
      { Vsect 
          { Scrollbar False }
          { Label "Type Name" }
	  { Row
	      { Cell
		  { Label "Name" }
		  { Bold True } } } }
      // basic attributes section
      { Vsect 
          { Label "Basic Type Atts" }
	  { Row { Cell { Label "Type Class" } { Bold True } }
	        { Cell { Label "Invalid Subdom." } { Bold True } }
	        { Cell { Label "Units" } { Bold True } }
	        { Cell { Label "Format" } { Bold True } } } }
      // int and real section
      { Vsect 
          { Label "Int/Real Type Atts" }
	  { Row { Cell { Label "Min Value" } { Bold True } }
	        { Cell { Label "Max Value" } { Bold True } }
	        { Cell { Label "Resolution" } { Bold True } } } }
      // boolean section
      { Vsect 
          { Label "Boolean Type Atts" }
	  { Row { Cell { Label "True Value" } { Bold True } }
	        { Cell { Label "False Value" } { Bold True } }
	        { Cell { Label "Unknown Value" } { Bold True } } } }
      // test values
      { Vsect 
          { Label "Test Values" }
	  { Row { Cell { Label "Value" } { Bold True } } } } }

  // the body  
  { Hsect 
      { Label "Types" }
      { Type TTABLEBODY_HSECT }
      // type name section
      { Vsect 
          { Scrollbar False }
	  { Row
	      { RepeatToEnd True }
	      { Cell
		  { Type TDataTypeName }
		  { HorzAlignment Left } } } }
      // basic attributes section
      { Vsect 	
	  { Row
	      { RepeatToEnd True }
	      { Cell
		  { Type TDataTypeClass }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TInvalidSubdomain }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TTestUnits }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TFormat }
		  { HorzAlignment Left } } } }
      // int and real section
      { Vsect 	
	  { Row
	      { RepeatToEnd True }
	      { Cell
		  { Type TMinimumValue }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TMaximumValue }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TResolution }
		  { HorzAlignment Left } } } }
      // boolean section
      { Vsect 	
	  { Row
	      { RepeatToEnd True }
	      { Cell
		  { Type TTrueValue }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TFalseValue }
		  { HorzAlignment Left } }
	      { Cell
		  { Type TUnknownValue }
		  { HorzAlignment Left } } } }
      // test values
	{ Vsect 	
	    { Row
		{ RepeatToEnd True }
		{ Cell
		    { Type TTestValue }
		    { HorzAlignment Left } 
		    { RepeatToEnd True }
		    { HorzAlignment Left } } } } } }

// TTestType Node Semantics

CellType 1 TDataTypeName
{ 
  { Semantics
      { NodeMapping
          { MappingName TTestType }
	  { Precondition { LabelSelector Label } }
	  { Name
	      { LabelSelector Label }
	  } 
	  { Type TTestType } } 
      { NoteMapping  
          { MappingName TestDataType }
	  { Type TestDataType } 
	  { Parent
              { MappingName TTestType }
              { Cell
                  { Hsect HSECT }
                  { Vsect VSECT }
                  { Row ROW }
                  { Col COL } } } } }
  { Methods
      { Navigation
          { Name Attributes }
          { Label "Attribute on Class Table" }
          { MappingName TTestType }
          { Query "node_ref[node[OMTAttribute && items[OMTAttributeType && value = '${node.name}']] && file[OMTClassTable]]" } } }

}

