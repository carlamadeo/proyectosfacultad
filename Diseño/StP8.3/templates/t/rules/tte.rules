    { RowSorts
	{ Sort
	    { Name SortByName }
	    { Label "By Name" }
	    { Key
		{ Value
		    { LabelSelector Label }
		    { Cell
			{ Hsect HSECT }
			{ Vsect TTYPE_VSECT }
			{ Row ROW }
			{ Col TTYPE_COL } } }
		{ Ordering LexicographicalIncreasing } 
	    } 
	} 
	{ Sort
	    { Name SortByTypeClass }
	    { Label "By Type Class" }
	    { Key
		{ Value
		    { LabelSelector Label }
		    { Cell
			{ Hsect HSECT }
			{ Vsect TBASIC_VSECT }
			{ Row ROW }
			{ Col TTYPECLASS_COL } } }
		{ Ordering LexicographicalIncreasing } 
	    } 
