Menu GeneralizationMenu
{
    { Label "Generalization Hierarchy" }
    { Pin True }

    // ECR 4703 - begin
    { MenuEntry  
        { Name DrawGeneralizationMenu }
        { Label "Draw Generalization Hierarchy" }
        { Builtin "EditorQrlEval DrawGeneralization();" }
    	{ Accelerator
             { Value "Ctrl<Key>g" }
    	}
	    { ActiveFunc class_or_ellipsis_selected_active_func }
    }
    // ECR 4703 - end
    { MenuEntry
	{ Name CreateHierarchyFromPE }
	{ Label "Create Subclass Hierarchy from Reverse Engineering" } 
	{ Builtin "EditorQrlEval CC_query_class_subclasses();" }
        // 12746 fix.
	//{ ActiveFunc idehook_single_class_selected_active_func }
	{ ActiveFunc UmlGeneralizationActive }
    }
    { MenuEntry
	{ Name CreateSuperHierarchyFromPE }
	{ Label "Create Superclass Hierarchy from Reverse Engineering" } 
	{ Builtin "EditorQrlEval CC_query_class_superclasses();" }
        // 12746 fix.
	//{ ActiveFunc idehook_single_class_selected_active_func }
	{ ActiveFunc UmlGeneralizationActive }
    }
    { MenuEntry
	{ Name CreateWholeHierarchyFromPE }
	{ Label "Create Whole Hierarchy from Reverse Engineering" } 
	{ Builtin "EditorQrlEval CC_query_class_allclasses();" }
        // 12746 fix.
	//{ ActiveFunc idehook_single_class_selected_active_func }
	{ ActiveFunc UmlGeneralizationActive }
    }
}

Menu AttrsAndOpsMenu
{
    { Label "Attributes and Operations" }
    { Pin True }
    { MenuEntry  
	{ Name AddSelectively }
      	{ Label "Construct Class by &Adding Selectively..." } 
	{ Builtin "EditorQrlEval DecorateSelectedBoochClassSymbolsFromSelectedAll();" }
	{ ActiveFunc idehook_single_class_selected_active_func }
    }
    { MenuEntry  
	{ Name RebuildFromTable }
      	{ Label "&Construct Class from Class Table Definitions" } 
      	{ Builtin "EditorQrlEval DecorateSelectedBoochClassSymbolsFromTable();" }
	{ ActiveFunc idehook_class_selected_active_func }
	{ Accelerator
       	     { Label "Alt+Ctrl+C" }
             { Value "Alt Ctrl<Key>c" }
	}
    }
    { MenuEntry  
	{ Name RebuildFromModel }
      	{ Label "C&onstruct Class from All Definitions" } 
      	{ Builtin "EditorQrlEval DecorateSelectedBoochClassSymbolsFromAll();" }
	{ ActiveFunc idehook_class_selected_active_func }
	{ Accelerator
       	     { Label "Ctrl+Shift+A" }
             { Value "Ctrl Shift<Key>a" }
	}
    }
    { Spacer { Name Spacer1 } }
    { MenuEntry
        // 12746 fix. idehook_uml.. checks for smdb anyway.
	//{ ParseCondition "is_smdb_available();" }
	{ Name RebuildFromPE }
	{ Label "Construct from Reverse Engineering" } 
	{ Builtin "EditorQrlEval CC_query_class_members();" }
	{ ActiveFunc idehook_uml_reverse_engineering_active_func }
    }
}

Menu PackageMenu
{
    { Label "Packages" }
    { Pin True }
    { MenuEntry  
	{ Name AddLSelectively }
      	{ Label "Construct Package by &Adding Selectively..." } 
	{ Builtin "EditorQrlEval DecorateSelectedBoochCatSymbolsFromSelectedAll();" }
	{ ActiveFunc idehook_single_cat_selected_active_func }
    }
    { MenuEntry  
	{ Name AddGSelectively }
      	{ Label "&Construct Package by Adding Selectively (Global)..." } 
	{ Builtin "EditorQrlEval DecorateSelectedBoochCatSymbolsFromSelectedGlobal();" }
	{ ActiveFunc idehook_single_cat_selected_active_func }
    }
    { MenuEntry  
	{ Name RebuildFromModel }
      	{ Label "C&onstruct Package from All Components" } 
      	{ Builtin "EditorQrlEval DecorateSelectedBoochCatSymbolsFromAll();" }
	{ ActiveFunc idehook_cat_selected_active_func }
    }
}

