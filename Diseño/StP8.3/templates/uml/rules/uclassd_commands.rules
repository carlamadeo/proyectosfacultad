Menu ProductMenu//UML
{
    { SubMenu
      { Name AutoDrawMenu }
      { Label "Auto Draw" }
      { PlaceBefore GeneralizationMenu }
      { ActiveFunc one_class_selected_active_func }
    }
}

Menu AutoDrawMenu
{
    { Label "Auto Draw" }
    { Pin True }
    { MenuEntry
		{ Name CreateGeneralizationParentView }
		{ Label "Draw Superclass(es)" }
		{ Builtin "EditorQrlEval CreateHierarchyView(\"superclasses\");" }
		{ ActiveFunc one_class_selected_active_func }
    }
    { MenuEntry
		{ Name CreateGeneralizationView }
		{ Label "Draw Subclass(es)" }
		{ Builtin "EditorQrlEval CreateHierarchyView(\"subclasses\");" }
		{ ActiveFunc one_class_selected_active_func }
    }
    { Spacer { Name Spacer100 } }
    { MenuEntry
		{ Name DrawDependenceOut }
		{ Label "Draw Dependent Class(es)" }
		{ Builtin "EditorQrlEval CreateHierarchyView(\"dependentclasses\");" }
		{ ActiveFunc one_class_selected_active_func }
    }
    { MenuEntry
		{ Name DrawDependenceIn }
		{ Label "Draw Dependencies" }
		{ Builtin "EditorQrlEval CreateHierarchyView(\"classdependencies\");" }
		{ ActiveFunc one_class_selected_active_func }
    }
    { Spacer { Name Spacer101 } }
    { MenuEntry
		{ Name DrawAssociationView1 }
		{ Label "Draw Associations/Aggregations" }
		{ Builtin "EditorQrlEval DrawAssociationView();" }
		{ ActiveFunc two_classes_selected_active_func }
    }
	{ SubMenu 
		{ Name DrawAssociationView2 }
		{ Label "Draw Associated Class(es)" } // ECR 7293 - removed ellipsis
		{ ActiveFunc one_class_selected_active_func }
	}
}

Menu DrawAssociationView2 
{
    { MenuEntry
		{ Name DrawAssociationView3 }
		{ Label "into this diagram" }
		{ ActiveFunc one_class_selected_active_func }
		{ Builtin "EditorQrlEval UclassdAutoDraw(\"associations\",\"\",0);" }
    }
    { MenuEntry
		{ Name DrawAssociationView4 }
		{ Label "into new diagram" }
		{ ActiveFunc one_class_selected_active_func }
		{ Builtin "EditorQrlEval UclassdAutoDraw(\"associations\",\"\",1);" }
    }
}

