Menu ProductMenu//UML
{
    { InMenuBar True }
    { Label "&UML" }
    { Pin True }
    { MenuEntry
	{ Name CreateAssocRoles }
	{ Label "Create Ass&ociation Roles" }
	{ Builtin "SymbolBachLabels" }
	{ ActiveFunc idehook_bach_labels_active_func }
		{ Accelerator
			 { Label "Alt+Ctrl+L" }
			 { Value "Alt Ctrl<Key>L" }
		}
    }

    { MenuEntry
	{ Name CreateAssociationClass }
	{ Label "Create A&ssociation Class" } 
	{ Builtin "EditorQrlEval CreateAttributedAssociation();" }
	{ ActiveFunc idehook_attributed_association_active_func }
    }
    { MenuEntry
	{ Name CreateNaryAssociation }
	{ Label "Create &N-ary Association" } 
	{ Builtin "EditorQrlEval uml_create_nary_association();" }
	{ ActiveFunc idehook_multiple_class_selected_active_func }
    }
    { MenuEntry
	{ Name CreateOrAssociation }
	{ Label "Create Or Association" }
	{ Builtin "EditorQrlEval CreateOrAssociation();" }
	{ ActiveFunc idehook_or_association_active_func }
    }

// Begin ECR947, ECR 4494
    { MenuEntry
	{ Name ConnectNoteToLink }
	{ Label "Connect Note To Link" }
	{ Builtin "EditorQrlEval ConnectNoteToLink();" }
//    	{ Accelerator
//             { Value "Ctrl<Key>n" }
//	}
	{ ActiveFunc idehook_note_link_active_func }
    }
// End ECR947, ECR 4494

    { Spacer }

    { SubMenu
        // 12746 fix.
	//{ ParseCondition "is_smdb_available();" } 
	{ Name GeneralizationMenu }
	{ Label "General&ization Hierarchy" }
	{ ActiveFunc idehook_single_class_selected_active_func }
    }

    { SubMenu
	{ Name AttrsAndOpsMenu }
	{ Label "Attributes and &Operations" }
	{ ActiveFunc idehook_class_selected_active_func }
    }
    { SubMenu
	{ Name PackageMenu }
	{ Label "Packages and &Classes" }
	{ ActiveFunc idehook_cat_selected_active_func }
    }
    { MenuEntry
	{ Name CreateAttribute }
	{ Label "Create Attribute " }
	{ Builtin "EditorQrlEval create_new_attribute();" }
	{ ActiveFunc idehook_class_selected_active_func }
	{ Show False }
	{ Accelerator
		{ Label "Alt+Ctrl+A" }
		{ Value "Alt Ctrl<Key>a" }
	}
    }
    { MenuEntry
	{ Name CreateOperaton }
	{ Label "Create Operaton " }
	{ Builtin "EditorQrlEval create_new_operation();" }
	{ ActiveFunc idehook_class_selected_active_func }
	{ Show False }
	{ Accelerator
		{ Label "Alt+Ctrl+O" }
		{ Value "Alt Ctrl<Key>o" }
	}
    }
}

