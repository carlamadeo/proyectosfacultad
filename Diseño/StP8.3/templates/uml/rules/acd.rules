//-------------------------------------------
// Globals for Codegeneration
//-------------------------------------------
Global 1 GenerateVBTdlCodeClass
{
    { ClassCommand
        { Name "GenerateVBTdlCodeClass" }
        { ShowOnMenuExpr "codegen_active(\"vb_code_gen\");" }
        { Label "Generate VB Code..." }
        { Command "" }
        { Chdir False }
        { SingleObject False }
        { UseProperty VBDefaultOutputDir }
        { UseProperty ACDOptions }
        { UseProperty ACDGrouping }
    }
}


Global 1 VBDefaultOutputDir
{
    { Properties
        { Text
            { Name "tdl_template" }
            { Label "Template:" }
            { Resizable ResizeAny}
            { Type Text }
            { MinLength 0 }
            { MaxLength 300 }
            { DispLength 60 }
            { InitVal "vb_main.tdl" }
        }
    }
}

Global 1 GenerateCORBAIDLTdlCodeClass
{
    { ClassCommand
        { Name "GenerateCORBAIDLTdlCodeClass" }
        { ShowOnMenuExpr "codegen_active(\"corbaidl_code_gen\");" }
        { Label "Generate CORBAIDL Code..." }
        { Command "" }
        { Chdir False }
        { SingleObject False }
        { UseProperty CORBAIDLDefaultOutputDir }
        { UseProperty ACDOptions }
        { UseProperty ACDGrouping }
    }
}


Global 1 CORBAIDLDefaultOutputDir
