Menu Ada95SubMenu
{
    { Pin True }
    { MenuEntry
        { Name GeneratePartTdl }
        { Label "Generate Ada 95 for selected Object by ACD Template..." }
        { ActiveFunc activeAda95TdlCodeGen }
        { Builtin "EditorQrlEval stp_run_command(NULL,\"GenerateAda95TdlCodeObjects\");" }
    }
    { MenuEntry
        { Name GenerateWholeTdl }
        { Label "Generate Ada 95 by ACD Template..." }
        { ActiveFunc stp_repos_is_open }
        { Builtin "EditorQrlEval stp_run_command(\"UmlModel\",\"GenerateAda95TdlCodeClass\");" }
    }
    { MenuEntry
        { Name OpenTDL }
        { Label "Open &ACD Template..." }
        { Builtin "FileChooser \"Open ACD Template\" \"TDL Files (*.tdl)|*.tdl|All Files (*.*)|*\" \"${ti.tdl_path}/ada\"" }
    }
}

