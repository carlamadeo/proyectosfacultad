Global 1 ACDOptions
{
    { Properties
        { Qrl { QrlFile "uml/rules/qrl/acd.qrl" } }
        { Button
            { Name "browse_for_file" }
            { Label "Choose" }
            { Event
                { Type Select }
                { Callback "select_tdl_template_callback" }
            }
        }
        { Text
            { Name "output_dir" }
            { Label "Output Directory:" }
            { Resizable ResizeAny}
            { Type Text }
            { MinLength 0 }
            { MaxLength 300 }
            { DispLength 60 }
            { InitVal "${projdir}${system}/src_files" }
        }
        { Button
            { Name "browse_for_folder" }
            { Label "Choose" }
            { Event
                { Type Select }
                { Callback "select_tdl_output_dir_callback" }
            }
        }
        { Choice
            { Name "generate_imf" }
            { Type CheckBox }
            { ChoiceItem
                { Name "generate_imf" }
                { Label "Generate IMF File" }
                { OnValue "True" }
                { OffValue "False" }
                { Init On }
            }
        }
        { Choice
            { Name "suppress_log" }
            { Type CheckBox }
            { ChoiceItem
                { Name "suppress_log" }
                { Label "Suppress ACD Logging" }
                { OnValue "True" }
                { OffValue "False" }
                { Init Off }
            }
        }
    }
}

Global 1 ACDGrouping
{
    { Properties
        { Group
            { Name "fields_group" }
            { Label "" }
            { Type RowCol }
            { Rows 2 }
            { Columns 2 }
            { Resizable ResizeAny }
            { ChildSizePolicy ResizeWidth }
            { LayoutPolicy ResizeAny }
            { HorizontalAlign AlignLabels }
            { Members ( tdl_template browse_for_file output_dir browse_for_folder ) }
        }
        { Group
            { Name "Options1" }
            { Label "Options:" }
            { Type Vertical }
            { Resizable ResizeAny }
            { ChildSizePolicy ResizeWidth }
            { LayoutPolicy ResizeAny }
            { HorizontalAlign AlignLabels }
            { Members ( fields_group generate_imf suppress_log ) }
        }
        { TopGroup
            { Align Left }
            { Members ( Options1 ) }
        }
    }
}
