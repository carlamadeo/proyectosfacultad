Global 1 UmlSeqCollabProp
{
    { Properties
        { Text
            { Name "NameComponent" }
            { Label "Name Component:" }
            { MinLength 1 }
            { DispLength 20 }
            { Resizable ResizeNone }
            { Event
                { Type Text }
                { Callback "ps_MessageValueChanged_cb" }
            }
        }
        // Begin Add ECR 4110
        { Button
        	{ Name "ChooseAction" }
        	{ Label "Choose..." }
        	{ Event
        		{ Type Select }
        		{ Callback "ps_ChooseAction_cb" }
        	}
        }
        { Group
            { Name NameComponentGroup }
            { HorizontalSpacing 5 }
            { Type Horizontal }
            { Align Labels }        //ECR 6073
            { Members ( NameComponent ChooseAction ) }
        }
        // End Add ECR 4110
        { Text
            { Name "ReturnType" }
            { Label "Return Type:" }
            { MinLength 1 }
            { DispLength 20 }
            { Resizable ResizeNone }
            { Event
                { Type Text }
                { Callback "ps_MessageValueChanged_cb" }
            }
        }
        { Text
            { Name "MessageArgs" }
            { Label "Message Args:" }
            { MinLength 1 }
            { DispLength 35 }
            { Resizable ResizeNone }
            { Event
                { Type Text }
                { Callback "ps_MessageValueChanged_cb" }
            }
        }
// Begin ECR4197
	{ Choice
	    { Name "Destruction" }
	    { Type CheckBox }
	    { Label "Destruction:" }
	    { ChoiceItem
		{ Label "" }
		{ InitVal 0 }
	    }
	    { Event 
		{ Type Select}
		{ Callback "ps_ValueChanged_cb" }
	    }
	}
// End ECR4197
        { Text
            { Name "PredecessorGuard" }
            { Label "Predecessor Guard:" }
            { MinLength 1 }
            { DispLength 20 }
            { Resizable ResizeNone }
            { Event
                { Type Text }
                { Callback "ps_MessageValueChanged_cb" }
            }
        }
        // Begin Add ECR 4110
        { Button
        	{ Name "ChooseGuard" }
        	{ Label "Choose..." }
        	{ Event
        		{ Type Select }
        		{ Callback "ps_ChooseGuard_cb" }
        	}
        }
        { Group
            { Name PredecessorGuardGroup }
            { HorizontalSpacing 5 }
            { Type Horizontal }
            { Align Labels }        //ECR 6073
            { Members ( PredecessorGuard ChooseGuard ) }
        }
        // End Add ECR 4110
        { Text
            { Name "MessageSequence" }
            { Label "Message Sequence:" }
            { MinLength 1 }
            { DispLength 20 }
            { Resizable ResizeNone }
            { Event
                { Type Text }
                { Callback "ps_MessageValueChanged_cb" }
            }
        }
        { Text
            { Name "MessageRecurrence" }
            { Label "Message Recurrence:" }
            { MinLength 1 }
            { DispLength 20 }
            { Resizable ResizeNone }
            { Event
                { Type Text }
                { Callback "ps_MessageValueChanged_cb" }
            }
        }
// Begin ECR4197
	{ Choice
	    { Name "Creation" }
	    { Type CheckBox }
	    { Label "Creation:" }
	    { ChoiceItem
		{ Label "" }
		{ InitVal 0 }
	    }
	    { Event 
		{ Type Select}
		{ Callback "ps_ValueChanged_cb" }
	    }
	}
// End ECR4197
        { Group
            { Name "LeftGroup" }
            { Type Vertical }
            { HorizontalAlign AlignLabels }
// ECR4197 Added Destruction to list of members
            { Members ( NameComponentGroup ReturnType MessageArgs Destruction ) } 
        }
        { Group
            { Name "RightGroup" }
            { Type Vertical }
            { HorizontalAlign AlignLabels }
// ECR4197 Added Creation to list of members
            { Members ( PredecessorGuardGroup MessageSequence MessageRecurrence Creation ) } 
        }
        { Group
            { Name "SeqCollabStuff" }
            { Label "Message Attributes" }
            { Type Horizontal }
            { HorizontalSpacing 10 }
            { HorizontalAlign Top }
            { Members ( RightGroup LeftGroup ) }
        }
    }
}

