Toolbar DefaultArcType
{
    //ECR 6380 begin
    //{ Position Top }
    //{ IsFloating True }
    { Position Left }
    //ECR 6380 end
    { Group
        { Exclusive True }
        { Entry
            { Name "ObjectLink" }
            { Type Toggle }
            { Builtin "SetDefaultArcType ObjectLink" }
            { HelpTip "Object Link" }
            { Image "objectlink" }
        }
        { Entry
            { Name "TypeElementLink" }
            { Type Toggle }
            { Builtin "SetDefaultArcType TypeElementLink" }
            { HelpTip "Type Link" }
            { Image "typeelementlink" }
        }
        { Entry
            { Name "CollaborationRole" }
            { Type Toggle }
            { Builtin "SetDefaultArcType CollaborationRole" }
            { HelpTip "Collaboration Link" }
            { Image "collaborationrole" }
        }
        { Entry
            { Name "NoteLink" }
            { Type Toggle }
            { Builtin "SetDefaultArcType NoteLink" }
            { HelpTip "Note Link" }
            { Image "notelink" }
        }
    }
}

