NodeSymbol deploymentComponent
{
    { NodeMapping
        { MappingName DeploymentComponent }
        { AppType UmlDeploymentNode }
        { NameIsRequired True }
    }
}

Include "rules/componentDeployment_mappings.rules.common"

LinkSymbol deploys
{
    { LinkMapping
	{ MappingName DeploysB }
	{ AppType UmlDeployment }
	{ NameIsRequired False }
	{ FromNode { MappingName DeploymentComponent } }
	{ ToNode { MappingName BinaryComponent } }
    }
    { LinkMapping
	{ MappingName DeploysE }
	{ AppType UmlDeployment }
	{ NameIsRequired False }
	{ FromNode { MappingName DeploymentComponent } }
	{ ToNode { MappingName ExecutableComponent } }
    }
    { LinkMapping
	{ MappingName DeploysI }
	{ AppType UmlDeployment }
	{ NameIsRequired False }
	{ FromNode { MappingName DeploymentComponent } }
	{ ToNode { MappingName InterfaceComponent } }
    }
    { LinkMapping
	{ MappingName DeploysO }
	{ AppType UmlDeployment }
	{ NameIsRequired False }
	{ FromNode { MappingName DeploymentComponent } }
	{ ToNode { MappingName objectComponent } }
    }
}

// Begin Add ECR 4515
LinkSymbol Communicates
{
    { LinkMapping
    	{ MappingName CommunicatesMapping }
    	{ AppType UmlDeploymentNodeCommunicates }
    	{ FromNode { MappingName DeploymentComponent } }
    	{ ToNode { MappingName DeploymentComponent } }
    }
}
// End Add ECR 4515
