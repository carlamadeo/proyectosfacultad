PropertySheet UmlAttribute_ps
{
    { Name "UmlAttribute_ps" }
    { Label "Properties" } 
    { UseProperty UmlObjectDescriptionButton}
    { UseProperty UmlExtensibilityProps }
    { UseProperty UmlBottomButtons }
    { OwnerActionCallback UmlAttribute_ps_ActionDone } // ECR 6880
    { Properties
        { Qrl { QrlFile "rules/qrl/uclassd.ps.qrl" } }
        { Text
            { Name "ObjectName" }
            { Label "Attribute Name:" }
            { MinLength 1 }
            { DispLength 25 }
            { Resizable ResizeWidth }
            { Event
                { Type Text }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        { Group
            { Name NameGroup }
	    { Label "Attribute:" }
            { Type Vertical }
            { VerticalSpacing 5 }
            { Resizable ResizeWidth }
            { HorizontalAlign AlignLabels }
            { Members (ObjectName ObjectDescription) }
        }
        { TopGroup
            { VerticalSpacing 10 }
            { VariableRow "1" }
            { Align Left }
            { Members ( NameGroup Extensibility ) }
        }
    }
}

