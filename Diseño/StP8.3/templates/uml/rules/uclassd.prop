Global 1 UmlClassNodeStuff

{
    { Properties
        { Choice
            { Name Abstract }
            { Label "Abstract" }
            { Type CheckBox }
            { ChoiceItem
                { Label "" }
            }
            { Event
                { Type Select }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        { Choice
            { Name External }
            { Label "External" }
            { Type CheckBox }
            { ChoiceItem
                { Label "" }
            }
            { Event
                { Type Select }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        { Choice
            { Name Visibility }
            { Label "Visibility:" }
            { Type AbbrMenu }
            // Added "    " for ECR4270, lower case for ECR 5281
            { ChoiceItem { Label "         " } }
            { ChoiceItem { Label "public   " } }
            { ChoiceItem { Label "protected" } }
            { ChoiceItem { Label "private  " } }
            { Event
                { Type Select }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        { Text
            { Name Multiplicity }
            { Label "Multiplicity:" }
            { MinLength    0 }
            { DispLength 5 }
            { InitVal "Many" }
            { Event
                { Type Text }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        { Group
            { Name ClassNodeStuff1 }
            { Type Horizontal }
            { Members ( External Abstract ) }
        }
        { Group
            { Name ClassNodeStuff2 }
            { Type Horizontal }
            { Members ( Visibility Multiplicity ) }
        }
        { Group
            { Name ClassNodeStuff }
            { Type Vertical }
            { Align Left }
            { Members ( ClassNodeStuff1 ClassNodeStuff2 ) }
        }
    }
}

// Package property sheets


PropertySheet UmlPackage_ps
{
    { Name "UmlPackage_ps" }
    {Label "Properties"} {UseProperty UmlObjectDescriptionButton}
    { UseProperty UmlExternalProp }
    { UseProperty UmlComponentProp }
    { UseProperty UmlExtensibilityProps }
    { UseProperty UmlBottomButtons }
    { OwnerActionCallback UmlPackage_ps_ActionDone } // ECR 6880
    { Properties
        { Qrl { QrlFile "rules/qrl/uclassd.ps.qrl" } }
        { Text
            { Name "ObjectName" }
            { Label "Name:" } // ECR 7425 - changed from 'Package Name' to 'Name'
            { MinLength 1 }
            { DispLength 25 }
            { Resizable ResizeWidth }
            { Event
                { Type Text }
                { Callback "ps_ValueChanged_cb" }
            }
        }
        { Group
            { Name ComponentModifyFieldsGroup }
            { Type Horizontal }
            { HorizontalSpacing 60 }
            { VariableColumn "0" }
            { Resizable ResizeWidth }
            { HorizontalAlign AlignLabels }
            { ChildSizePolicy ResizeAny }
            { LayoutPolicy ResizeAny }
            { Members ( Component
                        External ) }
        }
        { Group
            { Name ModifyGroup }
            { Label "Object" } // ECR 7425 - changed from 'Package' to 'Object'
            { Type Vertical }
            { Resizable ResizeWidth }
            { HorizontalAlign AlignLabels }
            { LayoutPolicy ResizeAny }
            { Members ( ObjectName ObjectDescription
                        ComponentModifyFieldsGroup) }
        }
        { TopGroup
            { Align Left }
            { VerticalSpacing 10 }
            { VariableRow "1" }
            { Members ( ModifyGroup
                        Extensibility ) }
        }
    }
}

