Command NodeGoToRequirementsTable
{
    // ECR 7464
   // { Builtin "EditorQrlEval to_requirements_table_from_node(${node.id});" }
   { Builtin "EditorQrlEvalNoSave to_requirements_table_from_node(${node.id});" }
}

